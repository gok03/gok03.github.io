<SCRIPT LANGUAGE=JavaScript>


var windws = navigator.appName.indexOf('Microsoft') != -1;
var netscpe = navigator.appName.indexOf('Netscape') != -1;


//------------------------------------------------------------------------------------
function whichBrowser() {

   var version  = parseFloat(navigator.appVersion);

   var NSvers   = 4.0;
   var IEvers   = 3.0;

   var newBrowser = 0;

   if (netscpe) {
      if (version >= NSvers) {
         newBrowser = 1;
      }                                                    
      else {
         newBrowser = 0;
      }
   }

   else {
      if (windws) {
         newBrowser = 1; }
      else newBrowser = 0;
   }

   return newBrowser;

}

//------------------------------------------------------------------------------------

function launch_contact(){
	window.open('contact.html','contact', 'resizable=yes,toolbar=no,personalbar=no,menubar=no,status=yes,directories=no,scrollbars=no,width=350,height=200');
	}

//------------------------------------------------------------------------------------

function launch_jobs() {
      window.open("jobs.html","jobs","resizable=yes,toolbar=no,personalbar=no,menubar=no,status=yes,directories=no,scrollbars=no,width=480,height=480");

	}

//------------------------------------------------------------------------------------

function launchFlash() {
      window.open("FL3/aftershock.htm","Flash","resizable=yes,toolbar=no,personalbar=no,menubar=no,status=no,directories=no,scrollbars=no,width=695,height=561");

	}

//------------------------------------------------------------------------------------

 
//------------------------------------------------------------------------------------

