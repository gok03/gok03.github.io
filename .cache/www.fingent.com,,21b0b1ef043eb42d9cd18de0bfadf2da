        <script type="text/javascript">
			$(document).ready(function(e) {
				setvimeoplayer();
				setTimeout(function(){$("#vimeoplayer").fadeIn(700)}, 1500)
            });
			$(window).resize(function(e) {
				setvimeoplayer()
            });
			function setvimeoplayer() {
				var dat = {
					fw : 1920,
					fh : 1079,
					rt : 0,
					ww : $(window).innerWidth(),
					nh : 0,
					wh : $(window).innerHeight()
				}
				dat.rt = dat.fw/dat.fh;
				dat.nh = dat.ww/dat.rt;
				
				$("#vimeoplayer").height(dat.nh);
			}
		</script>
