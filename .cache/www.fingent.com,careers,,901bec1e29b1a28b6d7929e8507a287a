<script type="text/javascript">
(function($) {
	$(document).ready(function() {
       
	    //foot-tab
		$(".nav-tabs-alt li").click(function(e) {
			e.preventDefault();
			$(".tab-content .tab-pane, .nav-tabs-alt li").removeClass("active");
			var nm = $(this).index()+1;
			$(this).addClass("active"); 
			$(".tab-content .tab-pane:nth-child("+nm+")").addClass("active");
		});
		
		//address-tab
		$(".adr-tab li").click(function(e) {
			$(".adr-tab-content, .adr-tab li").removeClass("active");
			var as = $(this).index()+2;
			$(this).addClass("active"); 
			$(".adr-tab-content:nth-child("+as+")").addClass("active");
		});
		
    });
})(jQuery);
</script>

