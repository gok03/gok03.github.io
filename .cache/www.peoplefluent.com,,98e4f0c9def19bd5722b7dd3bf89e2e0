<script type="text/javascript">
		$(function(){$('.menu-btn').click(function(){$('.responsive-menu').toggleClass('expand')})})
		$(document).ready(function(){
			$(".nav-sub__list").parent(".nav-global__item").append("<div class='open-menu-link open'>+</div>");
			$(".nav-sub__list").parent(".nav-global__item").append("<div class='open-menu-link close'>-</div>");
		
			$('.open').addClass('visible');
		
			$('.open-menu-link').click(function(e){
				var childMenu =  $(this).siblings('.nav-sub__list');
				if($(childMenu).hasClass('visible')){
					$(childMenu).removeClass("visible");
					$(childMenu).siblings('.open').addClass("visible");
					$(childMenu).siblings('.close').removeClass("visible");
				} else {
					$(childMenu).addClass("visible");
					$(childMenu).siblings('.open').removeClass("visible");
					$(childMenu).siblings('.close').addClass("visible");
				}
	    	});
		});
</script>
        
      
