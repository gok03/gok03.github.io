function homePagePromotionTrack(code,title)
{
	if(true)	{
	ACC.track.trackHomePagePromotionsGA(code,title); 
	}
}
function trackAddToCartListGrid(code,name,category,price)
{
ACC.track.trackAddToCartFromListAndGridGA("AddToCart-FromListAndGrid","AddToCart",code,name,1); 
ACC.track.trackAddToCartFromListAndGridCoremetrics("QuickAddToCart","AddToCart",code,name,1,category,price);
} 
