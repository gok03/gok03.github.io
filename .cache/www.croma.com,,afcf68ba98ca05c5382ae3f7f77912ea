<script type="text/javascript">
$(document).ready(function(){
	
	var url      = window.location.href; 
	if (url.indexOf("/c/") <= 0){document.getElementsByClassName('megaa_menu')[0].style.visibility = "visible";}
	else{var after ="c"+ url.substr(url.indexOf("/c/") + 3);
	if((document.getElementsByClassName(after)==null)||(document.getElementsByClassName(after)==="undefined")){document.getElementsByClassName('megaa_menu')[0].style.visibility = "visible";}
	else{document.getElementsByClassName('megaa_menu')[0].style.visibility = "visible";document.getElementsByClassName(after)[0].click();}
	}
});

function myFunction(){
		
	   // $('.menu_np li:nth-child(1)').children('.megaa_menu').css('visibility', 'visible');
		
		$('nav .menu_np li').click(function() {
			$('.megaa_menu').css('visibility', 'hidden');
			$(this).children('div.megaa_menu').css('visibility', 'visible');
		});
		
		$('nav .menu_np li').on('click', function(){
			$('li.current').removeClass('current');
			$(this).addClass('current');
			var divHeight = $('.footer ul.menu_np li.current .megaa_menu').height()+90; 
			$('.footer').css('min-height', divHeight+'px');
				
		});
		
		if($(window).width() < 800){
			$(".cromafooterbold").next('.slide').each(function(i){
                $(this).slideUp();
            });
			$('.cromafooterbold').on('click', function(e){
                $('.slide:visible').slideUp();
                $(this).toggleClass('active');
                $(this).next('div').slideUp();
                if($(this).hasClass('active')){
                    $(this).next('div').slideDown();
                    //$(this).removeClass('active');
                }else{
                    //$(this).addClass('active');
					$(this).next('div').slideUp();
				}
			});
		}
		
	}
</script>
 

