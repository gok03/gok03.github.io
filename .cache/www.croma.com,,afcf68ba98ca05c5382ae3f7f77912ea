<script type="text/javascript">
/* Google Analytics */

var googleAnalyticsTrackingId = 'UA-67533192-1';
var _gaq = _gaq || [];
_gaq.push(['_setAccount', googleAnalyticsTrackingId]);


		_gaq.push(['_trackPageview']);
	

(function() {
	var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
	ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
	var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
})();


function trackAddToCart_google(productCode, quantityAdded) {
    //	_gaq.push(['_trackEvent', 'Cart', 'AddToCart', productCode, quantityAdded]);
}

function trackUpdateCart(productCode, initialQuantity, newQuantity) {
	if (initialQuantity != newQuantity) {
		if (initialQuantity > newQuantity) {
			_gaq.push(['_trackEvent', 'Cart', 'RemoveFromCart', productCode, initialQuantity - newQuantity]);
		} else {
			_gaq.push(['_trackEvent', 'Cart', 'UpdateToCart', productCode, newQuantity - initialQuantity]);
		}
	}
}

function trackRemoveFromCart(productCode, initialQuantity) {
	_gaq.push(['_trackEvent', 'Cart', 'RemoveFromCart', productCode,initialQuantity]);
}


function trackPincodeCheckSuccess(productCode, pincode) {
	_gaq.push(['_trackEvent', 'ProductPage', 'Success-PincodeCheck',pincode+'(ProductCode:'+productCode+')' ]);
}

function trackPincodeCheckFail(productCode, pincode) {
	_gaq.push(['_trackEvent', 'ProductPage', 'Failure-PincodeCheck',pincode+'(ProductCode:'+productCode+')' ]);
}

function trackEMI(productCode, productName,amount) {
	
	_gaq.push(['_trackEvent', "ProductPage",'EMI', productName+'('+productCode+')']);	
}
function trackPDPButton(buttonName,productName,code) {

	_gaq.push(['_trackEvent', "ProductPage",buttonName,productName+'('+code+')']);
}
function trackCartPincode(pincode,cartCode) {

	_gaq.push(['_trackEvent',"CartPage-PincodeCheck",'CartPincodeChecked:'+pincode, 'CartId:'+cartCode]);	
}
function trackCouponCode(couponCode,cartCode) {
	
	_gaq.push(['_trackEvent', "Coupon","Coupon Applied",'CouponCode:'+couponCode, 'CartId:'+cartCode]);	
}
function trackPaymentMode(paymentMode,cartCode)
{
	_gaq.push(['_trackEvent',"PaymentDetails",paymentMode, 'CartId:'+cartCode]);
}
window.mediator.subscribe('trackAddToCart', function(data) {
	if (data.productCode && data.quantity)
	{
		trackAddToCart_google(data.productCode, data.quantity);
	}
});

window.mediator.subscribe('trackUpdateCart', function(data) {
	if (data.productCode && data.initialCartQuantity && data.newCartQuantity)
	{
		trackUpdateCart(data.productCode, data.initialCartQuantity, data.newCartQuantity);
	}
});

window.mediator.subscribe('trackRemoveFromCart', function(data) {
	if (data.productCode && data.initialCartQuantity)
	{
		trackRemoveFromCart(data.productCode, data.initialCartQuantity);
	}
});

window.mediator.subscribe('successPincodeCheckForProductGA', function(data) {
	
	if (data.productCode && data.pincode)
			{

trackPincodeCheckSuccess(data.productCode,data.pincode);
			//_gaq.push(['_trackEvent', 'PincodeCheck', 'Success', 'Product', 12345]);		
			}
		});

window.mediator.subscribe('failPincodeCheckForProductGA', function(data) {
			
	if (data.productCode && data.pincode)
			{
trackPincodeCheckFail(data.productCode,data.pincode);

	//	_gaq.push(['_trackEvent', 'PincodeCheck-Product', 'Failure', "PRODUCT:"+data.productCode, data.pincode]);

				
			}
		});

window.mediator.subscribe('trackEMIGA', function(data) {

	if (data.productCode && data.productName)
			{
			trackEMI(data.productCode,data.productName,data.amount);

	//	_gaq.push(['_trackEvent', 'PincodeCheck-Product', 'Failure', "PRODUCT:"+data.productCode, data.pincode]);

				
			}
		});
window.mediator.subscribe('trackPDPButtonGA', function(data) {

	if(data.buttonName && data.productName && data.code)
	{
		trackPDPButton(data.buttonName,data.productName,data.code);

	//	_gaq.push(['_trackEvent', 'PincodeCheck-Product', 'Failure', "PRODUCT:"+data.productCode, data.pincode]);

				
			}
		});
