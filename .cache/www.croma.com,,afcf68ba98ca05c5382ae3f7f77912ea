<script>
$(document).ready(function(){    
    var i = 1;
        $("<img class='previmg' id='0' src='_ui/desktop/common/images/pagination-prev.png' />").prependTo(".scroller");
     $(".yCmsContentSlot.footerParagraph").appendTo(".yCmsContentSlot.span-24");

	$('.jcarousel-next-horizontal').on("click",function() {
	var s= $(this).parent().attr('id');
	if (s === undefined || s === null)
	{
	$(this).parent().attr('id', "box" + i);
	i++;
	}
	var news=$(this).parent().attr('id');

	if($('#'+news+' > .jcarousel-prev').prop(':disabled'))
	{
	//$(this).parent().closest('img').find('.previmg').attr(id).css("display", "none");
	//$('#previmg < #'+news).css( "display", "none" );
	$(this).closest('.scroller').find('.previmg').attr("style", "display:none");
	}
	else
	{
	//$(this).parent().closest('img').find('#previmg').attr(id).css("display", "inline");
	$(this).closest('.scroller').find('.previmg').attr("style", "display:inline");
	var count = parseInt($(this).closest('.scroller').find('.previmg').attr('id'));
	count++;
	$(this).closest('.scroller').find('.previmg').attr('id',count);
	
	}


});

	$('.previmg').on("click",function() {
		
		$(this).closest('.scroller').find('.jcarousel-prev').click();
		var count = parseInt($(this).closest('.scroller').find('.previmg').attr('id'));
		count--;
		if(count==0)
		{
		$(this).closest('.scroller').find('.previmg').attr("style", "display:none");
		}
		$(this).closest('.scroller').find('.previmg').attr('id',count);

		
		/*if($(this).closest('.scroller').find('.jcarousel-prev').css('opacity')<1)
		{
		$(this).closest('.scroller').find('.previmg').attr("style", "display:none");
		}

		
		if($(this).closest('.scroller').find('.jcarousel-prev').prop('disabled') ) { 
		
		}
		alert($(this).closest('.scroller').find('.jcarousel-prev').css('opacity'));
		if($(this).closest('.scroller').find('.jcarousel-prev-disabled'))
		{
	
		$(this).closest('.scroller').find('.previmg').attr("style", "display:none");

		}

		if($(this).closest('.scroller').find('.jcarousel-prev').css('opacity')<1)
		{
		$(this).closest('.scroller').find('.previmg').attr("style", "display:none");
		}
		else*/
		


});
		






});


</script>

