	if(data.eventLabel)
		{
		_gaq.push(['_trackEvent',"Checkout","ProceedToCheckout",data.eventLabel]);
		}
});
window.mediator.subscribe('trackOrderConfirmationGA',function(data){
	if(data.eventCategory && data.eventAction)
		{
		_gaq.push(['_trackEvent',data.eventCategory,data.eventAction]);	
		}
});
window.mediator.subscribe('trackAddToCartFromListAndGridGA',function(data){
	if(data.productCode && data.productName)
		{
		_gaq.push(['_trackEvent',"Cart",data.eventLabel,data.productCode]);
		}
});
window.mediator.subscribe('trackAddtoCartPDPGA',function(data){
	if(data.eventLabel && data.eventAction)
		{
		_gaq.push(['_trackEvent',"Cart",data.eventLabel,data.productCode]);
		}
});
window.mediator.subscribe('trackMultiCheckoutChangeLoginGA',function(data){
	if(data.eventCategory&& data.eventAction && data.eventLabel)
		{
		_gaq.push(['_trackEvent',data.eventCategory,data.eventAction,data.eventLabel]);		
		}
});
window.mediator.subscribe('trackCheckoutLoginGA',function(data){
	if(data.eventCategory && data.eventAction && data.eventLabel)
		{
		_gaq.push(['_trackEvent',data.eventCategory,data.eventAction,data.eventLabel,data.email]);		
		}
});
window.mediator.subscribe('trackHomePageBannersGA',function(data){
	if(data.eventCategory && data.eventAction && data.eventLabel)
		{
		_gaq.push(['_trackEvent',data.eventCategory,data.eventAction,data.eventLabel]);		
		}
});
window.mediator.subscribe('trackProductReviewsGA',function(data){
	if(data.eventCategory && data.eventAction && data.eventLabel)
		{
		_gaq.push(['_trackEvent',data.eventCategory,data.eventAction,data.eventLabel]);		
		}
});
window.mediator.subscribe('trackStickerHover',function(data){
	if(data.eventCategory && data.eventAction && data.eventLabel)
		{
		_gaq.push(['_trackEvent',data.eventCategory,data.eventAction,data.eventLabel]);
		}
});
window.mediator.subscribe('trackRegister',function(data){
	if(data.email && data.phone && data.firstname && data.lastname)
		{
		_gaq.push(['_trackEvent',"Account_Login","RegisterNew",data.email,data.firstname+" "+data.lastname]);		
		}
});
window.mediator.subscribe('trackCheckoutLoginReturning',function(data){
	if(data.eventCategory && data.eventAction && data.eventLabel&& data.emailid)
		{
		_gaq.push(['_trackEvent',data.eventCategory,data.eventAction,data.eventLabel,data.emailid]);		
		}
});
window.mediator.subscribe('trackQuickViewPopupGA',function(data){
	if(data.categoryCode && data.productName && data.productCode)
		{
		_gaq.push(['_trackEvent',"ProductPage","Quick View",data.productName+"("+data.productCode+")",data.categoryCode]);
		}
});
window.mediator.subscribe('trackZoomImagePDPGA',function(data){
	if(data.categoryCode && data.productName && data.productCode)
		{
		_gaq.push(['_trackEvent',"ProductPage","Zoom Image",data.productName+"("+data.productCode+")",data.categoryCode]);
		}
});

window.mediator.subscribe('trackPDPWishList',function(data){
	if(data.eventLabel)
		{
		_gaq.push(['_trackEvent',data.eventCategory,data.eventAction,data.eventLabel]);	
		}
});
</script>
