<script type='text/javascript'>

    $(document).ready(function () {

        // hide flashing iframe
        var div = document.createElement('div'),
        ref = document.getElementsByTagName('base')[0] ||
              document.getElementsByTagName('script')[0];

        div.innerHTML = '&shy;<style> iframe { visibility: hidden; } </style>';

        ref.parentNode.insertBefore(div, ref);

        window.onload = function () {
            div.parentNode.removeChild(div);

        }



	     var isiPad = navigator.userAgent.match(/iPad/i) != null;
	     var hid = document.getElementById("hidisiPad")
	     if (hid) {
	         hid.value = isiPad;
	     }

	     doResize();

          // update resize event
          $(window).unbind("resize");
          $(window).resize(function () {
              doResize();
          });   

          $("#mainBanner").banner({ intervalinseconds: 8, autoplay: true });

      });

      function doResize()  {
            var DivHeight = GetHeight();
			var DivWidth = GetWidth();

			
			//$('.logintop').css('right', (DivWidth - 790) + "px");

			$('#MainTable').css('height', (DivHeight - 128) + "px");	
			
			if (DivHeight > 850) {
			//$('#Besocial').css('height', (DivHeight - 840) + "px");
			}
			//else {
			//	$('#MainTable').css('height', "440px");
			//}
			
      }
 



function OpenNewsMenu(url) {
 var iWidth=750;
 var iHeight=560;
 var iLeft=(screen.width/2)-(iWidth/2)-6;
 if (iLeft<0) {iLeft=0;}
 var iTop=((screen.height-30)/2)-(iHeight/2)-30;
 if (iTop<0) {iTop=0;}
appwin=window.open(url,'OpenNewsMenu','toolbar=no,scrollbars=no,location=no,menubar=no,resizable=no,titlebar=no,status=yes,width=' + iWidth + ',height=' + iHeight + ',left=' + iLeft + ',top=' + iTop);
appwin.focus()
}
