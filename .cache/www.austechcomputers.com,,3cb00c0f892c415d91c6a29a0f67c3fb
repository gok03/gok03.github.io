function ximrxnava(){
    var highestBox = 0;
        jQuery('.caption__portfolio > p.excerpt').each(function(){  
                if(jQuery(this).height() > highestBox){  
                highestBox = jQuery(this).height();  
        }
    });    
    jQuery('.caption__portfolio > p.excerpt').height(highestBox/4);

    var highestBox2 = 0;
        jQuery('.caption__portfolio > h3').each(function(){  
                if(jQuery(this).height() > highestBox2){  
                highestBox2 = jQuery(this).height();  
        }
    });    
    jQuery('.caption__portfolio > h3').height(highestBox2/2);
}


