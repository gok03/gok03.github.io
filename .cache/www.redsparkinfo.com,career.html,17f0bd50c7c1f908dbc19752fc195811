<script>

    function get_quote(){

        var gender_value = $('input[type="radio"].rediobtn:checked').val();

        var flag="";
        var quote_name = document.getElementById('quote_name').value;
        if(quote_name!=''){
            //jQuery('#weight_lb_alert').hide();
            jQuery('#quote_name').removeClass('quote_name_alert');


        }else {
            //jQuery('#weight_lb_alert').show();
            $('#quote_name').addClass('quote_name_alert');
            flag +='error';

        }
        var quote_company_name = document.getElementById('quote_company_name').value;
        if(quote_company_name!=''){
            // jQuery('#waist_alert').hide();
            jQuery('#quote_company_name').removeClass('quote_company_name_alert');


        }else {
            //jQuery('#waist_alert').show();
            jQuery('#quote_company_name').addClass('quote_company_name_alert');
            flag +='error';

        }

        var quote_number = document.getElementById('quote_number').value;
        if(quote_number!=''){
            // jQuery('#user_name_alert').hide();
            jQuery('#quote_number').removeClass('quote_number_alert');


        }else {
            //   jQuery('#user_name_alert').show();
            jQuery('#quote_number').addClass('quote_number_alert');
            flag +='error';

        }

        var quote_email = document.getElementById('quote_email').value;
        if(quote_email!=''){
            var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            var return_result = re.test(quote_email);

            if(!return_result){
                // jQuery('#user_email_alert').hide();
                jQuery('#quote_email').addClass('quote_email_alert');
                flag +='error';
            }else {
                jQuery('#quote_email').removeClass('quote_email_alert');
            }

        }else {
            //jQuery('#user_email_alert_valid').hide();
            jQuery('#quote_email').addClass('quote_email_alert');
            flag +='error';

        }

        var quote_intersted_in = document.getElementById('quote_intersted_in').value;
        if(quote_intersted_in!=''){
            // jQuery('#user_name_alert').hide();
            jQuery('#quote_intersted_in').removeClass('quote_intersted_in_alert');


        }else {
            //   jQuery('#user_name_alert').show();
            jQuery('#quote_intersted_in').addClass('quote_intersted_in_alert');
            flag +='error';

        }

        var quote_country = document.getElementById('quote_country').value;
        if(quote_country!='0'){
            // jQuery('#user_name_alert').hide();
            jQuery('#quote_country').removeClass('quote_country_alert');


        }else {
            //   jQuery('#user_name_alert').show();
            jQuery('#quote_country').addClass('quote_country_alert');
            flag +='error';

        }

        var quote_req = document.getElementById('quote_req').value;
        if(quote_req!=''){
            // jQuery('#user_name_alert').hide();
            jQuery('#quote_req').removeClass('quote_req_alert');


        }else {
            //   jQuery('#user_name_alert').show();
            jQuery('#quote_req').addClass('quote_req_alert');
            flag +='error';
        }

        var error_captcha = '';
        var quote_captcha = jQuery("#quote_captcha").val();
        if(quote_captcha == ''){
            jQuery('#quote_captcha').addClass('quote_captcha_alert');
            flag +='error';
        } else {
            jQuery('#quote_captcha').removeClass('quote_captcha_alert');

            if(jQuery("#quote_captcha").val() != jQuery("#hdn_val").val())
                {
                jQuery('#quote_captcha').addClass('quote_captcha_alert');
                flag +='error';
            }else {
                jQuery('#quote_captcha').removeClass('quote_captcha_alert');
            }
        }
        if(flag!='')
            {
            return false;
        }else {
            var quote_representative = jQuery("#quote_representative").val();
            jQuery.ajax({
                type: "POST",
                url: 'http://www.redsparkinfo.com/wp-admin/admin-ajax.php',
                data: {
                    'action':'get_quote_ajax',
                    'quote_name':quote_name,
                    'quote_company_name':quote_company_name,
                    'quote_number':quote_number,
                    'quote_email':quote_email,
                    'quote_intersted_in':quote_intersted_in,
                    'quote_country':quote_country,
                    'quote_req':quote_req,
                    'quote_representative':quote_representative,

                },
                success:function(data1) {
                    // jQuery('.submit_info').css('display','none');                   
                    //alert(data1);
                    window.location = "http://www.redsparkinfo.com/thank-you.html";
                },
                error: function(errorThrown){
                    console.log(errorThrown);
                }
            });
        }
    }
    //    function info_remove(){
    //        jQuery('.calculated_info').css('display','none');
    //        jQuery('.submit_info').css('display','block');
    //        jQuery("#weight_lb").val("");                    
    //        jQuery("#waist").val("");                    
    //        jQuery("#user_name").val("");                    
    //        jQuery("#user_email").val("");                    
    //    }
</script> 
