<script>
    $(document).ready(function()
    {
        success_story_tab_handler();




        $("#review_carousel").owlCarousel(
            {
                loop : true,
                nav : true,
                smartSpeed : 700,
                navText : ['<i class="fa fa-angle-left"></i>' , '<i class="fa fa-angle-right"></i>'],
                items : 3,
                dots : false,
                responsive:
                {
                    0:
                    {
                        items:1
                    },
                    650:
                    {
                        items: 1
                    },
                    651:
                    {
                        items: 2
                    },
                    767:
                    {
                        items:2
                    },
                    991:
                    {
                        items:2
                    },
                    992:
                    {
                        items:3
                    }
                }
            });
    });


    function success_story_tab_handler()
    {
        $('.case-study-details-container:first-child').addClass('case-study-details-container-active').show();
        $('.case-study-block').bind('click', function()
        {
            var tab_trigger = $(this).data('target');
            $('.case-study-block').removeClass('active');
            $(this).addClass('active');
            $('.case-study-details-container-active').fadeOut('fast', function()
            {
                $('.case-study-details-container').hide();
                $('.case-study-details-container').removeClass('case-study-details-container-active');
                $(tab_trigger).addClass('case-study-details-container-active').fadeIn('slow');
            });
        });
    }

    function work_flow_line_width_handler()
    {
        var left = $('#line_start_point').offset().left;
        var right = $('#line_end_point').offset().left;
        var line_width = parseInt(right) - parseInt(left);
        $('#work_flow_line').width(line_width + 1).css('left', left);
    }


    $(document).ready(function(){
        $('.smooth.scroll').on('click',function(){
            $('.smooth').removeClass('active');
            $(this).addClass('active');
        });

        $(".navbar-collapse").css({ maxHeight: $(window).height() - $(".navbar-header").height() + "px" });
    });

</script>

