<script>
jQuery( document ).ready(function() {

function getParameterByName(name) {
    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
        results = regex.exec(location.search);
    return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}

var myCat="";
var myClickAnchor;
var myCat = getParameterByName('category');
//var myCat = "SaaS";
//alert( myCat );

jQuery('.x-portfolio-filters-menu').each(function(){
    jQuery(this).find('li').each(function(){
        var listText = this.innerHTML;
        var myAnchor = jQuery(this).find("a");
        //alert( myAnchor.text() );
        var buttonText = jQuery(listText).text();
        //alert( buttonText );
        if( buttonText == myCat) {
			//myClickAnchor = myAnchor;
            //jQuery( myAnchor.click() );
			jQuery( myAnchor.click(function(){
				jQuery(this).attr('href','http://www.google.com');
			}) );
        }
    });
});

//jQuery( myClickAnchor.click() );

}); // end of doc ready

</script>
