// Set slideShowSpeed (milliseconds)
var slideShowSpeed = 8000

// Duration of crossfade (seconds)
var crossFadeDuration = 3

var Pic = new Array() 

Pic[0] = 'images/rotater/p001.gif'
Pic[1] = 'images/rotater/p002.gif'
Pic[2] = 'images/rotater/p003.gif'
Pic[3] = 'images/rotater/p004.gif'
Pic[4] = 'images/rotater/p005.gif'
