    <div class="field field-name-body field-type-text-with-summary field-label-hidden prose"><div class="field-items"><div class="field-item even" property="content:encoded"><div class="inner-wrap">
<p style="font-size:1px;margin: 0;">&nbsp;</p>
<div id="BambooHR-ATS"></div>
<style>
@import url('http://highlandsolutions.bamboohr.com/css/jobs-embed.css');
.BambooHR-ATS { padding: 0 10px; }
.BambooHR-ATS-board h2 { font-size: 35px; padding-bottom: 24px; border: none !important; }
.BambooHR-ATS-Department-Header { font-family: 'Aleo Regular',sans-serif; font-size: 20px; }
.BambooHR-ATS-board ul ul { margin-bottom: 10px !important; border: none; }
.BambooHR-ATS-board a { font-weight: normal; font-size: 18px; }
.BambooHR-ATS-Department-List > li:last-child ul { padding: 0; }
.BambooHR-ATS-Department-List > li:last-child ul li a { font-size: 22px !important; }
.BambooHR-ATS-Department-List > li:last-child .BambooHR-ATS-Location { display: none; }
</style>
<script>
(function() {
	var divId="BambooHR-ATS";
	var el=document.getElementById(divId);

	var xmlhttp;
	var ieFlag = 0;
	if (('XDomainRequest' in window && window.XDomainRequest !== null) && document.documentMode < 10) {
		xmlhttp=new XDomainRequest();
		ieFlag = 1;
	} else if (window.XMLHttpRequest) {
		xmlhttp=new XMLHttpRequest();
	} else {
		xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	}

	if (ieFlag == 1) { //needed for IE9 CORS
		xmlhttp.onload = loadd;
		xmlhttp.open("GET","//highlandsolutions.bamboohr.com/jobs/embed2.php");
		xmlhttp.send();
	} else {
		xmlhttp.onreadystatechange=function() {
			if(xmlhttp.readyState==4 && xmlhttp.status==200) {
				var content = xmlhttp.responseText;
				if(el) el.innerHTML=content;
				
			}
		}
		xmlhttp.open("GET","//highlandsolutions.bamboohr.com/jobs/embed2.php",true);
		xmlhttp.send();
	}

	function loadd() { //needed for IE9 CORS
		var content = xmlhttp.responseText;
		el.innerHTML=content;
	} 
}) ();
</script>
