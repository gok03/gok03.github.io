     <!-- Start Script Segment

     // This script is Copyright 1997 Nic Jansma, All Rights Reserved



     numlines = 12;  



     StatusBar = new MakeArray(numlines);



     StatusBar[1] = "Dominance Software Development - Adding Dollars To Your Bottom Line !!!"

     StatusBar[2] = "Thank You For Visiting Domsoft.com!!"

     StatusBar[3] = "ACCPAC/SBT Certified"

     StatusBar[4] = "Network Design, Installation And Support"

     StatusBar[5] = "Authorized Citrix Solution Providers"

     StatusBar[6] = "Manufacturing Systems"

     StatusBar[7] = "Distribution Systems"

     StatusBar[8] = "Service Industry Systems"

     StatusBar[9] = "Authorized TIW Solution Providers"

     StatusBar[10] = "Authorized Lahey Solution Providers"

     StatusBar[11] = "Authorized COSS Solution Providers"

     StatusBar[12] = "http://www.domsoft.com"



     DoLoop = 1;   

                   



     Speed = 10;  



     Pause = 1000; 

     

     timerID = null;

     IsRunning = false;

     CurrentMessage = 1;

     CurrentLetter = 0;



     function StartBanner() {

            StopBanner();

            ShowBanner();

     }



     function StopBanner() {

             if (IsRunning) { clearTimeout(timerID); }

           IsRunning = false

     }



     function ShowBanner() {

             if (CurrentMessage > numlines) {

                     if (DoLoop) {

                             CurrentMessage = 1;

                             CurrentLetter = 0; 

                     } else {

                             StopBanner();

                             window.status = ""

                             return;

                     }

             }

             currtext = StatusBar[CurrentMessage]



             if (CurrentLetter > currtext.length) {

                     CurrentLetter = 0

                     CurrentMessage++

                     timerID = setTimeout("ShowBanner()", Pause)

             } else {

                     Msg = currtext.substring(0, CurrentLetter + 1) 

                     window.status = Msg

                     CurrentLetter++

                     timerID = setTimeout("ShowBanner()", Speed)

             }

     }

                             

                     

     function MakeArray(n) { 

                  this.length = n; 

                  for (var i = 1; i <= n; i++) { 

                          this[i] = 0 }

                      return this 

                   



          }

     // End Script -->

     </SCRIPT>







