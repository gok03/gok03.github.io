<script type="text/javascript">
    jQuery(document).ready(function () {
        jQuery('.slicehover').hover(function () {
            jQuery(this).css('transform', 'scale(1.05)');
            jQuery(this).css('margin-left', '13px');
        }, function () {
            jQuery(this).css('transform', 'scale(1)');
            jQuery(this).css('margin-left', '0');
        });
        jQuery('.servicehover').hover(function () {
            jQuery(this).css('transform', 'scale(0.9)');
        }, function () {
            jQuery(this).css('transform', 'scale(1)');
        });
    });
    var tgtgtg = jQuery("section#projects").offset().top;
    var counter_running = false;
    jQuery(document).scroll(function () {
        if (jQuery("section#projects").inView()) {
            if (!counter_running) {
                counter_running = true;
                if (!jQuery('.timer').hasClass('cntcmpl')) {
                    jQuery('.timer').countTo({
                        onComplete: function () {
                            jQuery(this).addClass('cntcmpl');
                        }
                    });
                }
            }

        }
    });
    jQuery.fn.inView = function () {
        //Window Object
        var win = jQuery(window);
        //Object to Check
        obj = jQuery(this);
        //the top Scroll Position in the page
        var scrollPosition = win.scrollTop();
        //the end of the visible area in the page, starting from the scroll position
        var visibleArea = win.scrollTop() + win.height();
        //the end of the object to check
        var objEndPos = (obj.offset().top + obj.outerHeight());
        return(visibleArea >= objEndPos && scrollPosition <= objEndPos ? true : false);
    };
</script>
<script>
    jQuery(document).ready(function () {
        // you want to enable the pointer events only on click;

        jQuery('#map_canvas1').addClass('scrolloff'); // set the pointer events to none on doc ready
        jQuery('#canvas1').on('click', function () {
            jQuery('#map_canvas1').removeClass('scrolloff'); // set the pointer events true on click
        });
        // you want to disable pointer events when the mouse leave the canvas area;
        jQuery("#map_canvas1").mouseleave(function () {
            jQuery('#map_canvas1').addClass('scrolloff'); // set the pointer events to none when mouse leaves the map area
        });
    });
</script>
<script type="text/javascript">
    jQuery(document).ready(function () {
        jQuery('.btn.download').click(function () {
            jQuery('div#dwld-wrap').show('slow');
        });
        jQuery('#dwld-wrap .close').click(function () {
            jQuery('div#dwld-wrap').hide();
        });

        jQuery("#download-form").validate({
            rules: {
                name: {
                    required: true
                },
                email: {
                    required: true,
                    fullEmail: true
                }
            },
            messages: {
                name: "Please specify your name",
                email: {
                    required: "Please specify your email"
                }
            },
            errorPlacement: function (error, element) {
                error.insertAfter(element);
            },
            submitHandler: function () {
                var form_data = jQuery('form.contact').serialize();
                jQuery.ajax({
                    type: "POST",
                    url: 'http://srswebsolutions.com/wp-admin/admin-ajax.php', //process to mail
                    data: {'action': 'save_dwld_user', form_data: form_data},
                    success: function (msg) {
                        jQuery('#pdf-dwld-link22')[0].click();
                        jQuery('div#dwld-wrap').hide();
                    }
                });
            }

        });
        jQuery.validator.addMethod("fullEmail", function (value) {
            var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
            return pattern.test(value);
        }, 'Please enter a valid email address.');
    });
    jQuery(document).mouseup(function (e) {
        var container = jQuery("div#dwld-wrap #form-content");

        if (!container.is(e.target) // if the target of the click isn't the container...
                && container.has(e.target).length === 0) // ... nor a descendant of the container
        {
            jQuery("div#dwld-wrap").hide();
        }
    });
</script>
<script>
    jQuery(document).ready(function () {
        jQuery(".google-test-btn").click(function () {
            var ipval = jQuery("#google-ip input[type='text']").val();
            if (/^(http(s)?:\/\/)?(www\.)?[a-z0-9]+([\-\.]{1}[a-z0-9]+)*\.[a-z]{2,5}(:[0-9]{1,5})?(\/.*)?$/.test(ipval)) {
                var googleUrl = 'https://www.google.com/webmasters/tools/mobile-friendly/?url=' + ipval;
                window.open(googleUrl, '_blank');
            } else {
                alert("Invalid URL");
                return false;
            }
            return true;
        });

        jQuery("#google-ip input[type='text']").keypress(function (e) {
            var key = e.which;
            if (key == 13)  // the enter key code
            {
                jQuery('.google-test-btn').click();
                return false;
            }
        });
    });
</script>
