<SCRIPT language=javascript>

function ChangeNavLinkColorOn(menunumer) {	

	var x;

	x = eval("document.all.topnavcell" + menunumer);

	x.bgColor = "107BA8";

}

function ChangeNavLinkColorOff(menunumer) {

	var x;

	x = eval("document.all.topnavcell" + menunumer);

	x.bgColor = "F0F0E0";

}

</SCRIPT>
<script>
var myvar;
function menuinit() {
        document.getElementById('m1').style.display = 'none';
        document.getElementById('m2').style.display = 'none';
        document.getElementById('m3').style.display = 'none';
	document.getElementById('pm1').src = '../img/plus.png';
	document.getElementById('pm2').src = '../img/plus.png';
	document.getElementById('pm3').src = '../img/plus.png';
}
function menuexpand (i) {
        menuinit();
        if (myvar == i) {
		document.getElementById('p' + i).src = '../img/plus.png';
		document.getElementById(i).style.display = 'none';
		myvar = '';
	}
        else {
		document.getElementById('p' + i).src = '../img/minus.png';
		document.getElementById(i).style.display = 'block';
		myvar = i;
	}
}
</script>
</HEAD>

<BODY bgcolor="#FFFFFF" marginheight=1 marginwidth=1 topmargin=1 leftmargin=1 onLoad="menuinit();">

