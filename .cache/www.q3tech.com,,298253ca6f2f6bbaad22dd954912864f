<script type="text/javascript">
    function setLink(link){
        jQuery('#error_msg').hide();
        jQuery('#succ_msg').hide();
        jQuery('#send_msg').html('');
        jQuery('#contact').show();
		jQuery('#send').show();
        jQuery("#email").removeClass("error");
        document.getElementById('pdfurl').value = link;
    }
</script>  

</div>

<script type="text/javascript">
jQuery.noConflict();
    function validateEmail(email) { 
        var reg = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return reg.test(email);
    }
    jQuery(document).ready(function() {
        jQuery("#contact").submit(function() { return false; }); 
        jQuery("#send").on("click", function(){
            var emailval  = jQuery("#email").val();
            var mailvalid = validateEmail(emailval);
            
            if(mailvalid == false) {
                jQuery("#email").addClass("error");
                jQuery("#error_msg").show();
            }
            else if(mailvalid == true){
                jQuery("#email").removeClass("error");
                jQuery("#error_msg").hide();
            }
            if(mailvalid == true) {
                jQuery("#send_msg").replaceWith("");              
                jQuery.ajax({
                    type: 'POST',
                    url: 'http://www.q3tech.com/wp-content/themes/graphene/sendpdf-link.php',
                    data: jQuery("#contact").serialize(),
                    success: function(data) {
                        if(data == "true") {
                            jQuery("#contact").fadeOut("fast", function(){
                                jQuery("#send_msg").html('');
                                jQuery('#send_msg').hide();
                                jQuery("#succ_msg").show(); 
								jQuery('#send').hide();
								jQuery("#email").val('');
								
                            });
                        }
                    }
                });
            }
        });
    });
  
</script>
