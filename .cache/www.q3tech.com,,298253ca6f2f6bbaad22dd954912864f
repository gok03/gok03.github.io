function grabSearch(){
  //alert("test");
  var hasError = false;
  var searchReg = /^[a-zA-Z0-9-\s]+$/;
  var lang = '';
  var curLang = 'en';
   var search=trim_string(document.getElementById('s').value);
  //alert(search);
  if(lang=='de' || curLang=='de'){
	if((search == "") || (search == 'Suche')){
			alert('Bitte geben Sie Suchkriterien ein.');
		document.getElementById("s").focus();
		  hasError = true;
		}/*else if(!searchReg.test(search)) {
				alert('Enter valid text');
		  document.getElementById("s").focus();
				hasError = true;
			}*/
		 if(hasError == true) {return false;}
  } else if(lang=='es' || curLang=='es'){
	if((search == "") || (search == 'Buscar')){
			alert('por favor da su criterio de buscar.');
		document.getElementById("s").focus();
		  hasError = true;
		}/*else if(!searchReg.test(search)) {
				alert('Enter valid text');
		  document.getElementById("s").focus();
				hasError = true;
			}*/
		 if(hasError == true) {return false;}
  } else {
  if((search == "") || (search == 'Search')){
        alert('Please enter search criteria.');
    document.getElementById("s").focus();
      hasError = true;
    }/*else if(!searchReg.test(search)) {
            alert('Enter valid text');
      document.getElementById("s").focus();
            hasError = true;
        }*/
     if(hasError == true) {return false;}
	 }
 }
  function trim_string(txt) {
  return txt.replace(/^\s+|\s+$/g,"");
}
