<script type="text/javascript">
  $(document).ready(function(){
	//Function to start slick slider
	 $('.slider-dual').slick({
	   autoplay: true,
	   autoplaySpeed:10000,
	  slidesToShow: 1,
	  slidesToScroll: 1,
	  arrows: true,
	   dots: false,
	   infinite:true,
	   centerMode: true,
	  fade: true,
	  asNavFor: '.slider-nav',
	   responsive: [
		{
		  breakpoint: 480,
		  settings: {
			   arrows: false,
			   dots: true,
			centerMode: true,
			centerPadding: '10px',
			slidesToShow: 1
		  }
		},
													
		{
		  breakpoint: 982,
		  settings: {
		   dots: true,
		   infinite:true,
		   arrows: false,
		   centerMode: true,
		   slidesToShow: 1
		  }
		}                                           
									 
	]
	});
									 
	//Function to start slick navigation
	$('.slider-nav').slick({
	  slidesToShow: 8,
	  slidesToScroll: 0,
	  asNavFor: '.slider-dual',
	   infinite:false,
	   touchMove: false,
	  centerMode: false,
	  focusOnSelect: true
	});									 
	 $('.slider-dual').on('afterChange', function(event,slick,i){
					   $('.slider-nav .slick-slide').removeClass('slick-current');
					   $('.slider-nav .slick-slide').eq(i).addClass('slick-current');    
									
	 });									 
});									 
									 
</script>


<script type="text/javascript">
		//Function to show and hide on hover if data-id match
			$( document ).ready(function() {
				
				$('.thumb-slider').last().css('margin-right', 0);
				$(".thumb-slider").hover(
				
					function() {
						var elementID = $(this).attr("data-id");
						var thumb = $(this).offset().left - $(".slick-list").offset().left
						
						
						 $('span[data-id=' + elementID + ']').css('left', thumb+10);
						 
						 $('[data-id=' + elementID + ']').show();
					
					}, function() {
						var elementID = $(this).attr("data-id");
						  $('span[data-id=' + elementID + ']').hide();
						 
				 });				 
				 
				 $('.slider-nav .slick-track').each(function() {
				  var $this = $(this);
				 $('.slider-nav .slick-track').addClass("centered-"+$this.find('.thumb-slider').length);
				 
				 
				});		
			});
		</script>
						</div>

						
					
					
				
			
		
	





	
