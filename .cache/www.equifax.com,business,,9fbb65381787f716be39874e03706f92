<script>
	function ctem_null_null_3_search() {
		var keywords = document.ctem_null_null_3_fm._3_keywords.value;

		keywords = keywords.replace('Enter Key Words', '');
		keywords = keywords.replace(/^\s+|\s+$/, '');
		keywords = $.trim(keywords)
		
		
		if (keywords != '' && keywords != 'Enter Key Words') {
			document.ctem_null_null_3_fm.submit();
		}
	}
</script>

	
	

					
				
			
		
	
	



