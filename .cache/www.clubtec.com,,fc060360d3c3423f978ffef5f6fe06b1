function doPrintContent(PrintLayoutID,DocTitle){
	
	if(!PrintLayoutID) PrintLayoutID="-9";
	var bDHTML=true;
	try{
		var sContentHTML=document.getElementById("fwContent").innerHTML;
		if(!sContentHTML) bDHTML=false;
	}catch(e){
		bDHTML=false;
	}
	
	var sTitleParam=""
	if(DocTitle){sTitleParam="&DocTitle="+DocTitle};
	
	if(bDHTML){
		var printWin=window.open("/fw/main/fw_print.asp?PrintLayoutID="+ PrintLayoutID+sTitleParam, "PrintWindow", "height=500,width=780,left=20,top=20,location=no,menubar=yes,resizable=yes,scrollbars=yes,status=yes,toolbar=no;");
	}else{
		var printWin=window.open("/fw/main/default.asp?PrintLayoutID="+ PrintLayoutID+sTitleParam+"&SetUsersGridState=", "PrintWindow", "height=500,width=780,left=20,top=20,location=no,menubar=yes,resizable=yes,scrollbars=yes,status=yes,toolbar=no;");
	}
}
</script>
