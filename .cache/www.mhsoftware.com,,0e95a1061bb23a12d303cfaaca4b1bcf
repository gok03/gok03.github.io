    function showVideo() {
        window.open("http://www.mhsoftware.com/hosting/video/SalesVideo.html",
                    "VidWin_"+(new Date().getTime()),
                    "scrollbars=no,resizable=no,status=no,location=no,menubar=no,toolbar=no,width=980,height=600,left="+getPopLeft(980)+",top="+getPopTop(600));
    }
    function showWordPressVideo() {
        window.open("/tutorials/WordPressCalendar/",
                    "VidWin_"+(new Date().getTime()),
                    "scrollbars=no,resizable=no,status=no,location=no,menubar=no,toolbar=no,width=980,height=750,left="+getPopLeft(980)+",top="+getPopTop(750));
    }

	function showREVideo(){
window.open("/tutorials/RE/",
                    "VidWin_"+(new Date().getTime()),
                    "scrollbars=no,resizable=no,status=no,location=no,menubar=no,toolbar=no,width=980,height=600,left="+getPopLeft(980)+",top="+getPopTop(600));
	}
	function showFBVideo(){
window.open("/tutorials/SocialNetworkIntegration/",
                    "VidWin_"+(new Date().getTime()),
                    "scrollbars=no,resizable=no,status=no,location=no,menubar=no,toolbar=no,width=980,height=600,left="+getPopLeft(980)+",top="+getPopTop(600));
	}

    var preloadimages=new Array("SiteButtons_1_mouseover.jpg","SiteButtons_1_mouseclick.jpg","SiteButtons_3_mouseover.jpg","SiteButtons_3_mouseclick.jpg","SiteButtons_4_mouseover.jpg","SiteButtons_4_mouseclick.jpg","PlayVideo_hT.png");
    var theimages=new Array();
    for (p=0;p<preloadimages.length;p++)
    {
        theimages[p] = new Image();
        theimages[p].src = "/images/"+preloadimages[p];
    }
    function roll(img_name, img_src)
    {
        document.getElementById(img_name).src = "/images/"+img_src;
    }
