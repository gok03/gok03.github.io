<SCRIPT LANGUAGE="Javascript">

function Publish(s) {
  var t = document.URL;
  if (t.indexOf(".exe") == -1) {
    return ("/ic/icstore/cgi-bin/" + s) ;
  } else {
    return (s);
 }
}


function connector() {
  var s = document.URL;
  if (s.indexOf(".exe") == -1) {
    return ".htm?";
  } else {
    return "&";
  }
}
</script>

