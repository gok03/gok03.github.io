<script type="text/javascript" defer="defer">
//<![CDATA[
var SlideShow6773516initilized = false;
var SlideShow6773516 = new function() { };
LCMS.jq.extend(SlideShow6773516, slideShowClass);
//initialize the containers to that hold the previous, current and next images.
SlideShow6773516.autoTransitionWait = 10;
SlideShow6773516.container = LCMS.jq("#hpSlider_11_11_ctl00_SlideShowImageContent");
SlideShow6773516.currentContentContainer = LCMS.jq("#hpSlider_11_11_ctl00_current");
SlideShow6773516.nextContentContainer = LCMS.jq("#hpSlider_11_11_ctl00_next");
SlideShow6773516.previousContentContainer = LCMS.jq("#hpSlider_11_11_ctl00_previous");
//initialization function to set up the slideshow.  Once initialized the js will start the timer.


//set the height and the width of the container that contains the image and the caption so that the caption appears attached to the image
SlideShow6773516.resizeCaptionContainer = function() {
	var $img = LCMS.jq(this);
	$img.parents('.captionContainer').eq(0).width($img.width()).height($img.height());
};

SlideShow6773516.setContainers = function(current_container) {
	//set up the containers for the next transition
	if (current_container === undefined) current_container = 'index';
	
	switch (current_container) {
		case 'next':
			SlideShow6773516.currentContentContainer.html(SlideShow6773516.nextContentContainer.html());
		break;
		case 'prev':
			SlideShow6773516.currentContentContainer.html(SlideShow6773516.previousContentContainer.html());
		break;
		default:
			SlideShow6773516.currentContentContainer.html(SlideShow6773516.contents[SlideShow6773516.currentContentIndex]);
	}
    SlideShow6773516.nextContentContainer.html(SlideShow6773516.contents[SlideShow6773516.NextContentIndex(SlideShow6773516)]);
    SlideShow6773516.previousContentContainer.html(SlideShow6773516.contents[SlideShow6773516.PreviousContentIndex(SlideShow6773516)]);
};



SlideShow6773516.InitializeTransition = function() 
{
    if (SlideShow6773516initilized) return;
    SlideShow6773516initilized = true;
	//load the containers based on the currentContentIndex
    SlideShow6773516.setContainers();
    
    SlideShow6773516.container.append(SlideShow6773516.currentContentContainer.html());
    
    
    	
    	var current_caption_container = SlideShow6773516.container.children('.SlideShowImage').first().children('.captionContainer')

        //set the captionbox to resize on the image load event
        if (current_caption_container.children('img').length > 0) {
			current_caption_container.children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow6773516.resizeCaptionContainer);
        } else {
        	current_caption_container.children().children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow6773516.resizeCaptionContainer);
        }
}

// Perform transition is run after the timer expires or when the next/prev arrows are clicke
SlideShow6773516.PerformTransition = function(trans_dir) 
{
	if (SlideShow6773516.transitioning == true) return;    
    var is_fwd = (trans_dir === 'next');
    
    // next_img only references the correct element before setContainers is called!
    var next_img = is_fwd ? SlideShow6773516.nextContentContainer : SlideShow6773516.previousContentContainer;
    var next_caption_container = next_img.find('.captionContainer');
	if (next_caption_container.children('img').length > 0) {
		next_caption_container.children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow6773516.resizeCaptionContainer);
	} else {
		next_caption_container.children().children('img').unbind('load.LCMSslideshow').bind('load.LCMSslideshow', SlideShow6773516.resizeCaptionContainer);
	}
    
    
    
    
    
        next_img.css('display', 'block');
        if (next_img.children('.SlideShowImage').eq(0).children().children('img').length > 0) {
			next_img.children('.SlideShowImage').eq(0).children().children('img').first().each(SlideShow6773516.resizeCaptionContainer);
        } else {
        	next_img.children('.SlideShowImage').eq(0).children().children().children('img').first().each(SlideShow6773516.resizeCaptionContainer); 
        }

        
        
        
        
	        //hide the current container
	        next_img.css('display', 'none');
	        var fadeDiv = SlideShow6773516.container.children('.SlideShowImage').eq(0);
	        fadeDiv.fadeTo(0.8 * 500, 0, function() {                
	            fadeDiv.html(next_img.children(":first").clone(true));
	            next_img.css('display', 'block');
	            SlideShow6773516.container.parent().parent().height(next_img.height());
	            next_img.css('display', 'none');
	            SlideShow6773516.setContainers(trans_dir);
	            fadeDiv.fadeTo(0.8 * 500, 1);
	        });
	        
	        
        
}

function nextSlide6773516() {
	SlideShow6773516.MoveToNextSlide(SlideShow6773516, 'SlideShow6773516');
}
function previousSlide6773516() {
	SlideShow6773516.MoveToPreviousSlide(SlideShow6773516, 'SlideShow6773516');
}

SlideShow6773516.SetContents = function() {
    var imageContainer = LCMS.jq("#hpSlider_11_11_ctl00_divImages");
    var containerWidth = imageContainer.width();
    imageContainer.css("width", containerWidth + "px");
    
	SlideShow6773516.contents = [];

        //piece together the slideshowimage div based on properties set in the photo gallery settings
        //SlideShowImage is the base of all images
        var content = '<div class="SlideShowImage"><div class="captionContainer"';
        //if we have the properties from the settings we pass them in and set up the resizer
        
        content = content + ">";
        //code behind returns "" if the link is null or empty, otherwise it creates the anchor tag
        content = content + "";
        //base for the image tag
        content = content + "<img src=";
        //if we have the properties from the settings we pass them in and set up the resizer
         
        content = content + "\"/ResizeImage.aspx?m=" + containerWidth + "&img=%2fWebsites%2fgocodes%2fPhotoGallery%2f5468087%2fslide-phone.jpg&28708\""; 
         
        content = content + " alt=\"Assets and Inventory   Tracking System\" />";
        //code behind returns "" if the link is null or empty, otherwise it creates the closing anchor tag
        content = content + "";
        content = content + "";
        //if we have a caption the codebehind returns the div for the caption otherwise it returns ""
        
        content = content + "<div class=\"sldw_caption\"><h3>Assets and Inventory <br> Tracking System</h3></div>";
		
        content = content + "</div></div>";
        SlideShow6773516.contents.push(content);
    
