var SlideShow6773516Started = false;
LCMS.jq(function() {
    if (SlideShow6773516Started)
        return;
    SlideShow6773516Started = true;
	SlideShow6773516.SetContents(); 
	SlideShow6773516.StartSlideShow(SlideShow6773516, 'SlideShow6773516');
    LCMS.jq("#hpSlider_11_11_ctl00_divImagesMain")
        .mouseenter(function() {
		    LCMS.jq('#SlideShow6773516Navigation').css('visibility', 'visible');
		    SlideShow6773516.StopSlideShow(SlideShow6773516)
	    })
        .mouseleave(function() {
		    LCMS.jq('#SlideShow6773516Navigation').css('visibility', 'hidden');
		    SlideShow6773516.ResumeSlideShow(SlideShow6773516, 'SlideShow6773516');
	    });
	
		
	
       		var resize_timer = null;
       		
			LCMS.jq(window).bind('resize', function() {
				if (!resize_timer) { // timer to throttle how many times this fires on resize
					resize_timer = setTimeout(function() {
						
						var findCurrentMsize = function(img_src){
							var start_str = img_src.indexOf('?m='), first_amp = img_src.indexOf("&");
							return img_src.substring(start_str + 3, first_amp);
						};
						
						var	resizeM, 
							container_width = LCMS.jq('#hpSlider_11_11_ctl00_divImages').parent().width(),
							current_m_val = findCurrentMsize(LCMS.jq('.captionContainer img', SlideShow6773516.currentContentContainer).first().attr('src')) - 0; // - 0 to convert to number
						
						
						// only resize if our resize width doesn't match the container
						if (current_m_val !== container_width) {
							
							resizeM = function(img_src) {
								var start_str = img_src.indexOf('?m='), first_amp = img_src.indexOf("&");
								var old_str = img_src.substring(start_str, first_amp);
								return img_src.replace(old_str, '?m=' + container_width);
							};
							
							LCMS.jq('#hpSlider_11_11_ctl00_divImages').parent('.SlideShowContainer').css("height", "auto");
							LCMS.jq('#hpSlider_11_11_ctl00_divImages, #hpSlider_11_11_ctl00_divImages .captionContainer').removeAttr('style');
							LCMS.jq('#hpSlider_11_11_ctl00_divImages .SlideShowImage').css({'width': 'auto'});
							
							
							// resize the slideshow images currently showing
							LCMS.jq('#hpSlider_11_11_ctl00_divImages .captionContainer img').each(function() {
								var $ele = LCMS.jq(this);
								$ele.attr('src', resizeM($ele.prop('src'))).css({display: 'block'}); // set new src
								
								// manually fire load event, this is needed for some browsers that wont fire if the image gets cached. Some browsers will fire the event twice because of this but that's ok.
								if ($ele.prop('complete')) { $ele.trigger('load'); }													
							});
					
					
							// resize everything (in the array)
							LCMS.jq.each(SlideShow6773516.contents, function(indx, img_src) {
								SlideShow6773516.contents.splice(indx, 1, resizeM(img_src));	
							});

							
							LCMS.jq('#hpSlider_11_11_ctl00_divImages, #hpSlider_11_11_ctl00_divImages .SlideShowImage').css({'width': container_width});
							

							
						}

						resize_timer = null;				
					}, 300);	
				}				
			});        
    
});
