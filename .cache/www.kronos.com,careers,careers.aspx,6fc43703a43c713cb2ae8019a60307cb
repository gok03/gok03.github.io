<script type="text/javascript" src="/App_Pages/Plugins/typeahead/typeahead.bundle.min.js"></script>
<script type="text/javascript">
  

    $(document).ready(function () {
        
        var _langid = $(".SiteLangId");
        var _appendLangId = "";
        if (_langid.length !== 0 && _langid.val() != 'undefined') {
            _appendLangId = "?langid=" + _langid.val();
        }
        
        var keywords = new Bloodhound({
            datumTokenizer: function (d) { return Bloodhound.tokenizers.whitespace(d.name); },
            queryTokenizer: Bloodhound.tokenizers.whitespace,
            identify: function (d) { return d.id; },
            prefetch: {
                url: '/App_Handlers/Handler_GetSearchKeyWords.ashx' + _appendLangId,
                cache: false
            }

        });

        function keywordsWithDefaults(q, sync) {
            if (q === '') {
                var arr = [];
                for (var i = 0; i < 5; i++) {
                    arr[i] = i;
                }
                sync(keywords.get(arr));
            }

            else {
                keywords.search(q, sync);
            }
        }

        $('input#searchField').typeahead({
            minLength: 0,
            hint: true,
            highlight: true
        },
        {
            source: keywordsWithDefaults,
            displayKey: function (keywords) {
                return keywords.name;
            }
        });

        $('input#searchField').bind('typeahead:selected', function (obj, datum, name) {
            //alert(JSON.stringify(obj)); // object
            //// outputs, e.g., {"type":"typeahead:selected","timeStamp":1371822938628,"jQuery19105037956037711017":true,"isTrigger":true,"namespace":"","namespace_re":null,"target":{"jQuery19105037956037711017":46},"delegateTarget":{"jQuery19105037956037711017":46},"currentTarget":
            //alert(JSON.stringify(datum)); // contains datum value, tokens and custom fields
            //// outputs, e.g., {"redirect_url":"http://localhost/test/topic/test_topic","image_url":"http://localhost/test/upload/images/t_FWnYhhqd.jpg","description":"A test description","value":"A test value","tokens":["A","test","value"]}
            //// in this case I created custom fields called 'redirect_url', 'image_url', 'description'   

            //alert(JSON.stringify(name)); // contains dataset name
            //// outputs, e.g., "my_dataset"

            var s_text = datum.name;
            var uet = escape(s_text);
            var ssa = $("input#SiteSearchAlias").val();
            var rdu = ssa + "?searchtext=" + uet;
            window.location = rdu;
        });

        var searchtext = function () {
            var uet = $("input#searchField").val();
            uet = escape(uet);
            var ssa = $("input#SiteSearchAlias").val();
            var rdu = ssa + "?searchtext=" + uet;
            window.location = rdu;
        };


        $("img#searchButton").click(function () {
            searchtext();
        });

        $("input#searchField").keydown(function (event) {
            if (event.which || event.keyCode) {
                if ((event.which == 13) || (event.keyCode == 13)) {
                    event.preventDefault();
                    searchtext();
                }
            };
        });
    });
</script>
<style>
    img#searchButton:hover {
        cursor: pointer;
    }

    #searchBox .tt-menu {
        width: 120%;
        margin: 12px 0;
        padding: 8px 0;
        background-color: #fff;
        border: 1px solid #ccc;
        border: 1px solid rgba(0, 0, 0, 0.2);
        -webkit-border-radius: 8px;
        -moz-border-radius: 8px;
        border-radius: 8px;
        -webkit-box-shadow: 0 5px 10px rgba(0,0,0,.2);
        -moz-box-shadow: 0 5px 10px rgba(0,0,0,.2);
        box-shadow: 0 5px 10px rgba(0,0,0,.2);
    }

        #searchBox .tt-menu .tt-suggestion:hover {
            background-color: #e9f4fa;
            cursor: pointer;
        }

        #searchBox .tt-menu .tt-dataset {
            margin-left: 10px;
        }
</style>

        </div>
    </div>

    
</div>

