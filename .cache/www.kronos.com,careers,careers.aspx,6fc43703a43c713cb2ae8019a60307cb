$(document).ready(function () {

$('.mailto').on('click', function (e) {
e.preventDefault();
var mailtolink = $('#MailToLink').text();
var idx = mailtolink.indexOf('mailto:?subject=');
if (mailtolink != '' && idx > -1)
window.location.href = mailtolink;
});

$('.pdf-builder').on('click', function (e) {
e.preventDefault();
var pdflink = $('#PdfLink').text();
if (pdflink != '')
window.location.href = pdflink;
});

});

