<script type="text/javascript">

    /* <![CDATA[ */

    function getLocaleSpecificGoogleAnalyticsAccount()
    {
        var hiddenFieldID = 'ctl00_ctl00_SiteAnalytics_GoogleAnalyticsAccount';
        var googleAnalyticsAccount = document.getElementById(hiddenFieldID).value;
        return googleAnalyticsAccount;
    }

    /* ]]> */

</script>

<!-- Generc function to populate the href value of any dom element ID named "ContactKronos, ContactCTADescription, or ContactCTAButton with our link to the contact form on eloqua -->
<script type="text/javascript">

    /* <![CDATA[ */

    $(document).ready(function ()
    {
        var contactUsLinkID = 'ContactUsLink';
        var contactUsLinkValue = $("#" + contactUsLinkID).val();
        var nextGenUIContactButton = document.getElementById("ContactKronos");
        var contactCTADescription = document.getElementById("ContactCTADescription");
        var contactCTAButton = document.getElementById("ContactCTAButton");

        if (contactUsLinkID != null)
        {

            if (nextGenUIContactButton != null)
                nextGenUIContactButton.setAttribute("href", contactUsLinkValue);

            if (contactCTADescription != null)
                contactCTADescription.setAttribute("href", contactUsLinkValue);

            if (contactCTAButton != null)
                contactCTAButton.setAttribute("href", contactUsLinkValue);

        }

        // dma (02/10/2014)
        // send _trackEvent to google to capture viewport 3 seconds after page has loaded
        // dma (09/26/2014)
        // commenting out trackViewPort
        //setTimeout(trackViewPort, 3000);

        $('a.ContactKronosLink').each(function ()
        {
            $(this).attr('href', $('#ContactUsLink').val());
        });

    });

    /* ]]> */

</script>


            

