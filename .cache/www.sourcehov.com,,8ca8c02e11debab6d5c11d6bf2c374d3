<script>
		jQuery(document).ready(function () {
			jQuery('.do-content').hide();
			jQuery('.do-content-viwer').click(function (e) {
				e.preventDefault();
				if (jQuery('.do-content').is(':visible')) {
					jQuery('html,body').animate({scrollTop: jQuery('.blue_wrapper').offset().top}, 1000);
					jQuery('.do-content-viwer').val('+').css({'transform': 'rotate(90000deg)', 'animation-delay': '5s'});
				}
				else {
					jQuery('.do-content-viwer').val('+').css({'transform': 'rotate(46deg)', 'animation-delay': '5s'});				
				}
				jQuery('.do-content').slideToggle('slow');
				return false;
			});

			});

</script> 
<!-- :::::::::::::::::::::::::::::::::-->
<!-- ::: What we do section Sliding : End :-->
<!-- :::::::::::::::::::::::::::::::::-->





<!-- :::::::::::::::::::::::::::::::::-->
<!-- ::: Box Shadow : Start :-->
<!-- :::::::::::::::::::::::::::::::::-->
<script>if (!('boxShadow' in document.body.style)) {
        document.body.setAttribute('class', 'noBoxShadow');
    }

    document.body.addEventListener("click", function (e) {
        var target = e.target;
        if (target.tagName === "INPUT" &&
                target.getAttribute('class').indexOf('liga') === -1) {
            target.select();
        }
    });

    (function () {
        var fontSize = document.getElementById('fontSize'),
                testDrive = document.getElementById('testDrive'),
                testText = document.getElementById('testText');
        function updateTest() {
            testDrive.innerHTML = testText.value || String.fromCharCode(160);
            if (window.icomoonLiga) {
                window.icomoonLiga(testDrive);
            }
        }
        function updateSize() {
            testDrive.style.fontSize = fontSize.value + 'px';
        }
        fontSize.addEventListener('change', updateSize, false);
        testText.addEventListener('input', updateTest, false);
        testText.addEventListener('change', updateTest, false);
        updateSize();
    }());

</script> 
<!-- :::::::::::::::::::::::::::::::::-->
<!-- ::: Box Shadow : End :-->
<!-- :::::::::::::::::::::::::::::::::-->


<script>

    jQuery(window).scroll(function () {
        var fromTopPx = 3; // distance to trigger
        var scrolledFromtop = jQuery(window).scrollTop();
        if (scrolledFromtop > fromTopPx) {
            jQuery('.form-search').addClass('form-search1 ');
        } else {
            jQuery('.form-search').removeClass('form-search1');
        }
    });

</script>



<script>

    jQuery(window).scroll(function () {
        var fromTopPx = 3; // distance to trigger
        var scrolledFromtop = jQuery(window).scrollTop();
        if (scrolledFromtop > fromTopPx) {
            jQuery(' .form-group:hover').addClass('form-group1.hover  ');
        } else {
            jQuery('.form-group.hover').removeClass('form-group1.hover ');
        }
    });

</script>

<script>

    jQuery(window).scroll(function () {
        var fromTopPx = 3; // distance to trigger
        var scrolledFromtop = jQuery(window).scrollTop();
        if (scrolledFromtop > fromTopPx) {
            jQuery(' .form-control').addClass('form-control1  ');
        } else {
            jQuery('.form-control').removeClass('form-control1 ');
        }
    });

</script>

<script>

    jQuery(window).scroll(function () {
        var fromTopPx = 3; // distance to trigger
        var scrolledFromtop = jQuery(window).scrollTop();
        if (scrolledFromtop > fromTopPx) {
            jQuery('.dropdown-menu > li > a').addClass('wb');
        } else {
            jQuery('.dropdown-menu > li > a').removeClass('wb');
        }
    });

</script>


