<script>
 jQuery(window).scroll(function () {
        var fromTopPx = 3; // distance to trigger
        var scrolledFromtop = jQuery(window).scrollTop();
        if (scrolledFromtop > fromTopPx) {
			jQuery('.search-group').css({"background-color":"#455560", "width":"100%", "border-radius": "25px","border": "0px solid #ccc",'color':'#fff'});
			jQuery('.search-icon').css({"color":"#fff"});
			jQuery('.search-hover').css({"color":"#455560"});
			jQuery('.active').css({"color":"#fff"});
		
		} else {
            jQuery('.search-group').css({"background-color":"#fff", "width":"100%", "border-radius": "25px","border": "0px solid #ccc",'color':'#455560'});
			jQuery('.search-icon').css({'color':'#455560'});
			jQuery('.search-hover').css({"color":"#fff"});
			jQuery('.active').css({"color":"#455560"});
        }
	});	
</script>

<script>	
jQuery(document).ready(function() {

	jQuery('#search-block-form').addClass('navbar-form');
    jQuery("#edit-search-block-form--2").hide();
    jQuery("#search-btn").on("mouseenter touchstart", function(e) {	
        jQuery("#edit-search-block-form--2").show('slow');
       // e.stopPropagation();
	     jQuery('#search-btn').addClass('search-hover');
		 jQuery("#edit-search-block-form--2").addClass('active');
				 
    });

    if(jQuery(window).width() < 767){
				 jQuery("#edit-search-block-form--2").show();
		}

    jQuery("body").click(function(e) {
		
		if(jQuery(window).width() < 767){
		  
			 if (!jQuery(e.target).is('#edit-search-block-form--2, #edit-search-block-form--2 *, .btn, .btn *')) {		    
            jQuery("#edit-search-block-form--2").show();
		   //	jQuery('#search-btn').removeClass('search-hover');
			 //jQuery("#edit-search-block-form--2").removeClass('active');				
        }					
		}else {
        if (!jQuery(e.target).is('#edit-search-block-form--2, #edit-search-block-form--2 *, .btn, .btn *')) {		    
            jQuery("#edit-search-block-form--2").hide('slow');
			jQuery('#search-btn').removeClass('search-hover');
			 jQuery("#edit-search-block-form--2").removeClass('active');				
        }				
				}
    });	
});
jQuery(window).load(function() {
  if (jQuery('#edit-search-block-form--2').hasClass('active')){
           jQuery('#search-btn').addClass('search-hover');
        } else {
            jQuery('#search-btn').removeClass('search-hover');
          }
		  });
</script>


