<script>
    var comma_separator_number_step = jQuery.animateNumber.numberStepFactories.separator(',');
    var counter1 = jQuery('#counter1').text();
    var counter2 = jQuery('#counter2').text();
    var counter3 = jQuery('#counter3').text();
    var counter4 = jQuery('#counter4').text();


    //jQuery('.counter1').animateNumber({ number: counter1 });

    jQuery('.counter1').animateNumber(
            {
                number: counter1,
                numberStep: comma_separator_number_step
            }
    );

    jQuery('.counter2').animateNumber(
            {
                number: counter2,
                numberStep: comma_separator_number_step
            }
    );

    jQuery('.counter3').animateNumber(
            {
                number: counter3,
                numberStep: comma_separator_number_step
            }
    );

    jQuery('.counter4').animateNumber(
            {
                number: counter4,
                numberStep: comma_separator_number_step
            }
    );

    //jQuery('.counter3').animateNumber({ number: counter3 });
    //jQuery('.counter4').animateNumber({ number: counter4 });
</script> 
