if( jQuery('#edit-search-block-form--2').val().length === 0 ) {	   
        //alert("Please enter search criteria");
	    //jQuery('#edit-search-block-form--2').css('background': '#FEF1EC');
		//jQuery("#edit-search-block-form--2").addClass('errorClass');
		//jQuery('[data-toggle="tooltip"]').tooltip();  	 
		  event.preventDefault();
	    //jQuery("#edit-search-block-form--2").show('slow');
		 return  false;
    }
});
</script>

