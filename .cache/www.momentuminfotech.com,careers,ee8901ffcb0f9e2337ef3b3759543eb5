     <script type="text/javascript">
        $(document).ready(function () {

            GetFileTypes();
            //   GetJobs();
            $("input[type=file]").customFileInput();
            //  $('#PortfolioURL').val('http://');
            $("form")
                    .bootstrapValidator({
                        feedbackIcons: {
                            valid: 'glyphicon glyphicon-ok',
                            invalid: 'glyphicon glyphicon-remove',
                            validating: 'glyphicon glyphicon-refresh'
                        }
                    }).on('success.form.bv', function (e) {
                     
                        document.getElementById('formCareer').action = document.getElementById('formCareer').action.replace('No-Spam', 'AddCareer');

                    });

        });
        function GetFileTypes() {
            $.ajax({
                url: "/Careers/GetFileTypes",
                data: { pkId: 1 },
                type: "Get", cache: false,
                success: function (data) {
                    $('#fileType').html('Accepted files : ' + data.result + '');
                },
                error: function (data) {
                    // alert('error : ' + data);
                }
            });
        }
        function GetJobs() {
            $.ajax({
                url: "/Careers/GetJobs",
                data: { pkId: 1 },
                type: "Get", cache: false,
                success: function (data) {

                    var json = $.parseJSON(data.result);
                    var newDiv = '';
                    for (var i = 0, len = data.result.length; i < len; ++i) {
                        var job = data.result[i];
                        newDiv = newDiv + "<li> <a href ='jobs/" + job.PageName.toLowerCase() + "' >" + job.PageTitle + "</a></li>";

                    } $('#divJobs').append("<ul class='jobs'>" + newDiv + "</ul>");

                },
                error: function (data) {
                    // alert('error : ' + data);
                }
            });
        }
    </script>

