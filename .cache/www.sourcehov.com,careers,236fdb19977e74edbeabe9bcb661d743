<script>

	jQuery(window).scroll(function () {	  
        var fromTopPx = 3; // distance to trigger
        var scrolledFromtop = jQuery(window).scrollTop();
        if (scrolledFromtop > fromTopPx) {
            jQuery('.logo').css({"background": "#ffffff"});
        } else {
            jQuery('.logo').css({"background": "transparent"});
        }
	  });
	  
</script> 


<script>
    jQuery(window).scroll(function () {
        var fromTopPx = 3; // distance to trigger
        var scrolledFromtop = jQuery(window).scrollTop();
        if (scrolledFromtop > fromTopPx) {
            jQuery('.c_logo').addClass('c_logo1');
        } else {
            jQuery('.c_logo').removeClass('c_logo1');
        }
    });
</script>

