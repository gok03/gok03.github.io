<script>
jQuery(window).scroll(function() {
	if (jQuery(window).scrollTop() > 3 ){
    jQuery('.logo').addClass('show');
  } else {
    jQuery('.logo').removeClass('show');
  };   	
});

jQuery('.scroll').on('click', function(e){		
	e.preventDefault()
});
</script>

