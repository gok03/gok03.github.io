<script>
 
  jQuery(document).ready(function () {
		jQuery('.regional-contact-info').hide();
			jQuery('.headquarters-viwer').click(function (e) {
				e.preventDefault();
				if (jQuery('.regional-contact-info').is(':visible')) {
					jQuery('.headquarters-viwer').val('+').css({'transform': 'rotate(90000deg)', 'animation-delay': '5s'});
				}
				else {
					jQuery('.headquarters-viwer').val('+').css({'transform': 'rotate(46deg)', 'animation-delay': '5s'});				
				}
				jQuery('.regional-contact-info').slideToggle('slow');
				return false;
			});
  });
			
	jQuery(document).ready(function () {
		jQuery('.panels-flexible-region-3-address_').hide();
			jQuery('.locations-viwer').click(function (e) {
				e.preventDefault();
				if (jQuery('.panels-flexible-region-3-address_').is(':visible')) {
					jQuery('.locations-viwer').val('+').css({'transform': 'rotate(90000deg)', 'animation-delay': '5s'});
				}
				else {
					jQuery('.locations-viwer').val('+').css({'transform': 'rotate(46deg)', 'animation-delay': '5s'});				
				}
				jQuery('.panels-flexible-region-3-address_').slideToggle('slow');
				return false;
			});
  });		
  
	jQuery(document).ready(function () {  
	    jQuery('.story-filter-content1').hide();
       });	

	Drupal.behaviors.exampleModule = {
	  attach: function (context, settings) {
		//jQuery('.story-filter-content1').hide();
		jQuery('.story-filter', context).click(function () {            			  
				  if (jQuery('.story-filter-content1').is(':visible')) {
				 jQuery('.story-filter').val('+').css({'transform': 'rotate(90000deg)', 'animation-delay': '5s'});
				  }
				  else {
					jQuery('.story-filter').val('+').css({'transform': 'rotate(46deg)', 'animation-delay': '5s'});
				}
				jQuery('.story-filter-content1').slideToggle('slow');				           
				return false;
		});
	  }
	}; 

	
	
</script>
