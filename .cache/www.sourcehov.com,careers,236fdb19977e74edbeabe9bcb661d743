<script>
if(jQuery(window).width() <= 767){
jQuery("#search-block-form").hide();	
	jQuery(".tb-megamenu .btn-navbar").click(function(){
      jQuery("#search-block-form").toggle();
  });
}
</script>

<script>
if(jQuery(window).width() <= 979){
jQuery(".tb-megamenu .nav li.dropdown >.tb-megamenu-submenu").hide();	
	jQuery(".nav-collapse > ul > li > a").click(function(){
      
        jQuery(this).next().slideToggle(1000);
    });
}
jQuery('.tb-megamenu .nav li.dropdown > .dropdown-toggle .caret').addClass("icon-arrow-down arrow-icon1", function(){
       jQuery('.tb-megamenu .nav li.dropdown > .dropdown-toggle .icon-arrow-down').removeClass("caret");
    });

</script>

<script>	
	jQuery(document).ready(function () {
        jQuery('.lang-menu').hide();
		
        jQuery('.selected-country').click(function (e) {	
		 e.preventDefault();				
           jQuery('.lang-menu').toggle();
			return false;
		  });

		jQuery('body').click(function(e) {
        if (!jQuery(e.target).is('.lang-menu, .lang-menu *, .country_switcher')) {
            jQuery('.lang-menu').hide();				
        }
    });	
		  
    });
</script> 
