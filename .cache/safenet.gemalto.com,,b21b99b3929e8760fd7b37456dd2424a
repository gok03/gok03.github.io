           <input name="ucHeaderGemalto$hp_searchTxtTB" type="text" value="Search" id="ucHeaderGemalto_hp_searchTxtTB" class="text swaptext" onclick="javascript:if($(this).val() == &#39;Search&#39;) {$(this).val() = &#39;&#39;;} return false;" onchange="javascript:abletoSearch();" />
