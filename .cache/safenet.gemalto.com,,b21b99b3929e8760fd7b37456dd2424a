<script type="text/javascript" language="jscript">

    var objSearchTxt = document.getElementById("ucHeaderGemalto_hp_searchTxtTB");
    var canSubmit = false;

    function SubmitForSearch() {
        if (objSearchTxt != null && objSearchTxt.value != "") {
            window.location = "/search.aspx?q=" + objSearchTxt.value;
        }
    }

    function abletoSearch() {
        canSubmit = true;
    }

    objSearchTxt.onkeydown = function (e) {
        if (e.keyCode == 13) {
            e.preventDefault();
            // Remove global form checks
            try {
                console.log($("#global-form input").length);
                console.log($("#global-form select").length);
                $("#global-form input").each(function () {
                    $(this).removeAttr("onblur");
                });
                $("#global-form select").each(function () {
                    $(this).removeAttr("onblur");
                });
                SubmitForSearch();
            }
            catch (err) {
                console.log(err.message);
            }
        }
        else {
            // Do nothing
        }
    };

    function ChangeLanguage() {
        var selectedoption = document.getElementById("ucHeaderGemalto_hp_select_language");
        if(selectedoption == null) selectedoption = document.getElementById("ctl00_ucHeaderGemalto_hp_select_language");
        var selectedlanguage = selectedoption.options[selectedoption.selectedIndex].value;

        var redirectUrl = window.location.protocol + "//";
        redirectUrl += window.location.hostname.substring(0, window.location.hostname.lastIndexOf("."));

        switch (selectedlanguage) {
            case "english":
                redirectUrl += ".com";
                break;
            case "french":
                window.location = "http://www.safenet-inc.fr" + window.location.pathname;
                return;
            case "german":
                window.location = "http://www.safenet-inc.de" + window.location.pathname;
                return;
            case "italian":
                window.location = "http://www.safenet-inc.it" + window.location.pathname;
                return;
            case "japanese":
                window.location = "http://www.safenet-inc.jp" + window.location.pathname;
                return;
            case "korean":
                window.location = "http://www.safenet-inc.kr" + window.location.pathname;
                return;
            case "portuguese":
                window.location = "https://safenet.gemalto.com" + window.location.pathname;
                return;
            case "russian":
                window.location = "http://ru.safenet-inc.com" + window.location.pathname;
                return;
            case "chinese":
                window.location = "http://cn.safenet-inc.com" + window.location.pathname;
                return;
            case "spanish":
                window.location = "http://www.safenet-inc.es" + window.location.pathname;
                return;
        }

        redirectUrl += window.location.pathname;
        window.location = redirectUrl;
    }
</script>

                

