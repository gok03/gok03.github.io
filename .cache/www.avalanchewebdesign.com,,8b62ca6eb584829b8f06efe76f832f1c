	<script type="text/javascript">
		$(document).ready(function() {
			$("#protectEmailContainer,#cloudContainer,#managedHostingContainer").hover(
				function(){
					var bottom = $(this).find("div.show").height() + $(this).find("div.hide").height() + 105;
					$(this).find("div.slide").delay(200).stop(true).animate({
						scrollTop: bottom
					},1000)
				},
				function(){
					$(this).find("div.slide").delay(200).stop(true).animate({
						scrollTop: 0
					},1200)
				}
			);
			
			var domain = document.location.href;
			//remove 'http://' and everything after first '/'
			domain = domain.replace("http://","");
			if (domain.indexOf("/") > -1) {
				domain = domain.substring(0, domain.indexOf("/"));
			}
			$("#domainName").html(domain);
			
			$(".fancybox").fancybox({
				helpers : {
					media : {}
				}
			});
		});
	</script>
