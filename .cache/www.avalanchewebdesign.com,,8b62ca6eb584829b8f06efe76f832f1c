		<script>
		  (function() {
		    var c = document.createElement('script');
		    c.type = 'text/javascript'; c.async = true;
		    c.src = "https://chat.doteasy.com/ChatLink.ashx?config=3&id=delivechat";
		    var s = document.getElementsByTagName('script')[0];
		    s.parentNode.insertBefore(c,s);
		  })();
		</script>
		<script type="text/javascript">
			var liveChatOpen = function(e){
				e.preventDefault();
				var liveChatOffline = function(){
					 $.fancybox.open([
				        {
				            type: 'html',
				            content: '<h3 style="font-size:16px;font-weight:bold;margin-bottom:15px;line-height:1.5em;">Doteasy Live Chat Support</h3><p style="color:#222;font-size:14px;line-height:1.5em;">Our Live Chat is now either offline or busy. If you have any questions, please feel free to contact us by opening a new service ticket at <a href="https://www.doteasy.com/ContactUs/SMS3" target="_blank">https://www.doteasy.com/ContactUs/SMS3</a></p>'
				        }
				    ], {
				        maxWidth	: 400,
						maxHeight	: 150,
						fitToView	: false,
						autoSize	: false,
						closeClick	: false,
						openEffect	: 'none',
						closeEffect	: 'none'
				    });
				};
				$("a","#delivechat").each(function(){
					var $chatLink = $(this);
					if($chatLink.css("display") !== "none"){
						var liveChatID = $chatLink.attr("id");
						if(liveChatID==="delivechat_online"){
							$("a#delivechat_online","#delivechat").click();
						} else if (liveChatID==="delivechat_offline"){
							liveChatOffline();
						}
					}
				});
			};
		</script>
