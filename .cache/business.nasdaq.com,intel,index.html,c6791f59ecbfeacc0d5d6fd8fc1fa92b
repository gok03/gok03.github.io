            <div class="main-section-inner">
		        <div class="switch-block">
				    
                        <h2 class="eyebrow eyebrow__color">
						    Investor Relations Solutions
					    </h2>
                                            <span class="switch"></span>
				</div>

                <div class="main-section-tride">
                    <div class="h2-picture">
								<h3 class="block-title pointer">
                                    <a href="/intel/ir-management/ir-insight/index.html">Investor Relations Solutions</a>
                                </h3>
                                <span class="edit-design"><i>.</i></span>
                        <div class="main-section__image" data-bkstretch="/Images//INTEL_L2_Sub-Pillar_image-5-IR-MANAGEMENT_o_tcm5044-9564.jpg"></div>
