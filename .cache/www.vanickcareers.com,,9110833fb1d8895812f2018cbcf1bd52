    <script>

    $(function() {
        if(window.vimeo && !window.vimeo.options.vimeoPlayer){
            var iframe = $('#player1')[0];
            window.vimeo.options.vimeoPlayer = $f(iframe);
            var status = $('.status');
            window.vimeo.options = (window.vimeo && window.vimeo.options) || {start:-1, end:-1,vimeoPlayer:{}};
            var start = window.vimeo.options.start;
            var end= window.vimeo.options.end;
            var vimeoPlayer = window.vimeo.options.vimeoPlayer;
            // When the vimeoPlayer is ready, add listeners for pause, finish, and playProgress
            //vimeoPlayer.seekTo(20);
            vimeoPlayer.addEvent('ready', function () {
                status.text('ready');
                vimeoPlayer.api('seekTo', [start]);
                vimeoPlayer.addEvent('pause', onPause);
                vimeoPlayer.addEvent('finish', onFinish);
                vimeoPlayer.addEvent('playProgress', onPlayProgress);
            });

            // Call the API when a button is pressed
            $('button').bind('click', function () {
                vimeoPlayer.api($(this).text().toLowerCase());
            });

            function onPause() {
                status.text('paused');
            }

            function onFinish() {
                status.text('finished');
                vimeoPlayer.api('seekTo',[start]);
            }

            function onPlayProgress(data) {
                status.text(data.seconds + 's played');
                if(data.seconds > end){
                    vimeoPlayer.api('seekTo', [start]);
                }
            }

            $('footer').addClass('container-fluid');
        }
    });</script>
