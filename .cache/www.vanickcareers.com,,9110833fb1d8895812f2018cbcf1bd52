<script>
    var videoID = 'BY2x0hNLz8Y';
    var startSeconds = parseInt('0');
    var endSeconds = parseInt('15');
    var tag = document.createElement('script');

    tag.src = "https://www.youtube.com/iframe_api";
    var firstScriptTag = document.getElementsByTagName('script')[0];
    firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
    var player;
    function onYouTubeIframeAPIReady() {
        var mobileBoundPlayer = window.innerWidth < window.innerHeight ? window.innerWidth <= 768 : window.innerHeight <= 768;
        if (!mobileBoundPlayer) {
            player = new YT.Player('player', {
                playerVars: { loop: 1, autoplay: 1, controls: 0, showinfo: 0, autohide: 1, modestbranding: 0 },
                videoId: videoID,
                events: {
                    'onReady': onPlayerReady,
                    'onStateChange': onPlayerStateChange
                }
            });
        }
    }

    function onPlayerReady(event) {
        restartVideo();
    }

    function onPlayerStateChange(event) {
        if (event.data == YT.PlayerState.PLAYING) {
            var playerWait = document.getElementById('player-waiting');
            if (playerWait.classList) {
                playerWait.classList.add('raw-fade');
            } else {
                playerWait.className += " raw-fade";
            }
            if (endSeconds > 0 && endSeconds > startSeconds) {
                var desiredDuration = (endSeconds - startSeconds) * 1000;
                setTimeout(restartVideo, desiredDuration)
            }
        }

        if (event.data == YT.PlayerState.ENDED) {
            restartVideo();
        }
    }
    function restartVideo() {
        player.seekTo(startSeconds == 0 ? 0 : startSeconds);
        player.playVideo();
    }
</script>

