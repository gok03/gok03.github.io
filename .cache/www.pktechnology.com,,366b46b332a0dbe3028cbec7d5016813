            <script defer>

                console.log("Show - Background Videos - Version 1.0 - MuseThemes.com");

                $(function() {

                    "use strict";

                    // Initialize Video Params
                    var background_Type_u112066 = "full";
                    var section_Name_u112066 = ".section1";
                    var video_Source_u112066 = "youtube";

                    if (background_Type_u112066 == "full") {
                        var bgTarget = "body";
                    }
                    else {
                        var bgTarget = section_Name_u112066;
                    }
                    
                    // Set up sources
                    if (video_Source_u112066 == "local") {
                        var bgMpSource_u112066 = " ";
                        var bgWebmSource_u112066 = " ";
                        var bgPoster_u112066 = " ";
                    }
                    else if (video_Source_u112066 == "external") {
                        var bgMpSource_u112066 = "https://cdn.shopify.com/s/files/1/0099/9562/files/musethemes.mp4";
                        var bgWebmSource_u112066 = "https://cdn.shopify.com/s/files/1/0099/9562/files/musethemes.webm";
                        var bgPoster_u112066 = "https://cdn.shopify.com/s/files/1/0099/9562/files/poster.jpg";
                    }

                    // Initialize Custom Background Video
                    if (video_Source_u112066 != "youtube") {
                        $(bgTarget).vide('mp4: ' + bgMpSource_u112066 + ', webm: ' + bgWebmSource_u112066 + ', poster: ' + bgPoster_u112066 + '', {
                            posterType: "jpg",
                            muted: true,
                            loop: true
                        });
                    }

                    // Initialize YouTube Background Video
                    if (video_Source_u112066 == "youtube") {
                        var options = { videoId: '1uJwJNyrhPs', start: 3 };
                        $('body').tubular(options);
                    }

                });

            </script>

		

            <script defer>

                console.log("Show Video Player - Version 1.0 - MuseThemes.com");

                $(function() {
                    fluidvids.init({
                        selector: ['iframe', 'object', 'video'],
                        players: ['www.youtube.com', 'player.vimeo.com', 'www.facebook.com', 'www.flickr.com', 'www.youtube-nocookie.com', 'www.dailymotion.com', 'blip.tv', 'www.slideshare.net']
                    });
                });

            </script>

        
