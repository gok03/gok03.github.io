			google.load('search', '1', {language: 'en', style: google.loader.themes.MINIMALIST});
			google.setOnLoadCallback(function() {
			  var customSearchOptions = {};
			  var orderByOptions = {};
			  orderByOptions['keys'] = [{label: 'Relevance', key: ''} , {label: 'Date', key: 'date'}];
			  customSearchOptions['enableOrderBy'] = true;
			  customSearchOptions['orderByOptions'] = orderByOptions;
			  var customSearchControl =   new google.search.CustomSearchControl('003375206280626913574:y-otsjygxaa', customSearchOptions);
			  customSearchControl.setResultSetSize(google.search.Search.FILTERED_CSE_RESULTSET);
			  var options = new google.search.DrawOptions();
			  options.enableSearchboxOnly('/search', 'term');
			  options.setAutoComplete(true);
			  customSearchControl.draw('cse-search-form', options);

			  $('#hdr-srch-frm input.gsc-search-button').clone().attr({'type': 'image', 'src': '/common/img/hdr/hdr_2009_srch_btn.png'}).insertAfter('#hdr-srch-frm input.gsc-search-button').prev().remove();
			  $('#hdr-srch-frm').show();
			  $('#hdr-srch-frm #cse-search-form').show();

			}, true);
			</script>
