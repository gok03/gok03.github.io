<script type="text/javascript">


    function validationRecaptcha1550(){
         var b = true;
         jQuery.ajax({
            async:false,
            url: "/DesktopModules/DNNGo_PowerForms/ajaxValidate.aspx",
            cache: false,
            data: "ModuleID=1550&type=AjaxCustomCaptcha&fieldValue="+jQuery("#InputCaptcha1550").val(),
            success: function(Result){
                        if(Result != "true")
                        {
                            jQuery(".form_div_1550 #png_captcha1550").attr("src","/DesktopModules/DNNGo_PowerForms/Resource/images/no.png").show();
                        }else
                        {
                            //jQuery(".form_div_1550 #png_captcha1550").attr("src","/DesktopModules/DNNGo_PowerForms/Resource/images/yes.png").show();
                            jQuery(".form_div_1550 #png_captcha1550").hide();
                        }
                        if( Result != "true") { b = false; }  
                }
            });
       
            return b;
    }

     function validationFile1550(){
        var b = true;
         jQuery(".form_div_1550 input[type='file']").filter(".ajaxUpload").each(function(i){
            var fileid =  $(this).attr("id");
            jQuery.ajax({
            async:false,
            url: "/DesktopModules/DNNGo_PowerForms/ajaxValidate.aspx",
            cache: false,
            data: "ModuleID=1550&type=AjaxCustomUpload&fieldValue="+jQuery(this).val(),
            success: function(Result){
                        if(Result != "true")
                        {
                        jQuery(".form_div_1550 #png_upload_"+fileid).attr("src","/DesktopModules/DNNGo_PowerForms/Resource/images/no.png").show();
                        }else
                        {
                            //jQuery(".form_div_1550 #png_upload_"+ fileid).attr("src","/DesktopModules/DNNGo_PowerForms/Resource/images/yes.png").show();
                            jQuery(".form_div_1550 #png_upload_"+ fileid).hide();
                        }
                          if( Result != "true") {    b= false; } 
                }
            });
        });
        return b;
     }






    jQuery(function ($) {
        var sisForm = $(".form_div_1550").sisyphus({ customKeyPrefix: 'bg', timeout: 0, autoRelease: true });
        $("#Form").submit(function () {sisForm.manuallyReleaseData();});

        tinymce.init({
            selector: "textarea.tinymce",
            entity_encoding: "raw",
            convert_urls: false,
             menubar : false
        });

        
       
       $(".form_div_1550 input[lang='Submit']").click( function () { 
            
                if(!validationRecaptcha1550())
                {
                    return false;
                }
             

                if(!validationFile1550())
                {
                    return false;
                }
             });
             $("#ImageCheck1550").click( function () { $("#InputCaptcha1550").val('') });
    });
</script>
 
 
