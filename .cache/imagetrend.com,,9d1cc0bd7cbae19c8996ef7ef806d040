			}			
		}
	</script>
		
	<!--Form Pop-up-->
	<script type="text/javascript">
		var appear = false;
		var storage = null;

		function Show_Div(demoForm) {
			Close_Div();
			storage = demoForm;
			
			if ((false == $(demoForm).is(':visible'))) {
				$(demoForm).show(250);
				appear = true;
			} else {
				$(demoForm).hide(250);
			}
		}

		function hideForm(form) {
			$(form).parents('form').parent('div').hide(250);
		}

		function AppearIsTrue(){
			appear = true;
		}

		function Close_Div() {
			if(appear == false) {
				$(storage).hide(250);
			}
			appear = false;
		}
	</script>


