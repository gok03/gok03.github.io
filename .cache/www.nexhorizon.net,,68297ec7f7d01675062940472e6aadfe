$(document).ready(function () {
	
	$("a.fancybox").fancybox();
	
	$("#nh-topNav ul li a span.nh-topNavRO").css("opacity","0");
	$("#nh-contactFormSmall #divToUpdate").css("opacity","0");
	$("#nh-contactForm #divToUpdate").css("opacity","0");
	
	// megamenu hover
	function addMega(){
		$(this).addClass("hovering");
		$(this).find("span.nh-topNavRO").stop().animate({
			opacity: 1
		}, 250);
	};
	function removeMega(){
		$(this).removeClass("hovering");
		$(this).find("span.nh-topNavRO").stop().animate({
			opacity: 0
		}, 250);
	};
	
	// megamenu settings
	var megaConfig = {
	 interval: 0,
	 sensitivity: 4,
	 over: addMega,
	 timeout: 0,
	 out: removeMega
	 };
	 
	 // megamenu call
	 $("#menu > li").hoverIntent(megaConfig);
	 
	 // spotlight slider
	 $('#nh-spotlightSlider').flexslider({
		slideshow: false,
		animation: "fade",
		slideshowSpeed: 7000,
		animationDuration: 1200,
		randomize: false,
		startAt: 2,
		controlNav: false,
		pauseOnHover: true
	  });
	 
	 //procorner blog feed
	 $.ajax({
		type: "GET",
		url: "http://www.nexhorizon.net/procorner/index.php/category/featured/feed/",
		dataType: "xml",
		success: function(xml) {
			$(xml).find('item').each(function(i){
				if (i < 3) {
					var id = $(this).attr('dc:creator');
					var title = $(this).find('title').text();
					var url = $(this).find('link').text();
					var brief = $(this).find('description').text();
					$('<div class="latestBlog"></div>').html('<h3><a href="'+url+'" title="'+title+'">'+title+'</a></h3><p><span class="blogSummary"><span style="color:#333;">'+brief+'</span>...</span></p>').prependTo('#nh-proCorner');
					//$fC = $(".blogSummary span");
					//$fC.html($fC.html().substring(0,120));
				}
			});
		}
	});
	 
	
	// homepage testimonials slider
	var slider = $('#nh-testimonialSlider').bxSlider({
		controls: false
	});
	
		$('.nh-smlPrev').click(function(){
			slider.goToPreviousSlide();
			return false;
		});
		
		$('.nh-smlNext').click(function(){
			slider.goToNextSlide();
			return false;
		});
	
	// testimonials slider
	var slider2 = $('#nh-testimonialsSliderSmall').bxSlider({
		controls: false
	});
	
		$('.nh-smlPrev2').click(function(){
			slider2.goToPreviousSlide();
			return false;
		});
		
		$('.nh-smlNext2').click(function(){
			slider2.goToNextSlide();
			return false;
		});
	
	// client slider
	var slider3 = $('#nh-clientSlider').bxSlider({
		controls: false
	});
	
		$('.nh-smlPrev3').click(function(){
			slider3.goToPreviousSlide();
			return false;
		});
		
		$('.nh-smlNext3').click(function(){
			slider3.goToNextSlide();
			return false;
		});
		
	// company slider
	var slider4 = $('#nh-teamSlider').bxSlider({
		controls: false
	});
	
		$('.nh-teamMemberPrev').click(function(){
			slider4.goToPreviousSlide();
			return false;
		});
		
		$('.nh-teamMemberNext').click(function(){
			slider4.goToNextSlide();
			return false;
		});
	
	// animated rollover for service icons
	$('ul.nh-servicesNav li').hover(function(){
		$(this).find('img').animate({top:'-20px'},{queue:false,duration:100});
		$(this).find('img').css("z-index","999");
	}, function(){
		$(this).find('img').animate({top:'0px'},{queue:false,duration:300});
		$(this).find('img').css("z-index","1");
	});
	
	$('ul.hover_block li').hover(function(){
		$(this).find('img').animate({top:'-20px'},{queue:false,duration:100});
		$(this).find('img').css("z-index","999");
	}, function(){
		$(this).find('img').animate({top:'0px'},{queue:false,duration:300});
		$(this).find('img').css("z-index","1");
	});
	
	// animated rollover for top timeline info
	$('.nh-top .nh-frame').hover(function(){
		$(this).find('.nh-info').animate({top:'0px'},{queue:false,duration:300});
	}, function(){
		$(this).find('.nh-info').animate({top:'130px'},{queue:false,duration:300});
	});
	
	// animated rollover for bottom timeline info
	$('.nh-btm .nh-frame').hover(function(){
		$(this).find('.nh-info').animate({bottom:'0px'},{queue:false,duration:300});
	}, function(){
		$(this).find('.nh-info').animate({bottom:'130px'},{queue:false,duration:300});
	});
	
	var ul = $(this).find(".tweet_list");
	var ticker = function() {
	setTimeout(function() {
	var top = ul.position().top;
	var h = ul.height();
	var incr = (h / ul.children().length);
	var newTop = top - incr;
	if (h + newTop <= 0) newTop = 0;
	ul.animate( {top: newTop}, 0 );
	ticker();
	}, 5000);
	};
	ticker();
	$('.hjtweet a').attr('target','_blank');
	
	// Small Contact Form on Service Pages 
    var options = {
        url: 'doAction/doAction.asp',
		type: 'post',
        success: function () {
            //$('#divToUpdate').text('Thanks! Your message has been sent');
			$("#nh-contactFormSmall #nh-contactFormSmallFields").css("display","none");
			$('#nh-contactFormSmall').find("#divToUpdate").stop().animate({
			opacity: 1
			}, 2000);
			$('#nh-contactFormSmall').find('#divToUpdate').animate({top:'0px'},{queue:false,duration:1000});
        }
    };
    $("#nh-contactFormSmall").validate({
        rules: {
            strName: {
                required: true,
                minlength: 2
            },
            strEmail: {
                required: true,
                email: true
            },
			strInterest: {
                required: true
            }
        },
        messages: {
            strName: {
                required: "Please enter a name",
                minlength: "Your name must consist of at least 2 characters"
            },
            strEmail: "Please enter a valid email address",
			strInterest: "Select an Option"
        }
    });
	
    $('#nh-contactFormSmall').ajaxForm(options);
	
	// Contact Page Form
    var options2 = {
        url: 'doAction/doActionFull.asp',
		type: 'post',
        success: function () {
            //$('#divToUpdate').text('Thanks! Your message has been sent');
			$("#nh-contactForm #nh-contactFormFields").css("display","none");
			$('#nh-contactForm').find("#divToUpdate").stop().animate({
			opacity: 1
			}, 2000);
			$('#nh-contactForm').find('#divToUpdate').animate({top:'290px'},{queue:false,duration:1000});
        }
    };
    $("#nh-contactForm").validate({
        rules: {
            strName: {
                required: true,
                minlength: 2
            },
            strEmail: {
                required: true,
                email: true
            },
			strPhone: {
                required: true
            },
			strProject: {
                required: true,
                minlength: 10
            }
        },
        messages: {
            strName: {
                required: "Please enter a name",
                minlength: "Your name must consist of at least 2 characters"
            },
            strEmail: "Please enter a valid email address",
			strPhone: "Please enter a phone number",
			strProject: {
                required: "Please enter some details",
                minlength: "You must enter at least 10 characters"
            }
        }
    });

    $('#nh-contactForm').ajaxForm(options2);
	
	
	// facebook character limit
	$('span.feed-message').each(function() {
		var $this = $(this);
		$this.text( $this.text().slice(0,65) );
	});
	
	$('a.feed-link').attr('target', '_blank');
	
	// show tweet count
	var username = 'NextHorizonWeb';
	$.ajax({
		url: 'https://api.twitter.com/1/users/show.json?screen_name=' + username + '&include_entities=false',
		dataType: 'jsonp',
		success: function(data){
			$('#statuses_count').html(data.statuses_count);
		}                
	});
	
	$('.scroll-pane').jScrollPane({showArrows: true});
	
	//faq toggle
	$(".nh-answer").hide();
	$(".nh-question").click(function(){
		$(this).next(".nh-answer").slideToggle(200);
	});
	
	if ($.fn.cssOriginal!=undefined)
		$.fn.css = $.fn.cssOriginal;

		$('#products').portfolio({	
		 
			<!-- GRID SETTINGS -->
			gridOffset:90,				<!-- Manual Right Padding Offset for 100% Width -->
			cellWidth:176,						<!-- The Width of one CELL in PX-->						
			cellHeight:176,						<!-- The Height of one CELL in PX-->
			cellPadding:10,						<!-- Spaces Between the CELLS -->
			entryProPage:16,					<!-- The Max. Amount of the Entries per Page, Rest made by Pagination -->
			captionOpacity:100,					<!-- Opacity of Caption -->
			captionPosition:"bottom",
			captionYOffset:-20,
			
			<!-- FILTERING -->
			filterList:"#portfolio-filter",		<!-- Which Filter is used for the Filtering / Pagination -->
			title:"#selected-filter-title",		<!-- Which Div should be used for showing the Selected Title of the Filter -->												
			
			<!-- Page x from All Pages -->
			pageOfFormat:"Page #n of #m",		<!-- The #n will be replaced with the actual Item Nr., #m will be replaced with the amount of all items in the filtered Gallery-->						
			
			<!-- Social Settings-->
			showGoogle:"yes",					<!-- Show The Social Buttons ...-->
			showFB:"yes",
			showTwitter:"yes",
				
			showEmail:"yes",																<!-- ADD EMAIL TO LINK ALSO TO THE LIGHTBOX  -->
			emailLinkText:"Email to Friend",
			emailBody:"mailto:email@echoecho.com?body=I found some great File here #url",	<!-- The #url will be replaced with the url of the image -->
			emailUrlCustomPrefix:"http://www.themepunch.com/",								<!-- Use this if you wish a Custom Prefix to Link Path -->						
			emailUrlCustomSuffix:"?ref=...",												<!-- Use This if you wish to use a Custtom Suffix for Link Path -->
			
			urlDivider:"?",						<!-- What is the Divider in the Url to add the Variables, Filter and Image ID . Impotant for WordPress i.e. Social will share this link with this divider -->
			
			<!-- BACKGROUND -->
			backgroundHolder:"#main-background",
			backgroundSlideshow:0
			
		})
	 
}); 


