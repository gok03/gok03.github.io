<script type="text/javascript">
function formvalidation(thisform){
	with (thisform){
		if (emptyvalidation(name,"Please enter your name.")==false) {return false;};
		if (emptyvalidation(company,"Please enter your company name.")==false) {return false;};
		if (emailvalidation(email,"Please enter a valid email address.")==false) {return false;};
	}
}
</script>
<script type="text/javascript">

jQuery(function($){
