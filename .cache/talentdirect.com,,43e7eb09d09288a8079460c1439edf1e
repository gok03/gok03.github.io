    <script type="text/javascript">

        function rescale() {
            var IMGcont = $('#thumbO')

                $('#slideshows').find('img').load(function () {
                    var IMG = $(this)
                    var IMGR = IMG.width() / IMG.height()
                    var IMGcontR = 1

                    if (IMGR >= IMGcontR) { // Landscape img
                        IMG.addClass('maxHeight').removeClass('maxWidth');
                        IMG.css('margin-left', '-' + (IMG.width() - 370) / 2 + 'px')

                    }
                    else { // Portrait img
                        IMG.addClass('maxWidth').removeClass('maxHeight');
                    }
                });
                
                $('#slideshows').find('img').each(function () {
                    var IMG = $(this)
                    var IMGR = IMG.width() / IMG.height()
                    var IMGcontR = 1

                    if (IMGR >= IMGcontR) { // Landscape img
                        IMG.addClass('maxHeight').removeClass('maxWidth');
                        IMG.css('margin-left', '-' + (IMG.width() - 370) / 2 + 'px')

                    }
                    else { // Portrait img
                        IMG.addClass('maxWidth').removeClass('maxHeight');
                    }
                });
        }

        $(document).ready(function () {
            $('.dk').dropkick({
                theme: 'black',
                change: function (value, label) {
                    $(this).dropkick('theme', value);
                }
            });
            rescale();
        });

        $(window).load(function () {
            rescale();
            setTimeout(function () { 
                $('#thumbO').xfade({ height: 370 });
                $('#thumbE').xfade({ height: 370 });
            },500);
        });
</script>


