<script language="javascript" type="text/javascript">
    $(document).ready(function () {
        $('#slider').cycle({
            fx: 'scrollHorz',
            speed: 'fast',
            timeout: 0,
            next: '#slideRight',
            prev: '#slideLeft',
            after: function () {
                $("#jquery_cycle li").removeClass("active");
                var spcl = $("#slider img:visible").data("special");
                $("#jquery_cycle li[data-special=" + spcl + "]").addClass("active");
            }
        });

        $('#jquery_cycle #tools li a').click(function () {
            $("#jquery_cycle li").removeClass("active");
            var li = $(this).closest('li');
            li.addClass("active");
            var index = parseInt(li.data('special')) - 1;
            $('#slider').cycle(index);
        });
    });
	</script>

