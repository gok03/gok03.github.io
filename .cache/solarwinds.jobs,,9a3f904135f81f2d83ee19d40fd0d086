<script>
function findValue(e,p){
	//returns array of instances it finds
	var c=$(e).text();		
	var r=new RegExp(p, 'g');	
	return c.match(r);
}
function parseValueSubstring(s,b,a){
	//pass number of characters before and after to strip
        var cs='https://czcareers-solarwinds.icims.com/jobs/';
        var p= $('#direct_jobListingTitle meta[itemprop="addressCountry"][content="United States"]');
         if(p.length>0){
               cs='https://uscareers-solarwinds.icims.com/jobs/';
         }

	return cs+s.substring(b,s.length-a);
}
function parseValueArray(s,d,i){
	//pass delimeter and array element you want to return
	return s.split(d)[i];
}
function writeRefer(){
        try{
               var ji=parseValueSubstring(findValue('#direct_jobDescriptionText','\\d{4}-\\d{4}')[0],5,0);
               $('#direct_disambiguationDiv').before('<a onclick="goalClick(\'/G/talent-network-click\', this.href); return false;" id="sidebar-connect" class="blue-btn" href="' + ji + '/referral">Email this job to a friend</a>');
         }
         catch(e){
         }
}
	$('#direct_moreLessLinks_listingDiv').after('<div class="connect-bar clearfix"><p>Not Ready to Apply?<br>Stay in touch with Us</p><a onclick="goalClick(\'/G/talent-network-click\', this.href); return false;" class="orange-btn" href="https://careers-hub-solarwinds.icims.com/connect">Connect to SolarWinds</a><img src="https://seo-nlx-org.s3.amazonaws.com/solarwinds/img/mascot-thought-buble.png"></div></div>');
	$('#addThis').before('<div id="character-megaphone"><img src="https://seo-nlx-org.s3.amazonaws.com/solarwinds/img/mascot-megaphone.png"></div>');
	$('#direct_disambiguationDiv').before('<a onclick="goalClick(\'/G/talent-network-click\', this.href); return false;" id="sidebar-connect" class="blue-btn" href="https://careers-hub-solarwinds.icims.com/connect">Not ready to apply?<br>Connect with Us</a>');

        writeRefer();

	$('#direct_applyButtonBottom').after('<div class="connect-bar clearfix"><p>Not Ready to Apply?<br>Stay in touch with Us</p><a onclick="goalClick(\'/G/talent-network-click\', this.href); return false;" class="orange-btn" href="https://careers-hub-solarwinds.icims.com/connect">Connect to SolarWinds</a><img src="https://seo-nlx-org.s3.amazonaws.com/solarwinds/img/mascot-thought-buble.png"></div></div>');


