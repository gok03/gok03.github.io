<script language="javascript" type="text/javascript">

    function TextBoxSearch_Focus(control)
    {
        try
        {
            //if text equals SearchLabel then blank out
            var searchLabel = "";
        
            if (control.value == searchLabel)
            {
                control.value = "";
            }
        }
        catch (ex)
        {
        }
    }
    
    function TextBoxSearch_Blur(control)
    {
        try
        {
            //if text is blank then populate with SearchLabel
            var searchLabel = "";
        
            if (control.value.trim() == "")
            {
                control.value = searchLabel;
            }
        }
        catch (ex)
        {
        }
    }

</script>

