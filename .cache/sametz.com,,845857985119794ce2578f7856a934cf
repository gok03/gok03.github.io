<script>
	
	$(document).ready(function(){
		
		//Labelify	
			$("input#search,input#search-core,input#mobile-search" ).labelify({text:"label"});
		  
		//Twitter
			//$.getScript("http://twitter.com/javascripts/twitter_feed.js");
			//$.getScript("http://api.twitter.com/1/statuses/user_timeline.json?screen_name=Sametz&count=3&callback=twitterCallback2");
			//var twitter = $.getScript("http://api.twitter.com/1.1/statuses/user_timeline.json?screen_name=Sametz&count=3");
			//alert(twitter);
			
		 //Home Introduction Tabs
		 	introTabs(); 
		 
		 //Home Background Images
		 	$('#tabs #discipline-nav ul li a').click(homeBackgrounds);
		 
		//BEGIN Client Page Carousel		
			//Establish Current Image
				$('#items').css('margin-left', '0px');	
					
				if($("#items div:first-child").children('img').length == 1){
					$("#items div:first-child").addClass('current-image').css('width',$('.current-image').find('img').attr('width'));					
				}else if($("#items div:first-child").children('iframe').length == 1){
					$("#items div:first-child").addClass('current-image').css('width',$('.current-image').find('iframe').attr('width'));				
				}
			
			
			//Add IDs to all Projects
				var count = 0;
				$("#items > div").each(function(){
					count++;
					$(this).attr('id','item-' + count);
					});
					
			//---- Video Functionality
			$('#items iframe').each( function () {
				
				var parentHeight = $(this).parent().css('height');
			
				$(this).before('<div style="height:' + parentHeight + '; width: 100%; position: absolute; top: 0; left: 0;" />');
			
			});
				
				
			//---- Full Gallery Navigation
				//Next Button	
				$('#next').click(nextButton);
				
					
				//Previous Button	
				$('#prev').click(prevButton);
					
				//Project Navigation	
				$('#items > div').click(projectNav);	
					
				//Mobile Next Button
				$('#next-mob').click(mobNextButton);
					
				//Mobile Gallery Navigation
				$('#prev-mob').click(mobPrevButton);		
		// END Client Page Carousel
		
		// BEGIN Content Display Effects		
			//For Work Page
			$('.content-pop-open').click(workMoreButton);		
		
			// For Single Client Page and Our Team Page
			$('.content-animate').click(contentToggle);		
		// END Content Display Effects 
			
		// Social Nav Tabs
		socialNavTabs();
		
		// Social Nav Instafeed
		socialInstafeed();
		
		//Social Nav Position
		socialNavPosition();
  
	});
</script>

