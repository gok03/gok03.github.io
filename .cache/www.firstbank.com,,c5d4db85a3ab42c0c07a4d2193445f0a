    <script>

      var checkscroll = true, 
          count = 1,
          c_height = $('.circles').height();
          
      $(window).scroll(function() {

        var scrollTop = document.documentElement.scrollTop || document.body.scrollTop, // Scroll Position
            circleTop = $('.circles').offset().top, // Distance from top
            d_height=$(document).height(), // Height of the webpage
            v_height=$(window).height(), // Height of the viewport
            dif = circleTop - scrollTop; // Difference of the scroll and the cicles top 


        if(dif - (v_height - c_height) <= 0 && checkscroll) {
          var myVar = setInterval(function(){ showCircle() }, 250);

          checkscroll = false;
        }

        function showCircle() {
          $('.circle-icon' + count).fadeIn();
          $('#myStat' + count).circliful();
          

          if(count == 4) {
            clearInterval(myVar);
          }

          count++;
        }
      });

    
    </script><!-- Google Tag Manager -->

    
