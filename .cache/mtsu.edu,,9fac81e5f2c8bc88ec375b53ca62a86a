      		<script type="text/javascript">
		$(function(){
			resizeHomeCols();
			$(window).resize(function(){resizeHomeCols();});
		});
		function resizeHomeCols() {
			var boxh = 0;
			$('.home-col-content').attr('style','');
			$('.home-col-content .color_button').parent('p').attr('style','');
			$('.home-col-content').each(function(){
				if ($(this).height() > boxh){
					boxh = $(this).height();
				}
			});
			$('.home-col-content').css('height',boxh+'px');
			$('.home-col-content .color_button').parent('p').css({position:'absolute',bottom:0,width:'90%','text-align':'center','margin-bottom':0});
		}
		</script>
      		
