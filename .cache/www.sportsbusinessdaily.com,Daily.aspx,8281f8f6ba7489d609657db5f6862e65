<script type="text/javascript">
    var aryImages = [];
    var currentIndex = 0;
    var maxIndex = -1;
    var beautyTimer;
    var pause = false;
    //var delayTime = 10000;
    var delayTime = 10000;

    var oldonload = window.onload; 

    window.onload = function () {

        if (isJournal) {
            $("#beautyNavLeftLink").mouseenter(function () {
                $("#beautyNavLeft_hover").fadeIn("fast");
            });
            $("#beautyNavLeftLink").mouseleave(function () {
                $("#beautyNavLeft_hover").fadeOut("fast");
            });
            $("#beautyNavRightLink").mouseenter(function () {
                $("#beautyNavRight_hover").fadeIn("fast");
            });
            $("#beautyNavRightLink").mouseleave(function () {
                $("#beautyNavRight_hover").fadeOut("fast");
            });
        }

        $.get("/Daily/Features/Rotating-Flash-Player.aspx?seed=636097549108089502&eventPage=False", function (data) {
            $(data).find("item").each(function (index, element) {
                var imgSrc = $(this).attr("image");
                var thumbSrc = $(this).attr("thumb");
                var shortTitle = $(this).children("shortTitle").text();
                var embedCode = $(this).children("embedCode").text();
                var href = embedCode != "" ? "javascript:playVideo();" : $(this).children("link").text();
                var pageLink = $(this).children("link").text();

                //var html = embedCode != "" ? embedCode : "<a href=\"" + href + "\">"
                //    + "<img src=\"" + imgSrc + "\">"
                //    + "</a>";

                var html = "";
                var overlay = embedCode != "" ? "<img src=\"/Media/buttons/play.png\" class=\"videoOverlay\" />" : "";

                html = "<a href=\"" + href + "\" class=\"ctbi\" ct=\"Daily: Sub Homepage: Beauty - Link - Image\">"
                    + overlay
                    + "<img src=\"" + imgSrc + "\">"
                    + "</a>";

                var image = $("<img />").attr("src", imgSrc);

                aryImages.push({
                    "html": html
                    , "href": href
                    , "pageLink": pageLink
                    , "title": $(this).children("title").text()
                    , "subTitle": $(this).children("subTitle").text()
                    , "embedCode": embedCode
                });

                if (isJournal) {
                    $("#journalNav").append("<a href=\"javascript:showImage(" + index + ");\" class=\"ctb\" ct=\"Daily: Sub Homepage: Beauty - Navigation - Dot - " + (index + 1) + "\"><div class=\"beautyJournalButton\"></div></a>");
                } else {
                    $("#beautyThumbImages").append("<div class=\"beautyThumb\"><a href=\"javascript:showImage(" + index + ");\" class=\"ctb\" ct=\"Daily: Sub Homepage: Beauty - Navigation - Thumb - " + (index + 1) + "\">"
                    + "<img src=\"" + thumbSrc + "\" /><br />"
                    + shortTitle
                    + "</a></div>");
                }

                maxIndex++;
            });

            initClickTracker();

            showImage(0);
            if (!isJournal) {
                if ((navigator.userAgent.match(/iPhone/i)) ||
                (navigator.userAgent.match(/iPod/i)) ||
                (navigator.userAgent.match(/iPad/i))) {
                    $(".beautyThumb").bind("touchstart", function () {
                        $(this).addClass("beautyThumbHover");
                    });
                    $(".beautyThumb").bind("touchend", function () {
                        $(this).removeClass("beautyThumbHover");
                    });
                } else {
                    $(".beautyThumb").bind("mouseenter", function () {
                        $(this).addClass("beautyThumbHover");
                    });
                    $(".beautyThumb").bind("mouseleave", function () {
                        $(this).removeClass("beautyThumbHover");
                    });
                }
            } else {
                if ((navigator.userAgent.match(/iPhone/i)) ||
                (navigator.userAgent.match(/iPod/i)) ||
                (navigator.userAgent.match(/iPad/i))) {
                    $(".beautyJournalButton").bind("touchstart", function () {
                        $(this).addClass("beautyJournalButtonHover");
                    });
                    $(".beautyJournalButton").bind("touchend", function () {
                        $(this).removeClass("beautyJournalButtonHover");
                    });
                } else {
                    $(".beautyJournalButton").bind("mouseenter", function () {
                        $(this).addClass("beautyJournalButtonHover");
                    });
                    $(".beautyJournalButton").bind("mouseleave", function () {
                        $(this).removeClass("beautyJournalButtonHover");
                    });
                }
            }
        });

        if (oldonload) { oldonload(); }
    }

    function showImage(imgIndex) {
        if (beautyTimer != undefined)
            clearTimeout(beautyTimer);

        if (!pause) {
            if (imgIndex > maxIndex) imgIndex = 0;
            if (imgIndex < 0) imgIndex = maxIndex;
            currentIndex = imgIndex;
            $("#beautyCaptionArea").fadeOut(function () {
                $("#mainImageWrapper").fadeOut(function () {
                    var alink = "<a href=\"" + aryImages[imgIndex].pageLink + "\" class=\"ctbi\" ct=\"Daily: Sub Homepage: Beauty - Link - Text\">";
                    //alert(alink);
                    var title = "<h3>" + aryImages[imgIndex].title + "</h3>";
                    var subTitle = aryImages[imgIndex].subTitle;
                    $("#beautyCaptionArea").html(alink + title + subTitle + "</a>");
                    $("#mainImageWrapper").html(aryImages[imgIndex].html).fadeIn(function () {
                        $("#beautyCaptionArea").fadeIn(function () {
                            beautyTimer = setTimeout("showImage(" + (imgIndex + 1) + ")", delayTime);
                        });

                        initClickTrackerImage();
                    });
                });
            });

            if (isJournal) {
                $(".beautyJournalButtonSelected").removeClass("beautyJournalButtonSelected");
                $(".beautyJournalButton").eq(imgIndex).addClass("beautyJournalButtonSelected");
            } else {
                $(".beautyDailySelected").removeClass("beautyDailySelected");
                $(".beautyThumb").eq(imgIndex).addClass("beautyDailySelected");
            }
        }
    }

    function playVideo() {
        clearTimeout(beautyTimer);
        var embed = aryImages[currentIndex].embedCode;
        $("#mainImageWrapper").html(embed);
        $(".video-js").bind("ended", function () {
            showImage(currentIndex);
        });
        $(".video-js").get(0).play();
    }
</script>

   


        </div>
        
