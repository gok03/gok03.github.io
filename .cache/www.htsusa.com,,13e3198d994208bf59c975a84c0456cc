	huge_video_playing['video_is_playing_'+3]=false;
	data_3["0"]=[];data_3["0"]["id"]="0";data_3["0"]["image_url"]="http://htsusa.com/wp-content/uploads/2015/12/Shoretel.png";data_3["0"]["description"]="";data_3["0"]["alt"]="";data_3["1"]=[];data_3["1"]["id"]="1";data_3["1"]["image_url"]="http://htsusa.com/wp-content/uploads/2015/12/Allworx.png";data_3["1"]["description"]="";data_3["1"]["alt"]="";data_3["2"]=[];data_3["2"]["id"]="2";data_3["2"]["image_url"]="http://htsusa.com/wp-content/uploads/2015/12/toshiba.jpg";data_3["2"]["description"]="";data_3["2"]["alt"]="";	
	

      var huge_it_trans_in_progress_3 = false;
      var huge_it_transition_duration_3 = 1000;
      var huge_interval ={};
      var id_array_index=sliderID_array.length

	  
      
	  var ifhasthumb ="nonav";
	  sliderID_array[id_array_index] =       // Stop autoplay.
