	  			<script type="text/javascript">

    // creates and appends script tag with given source
    function loadScript(source){
		   var script = document.createElement('script');
		   script.type = "text/javascript";
		   script.src = source;
		   document.getElementsByTagName('body')[0].appendChild(script);	
		   
		   return script;
    }
  
    //Checks IE version for compability support
    function msieversion(script) {
        var ua = window.navigator.userAgent;
        var msie = ua.indexOf("MSIE ");

        if (msie > 0){ // If Internet Explorer
        	script.onreadystatechange = function() {
	  		  if ( this.readyState == "loaded" )	{
					$('#smt_orbitFeatured').orbit(
							{
		animation: 'fade', 		
		animationSpeed: 2000,
		timer: true,
		advanceSpeed: 4000,
		captionAnimationSpeed: 2000
	});		  
					} 
	  		  }	
        }else{ // Other browser
			script.onload = function(){
				$('#smt_orbitFeatured').orbit(
						{
		animation: 'fade', 		
		animationSpeed: 2000,
		timer: true,
		advanceSpeed: 4000,
		captionAnimationSpeed: 2000
	});	
			}
        } 
   }
    //Determines if jquery is already defined, if not loads jquery
	addLoadEvent(function(){if (typeof jQuery == "undefined") {
		loadScript("//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js");
	}});
    
	// loads and initializes orbit script
	addLoadEvent(function(){
		msieversion(loadScript("/binary/common/scripts/jquery-orbit/jquery.orbit-1.2.3.js"));});
</script>
