<!--

function newImage(arg) {
	if (document.images) {
		rslt = new Image();
		rslt.src = arg;
		return rslt;
	}
}

function changeImages() {
	if (document.images && (preloadFlag == true)) {
		for (var i=0; i<changeImages.arguments.length; i+=2) {
			document[changeImages.arguments[i]].src = changeImages.arguments[i+1];
		}
	}
}

var preloadFlag = false;
function preloadImages() {
	if (document.images) {
		home_over = newImage("images/home-over.gif");
		pst_solutions_over = newImage("images/pst_solutions-over.gif");
		software_technologies_over = newImage("images/software_technologies-over.gif");
		clients_over = newImage("images/clients-over.gif");
		about_us_over = newImage("images/about_us-over.gif");
		support_over = newImage("images/support-over.gif");
		preloadFlag = true;
	}
}

