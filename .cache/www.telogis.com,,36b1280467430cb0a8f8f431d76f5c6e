  var _gaq = _gaq || [];

  (function() {
    var hostname = window.location.hostname;
    var domainFragment = hostname.substr(hostname.indexOf('.telogis')); // Should be, e.g., '.telogis.com' on the US site

    _gaq.push(['_setAccount', 'UA-3470755-1']);
    _gaq.push(['_setDomainName', domainFragment]);
    _gaq.push(['_setAllowLinker', true]);

    var trackPageview = function() {
      _gaq.push(['_trackPageview']);
    }

    if(typeof deferredAnalytics !== "undefined") {
      deferredAnalytics.promise().always(trackPageview);

      // Resolve the deferred object once GA initializes
      _gaq.push(function() {
        deferredAnalytics.resolve();
      });
    } else {
      trackPageview();
    }

    // Load the alternative ga.js script -- identical, but includes extra demographic information
    var ga = document.createElement('script');
    ga.type = 'text/javascript';
    ga.async = true;

    ga.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'stats.g.doubleclick.net/dc.js';

