    jQuery().ready(function ($) {

        function HoverOver93b4940d58() {

            $(this).addClass('rmhover').find(".subMenu:first").stop().animate({ height: 'show' }, 450);
        }

        function HoverOut93b4940d58() {
            $(this).removeClass('rmhover').find(".subMenu:first").animate({ height: 'hide' }, 450, function () {
                $(this).hide();
            });
        }

        function HoverOversub93b4940d58() {
            $(this).find(".subMenu:first").stop().animate({ height: 'show' }, 450);
        }

        function HoverOutsub93b4940d58() {
            $(this).find(".subMenu:first").stop().animate({ height: 'hide' }, 450, function () {
                $(this).hide();
            });
        }

        var config93b4940d58 = {
            sensitivity: 1,
            interval: 100,
            over: HoverOver93b4940d58,
            timeout: 1000,
            out: HoverOut93b4940d58
        };
        var configsub93b4940d58 = {
            sensitivity: 1,
            interval: 100,
            over: HoverOversub93b4940d58,
            timeout: 1000,
            out: HoverOutsub93b4940d58
        };
        $("#gomenu93b4940d58 li.dir").hoverIntent(config93b4940d58);
        $("#gomenu93b4940d58 .subMenu li.dir").hoverIntent(configsub93b4940d58);
        $("#gomenu93b4940d58").find("li.current").each(function (i) {
            $(this).find(".subMenu:first").show();
        });
    });
