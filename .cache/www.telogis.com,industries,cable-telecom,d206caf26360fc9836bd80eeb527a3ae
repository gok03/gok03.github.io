<script type="text/javascript">
  (function (numberSelector) {
    if ($(numberSelector).length >= 1) {

      // Finds 'mcid' in the cookie if not in the query string
      var findMcid = function() {
        var invoca_session = InvocaSolutions.PNAPI.readCookie("invoca_session");
        var invoca_mcid = null;
        if (invoca_session) {
          var json = JSON.parse(decodeURIComponent(invoca_session));
          invoca_mcid = json.mcid || null;
        }
        return invoca_mcid;
      }

      // This is where we capture the referrer and current page
      // It will attribute the media channel by "utm_source" or "orgd" if no source is found
      var invocaParams = {
        referrer    : document.referrer || "Direct",
        currentPage : window.location.href,
        utm_source  : InvocaSolutions.PNAPI.readUrl("utm_source") || "orgd",
        mcid        : InvocaSolutions.PNAPI.readUrl("utm_source") || findMcid() || "orgd"
      };

      InvocaSolutions.PNAPI.integration({
        networkId               : 938,
        numberSelector          : numberSelector,
        numberFormat            : 1,
        campaignIdOverrideParam : "cid",
        poolParams              : invocaParams,
        params                  : ["utm_source", "utm_medium", "utm_term", "utm_campaign", "network", "device", "gclid", "referrer", "currentPage"]
      });
    }
  })('[data-invoca]');
</script>
