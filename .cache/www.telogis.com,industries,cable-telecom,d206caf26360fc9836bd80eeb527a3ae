  $(document).ready(function() {
    var insertChat = function() {
      var se = document.createElement('script');
      se.type = 'text/javascript';
      se.async = true;
      se.src = '//commondatastorage.googleapis.com/code.snapengage.com/js/502e5259-5ba6-4d2f-b2e4-b1a9929bbb4d.js';

      var done = false;

      se.onload = se.onreadystatechange = function() {
        if (!done&&(!this.readyState||this.readyState==='loaded'||this.readyState==='complete')) {
          done = true;

          if(typeof(snapEngageLoaded) !== 'undefined') {
            snapEngageLoaded.resolve();
          }
        }
      };

      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(se, s);
    };

    var EVENTS = $(document);

    EVENTS.on("visitorInformation:country", function(event, country, client_ip) {
      var enabledCountries = ["IE","GB",false,"SE","DK","IS","FI","FR","DE","AT","CH","IT","ES","PT","AE","QA","SA","ZA","NL","LU","BE","US","CA","BR","MX","CL","BO","CO","AU","NZ","PH"];
      var blocked_ips = ["71.41.18.162","64.58.161.50"]

      if((!country || blocked_ips.indexOf(client_ip) || enabledCountries.indexOf(country) >= 0) && !Cookies.get("customer")) {
        insertChat();
      }
    });

    var scheme = document.location.protocol;
    var ipEndpoint = scheme + "//telogis-postmaster.herokuapp.com/visitor.json?callback=?";

    $.ajax({ url: ipEndpoint, dataType: "jsonp" })
      .done(function(response) {
        EVENTS.trigger("visitorInformation:country", response.country, response.client_ip);
      })
      .fail(function() {
        EVENTS.trigger("visitorInformation:country", null, null);
      });
  });
</script>

