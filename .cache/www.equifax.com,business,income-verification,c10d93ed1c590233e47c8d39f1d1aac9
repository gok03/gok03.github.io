<script>
var showChar = 2000;
var maxContent = document.getElementById('commentTxt').innerHTML;
var lessContent = maxContent.substr(0, showChar);
var moreContent = maxContent.substr((showChar-1), (maxContent.length - showChar));
var showingMore = false;

function showMore() {
    if(showingMore) {
		document.getElementById("extContent").innerHTML = lessContent + "..." + '</li></ul></p>';
				showingMore =false;
	} else {
		document.getElementById("extContent").innerHTML = maxContent;
				showingMore = true;
	}
}
</script>
