							<script type="text/javascript">
$(document).ready(function() {
    var showChar = 2000;
    var ellipsestext = "...";
    var moretext = "More+";
    var lesstext = "Less-";
    $('.more').each(function() {
        var content = $(this).html();
    	//var content = $(this).text();
 
        if(content.length > showChar) {
 
            var c = content.substr(0, showChar);
            var h = content.substr(showChar-1, content.length - showChar);
 
            var html = '<span id="extContent">' + c + ellipsestext + '</li></ul></p></span>'+'<span id="ellipseText" class="moreellipses"><a id="mLink" href="" onclick="showMore()" class="morelink">' + moretext + '</a></span>';
 
            $(this).html(html);
        }
 
    });
 	
	
    $(".morelink").click(function(){
        if($(this).hasClass("less")) {
            $(this).removeClass("less");
            $(this).html(moretext);
        } else {
            $(this).addClass("less");
            $(this).html(lesstext);
        }
        $(this).prev().toggle();
        return false;
    });
});
</script>



