<script type="text/javascript">
//Track PDF links
function trackPdfLink(linkElem) {

	var url = linkElem.href;
	var target = linkElem.target;
	_gaq.push([ '_trackEvent', 'Downloads', 'PDF', url.slice(url.indexOf("/", 7)) ]);
	if ("_blank" == target) {
		return true;
	} else {
		setTimeout(function() {
			document.location.href = url;
		}, 250);
		return false;
	}
}

//Track outbound links
function trackOutboundLink(linkElem) {
	var url = linkElem.href;
	var target = linkElem.target;
	_gaq.push([ '_trackEvent', 'Outbound Links', url, , , true ]);
    if ("_blank" == target) {
        return true;
    } else {
    	   setTimeout(function() {
    	        document.location.href = url;
    	    }, 250);
        return false;
    }

}

//PDF and external event tracking setup
var links = document.getElementsByTagName("a");
var domainSearch = "\.(sas|jmp)\.com";
for ( var i = 0; i < links.length; i++) {
	var l = links[i];
	if (l.href.search(domainSearch+".*\.pdf") != -1) {
		l.onclick = function() {
			return trackPdfLink(this);
		}; 
	} else if (l.href.search(domainSearch) == -1) {
        console.log("external link");
		l.onclick = function() {
			return trackOutboundLink(this);
		};
	}
}
</script>
