<script>

function showHideSupportMenu ()
   {
   if ((document.getElementById('supportToolsMenu').style.marginLeft == '0px')) {
      document.getElementById('supportDiv').style.width='33px';
      document.getElementById('supportToolsMenu').style.marginLeft='-200px';
      }
   else {
      document.getElementById('supportDiv').style.width='200px';
      document.getElementById('supportToolsMenu').style.marginLeft='0px';
      }
   }

</script>

