function submitMoreInfo()
   {
   var re = /[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?/;
   if ( 
 (document.getElementById("fullName").value=="") ||  
/* (document.getElementById("company").value=="") ||
 (document.getElementById("phone").value=="") ||   */
       (document.getElementById("emailadd").value==""))
      {
      alert("Please enter all fields.  We promise not to share any information.")
      }
   else
      {
      if (re.test(document.getElementById("emailadd").value) == true)
	document.moreInfo.submit()
      else
	alert("Please enter a valid email address.  We promise not to share an information.");
      }
   
   }

</script>

