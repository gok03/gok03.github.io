<script type="text/javascript">

     

     function validationFile926(){
        var b = true;
         jQuery(".form_div_926 input[type='file']").filter(".ajaxUpload").each(function(i){
            var fileid =  $(this).attr("id");
            jQuery.ajax({
            async:false,
            url: "/DesktopModules/DNNGo_PowerForms/ajaxValidate.aspx",
            cache: false,
            data: "ModuleID=926&type=AjaxCustomUpload&fieldValue="+jQuery(this).val(),
            success: function(Result){
                        if(Result != "true")
                        {
                        jQuery(".form_div_926 #png_upload_"+fileid).attr("src","/DesktopModules/DNNGo_PowerForms/Resource/images/no.png").show();
                        }else
                        {
                            //jQuery(".form_div_926 #png_upload_"+ fileid).attr("src","/DesktopModules/DNNGo_PowerForms/Resource/images/yes.png").show();
                            jQuery(".form_div_926 #png_upload_"+ fileid).hide();
                        }
                          if( Result != "true") {    b= false; } 
                }
            });
        });
        return b;
     }



    jQuery(function ($) {
        var sisForm = $(".form_div_926").sisyphus({ customKeyPrefix: 'bg', timeout: 0, autoRelease: true });
        $("#Form").submit(function () {sisForm.manuallyReleaseData();});

        $("input.other_textbox").bind("keyup change",function(){
           var forid = "#" + $(this).data("forid");
           $(forid).val($(forid).data("value")+ $(this).val());
        });


        tinymce.init({
            selector: "textarea.tinymce",
            entity_encoding: "raw",
            convert_urls: false,
             menubar : false
        });

        
       
       $(".form_div_926 input[lang='Submit']").click( function () { 

            if(!validationFile926())
            {
                return false;
            }
            $(".form_div_926 .group_title span").addClass("min").removeClass("max");
            $(".form_div_926 div[class='group_inside']").show(function () {
               
            });
       });


        
    


        

           
    });
</script>

 
