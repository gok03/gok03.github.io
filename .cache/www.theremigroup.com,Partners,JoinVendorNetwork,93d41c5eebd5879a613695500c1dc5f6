             promptPosition: 'topRight',
             ajaxFormValidationURL: '/DesktopModules/DNNGo_PowerForms/ajaxValidate.aspx?ModuleID=926'
         });

         q(".form_div_926 input[lang='Submit']").click(function () {

             if (!$('.form_div_926').validationEngine('validate')) {
                 return false;
             }

             if($("#g-recaptcha-926").is("div") )
             {
                 if ( !$("#g-recaptcha-926").data("GRecaptcha"))
