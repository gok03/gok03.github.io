<!-- Hide JavaScript from Java-Impaired Browsers
function highlites() {
el = event.srcElement ;
if (el.getAttribute("bright") != null) {
el.className = "liteson" ;
}
}

function lowlites() {
el = event.srcElement ;
if (el.getAttribute("bright") != null) {
el.className = "litesoff";
}
}

document.onmouseover = highlites ;
document.onmouseout = lowlites ;
