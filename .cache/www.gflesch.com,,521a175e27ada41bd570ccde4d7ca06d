    <script type="text/javascript">
        var S;

        function recordEvent(link, category, action) {
            try {
                var pageTracker = _gat._getTracker("UA-39460949-1");
                pageTracker._trackEvent(category, action);
                return true;

            } catch (err) {
                return false;
            }
        }

        $(document).ready(function () {

            var slidesCount = '5';
            var slidesUrls = '["/Slides/slide1.html","/Slides/slide2.html","/Slides/slide3.html","/Slides/slide4.html","/Slides/slide5.html"]';

            if (slidesCount > 0) {
                $('#heroShadowLeftHp').attr('src', '/App_Themes/GFC/Images/body-shadow-left-hp-high.png').css({ "left": "-43px", "top": "5px" });
                $('#heroShadowRightHp').attr('src', '/App_Themes/GFC/Images/body-shadow-right-hp-high.png').css({ "right": "-42px", "top": "5px" });
            }

            try {

                S = new Slider($('div.slider'));
            } catch (e) {
                console.log(e.message);
                S.refreshSlider();
            }

            init(JSON.parse(slidesUrls));

            $('iframe').load(function () {
                $(this).contents().find('body').on('click', function () {

                    var href = $(this).attr('href');
                    e.preventDefault();
                    //console.log('before f');
                    recordEvent(this, 'Banner - Click', 'Homepage Banner');
                    //console.log('after f');
                    //                                setTimeout(function () {
                    //                                    window.location = href;
                    //                                }, 500);
                });
            });

            if ($('.sliderButtonCon a').length > 1) {
                S.start();
                $('.sliderButtonCon').show();
            }
            else {
                $('.sliderButtonCon').hide();
            }

            $('.sliderButtonCon a').click(function () {
                S.gotoSlide($(this).index());
            });

            $('#hero').css({
                left: '1px',
                width: '99.95%'
            });
        });
    </script>
