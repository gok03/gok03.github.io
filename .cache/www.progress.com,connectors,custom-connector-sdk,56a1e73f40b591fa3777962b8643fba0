$(window).on("load resize", function (e) {
    var boxHighest = null;
    var boxHeight = 0;
    function setBoxHeight() {
        var boxH = $(this).height();
        if (boxH > boxHeight) {
            boxHeight = boxH;
            boxHighest = $(this);
        }
    }
    if ($(window).innerWidth() >= 960) {
        $(".Box div.Box-description").each(setBoxHeight);
        $(".Box div.Box-description").css({ "height": boxHighest.height() });
    }
    else {
        $(".Box div.Box-description").css({ "height": "auto" });
    }
});
</script>
  

