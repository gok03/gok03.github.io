<script>
    $("nav.main").sticky({ topSpacing: 28 });
    var navHideTimeout;
    var navHide = function (e) {
        var isNavOrDropDown = $(e.toElement).hasClass("navItem") || $(e.relatedTarget).hasClass("navItem") ||
                                ($.contains($("#NavDropDown")[0], e.toElement)) || $.contains($("#NavDropDown"), e.relatedTarget) || $(e.relatedTarget).attr("id") == "NavDropDown";
        if (!isNavOrDropDown) {
            $("#NavDropDown").slideUp(200);
        }
    };
    var navShowTimeout;
    var navShow = function () {
        $("#NavDropDown").slideDown(200);
    }
    $(".navItem, #NavDropDown").mouseover(function () {
        clearTimeout(navHideTimeout);
        if (navShowTimeout) {
            clearTimeout(navShowTimeout);
            navShowTimeout = null;
        }
        navShowTimeout = setTimeout("navShow()", 400);
    })
    .mouseleave(function (e) {
        clearTimeout(navShowTimeout);
        navHideTimeout = setTimeout(function () { navHide(e) }, 400);
    });

    $(".navItem").mouseover(function () {
        var sectionName = $(this).data("dropdown") + "_dropdown";
        $(".navDropDownItem").hide();
        $("#" + sectionName).show();
    });

    //download tab
    $(".DLpanel").hide();
    $("#trigger").click(function () {
        if (!$(this).hasClass("active")) {
            ga('send', 'pageview', '/vp/downloads-tab/'); //track opening the tab
        }
        $(this).toggleClass("active");
        $(".DLpanel").toggle("fast");
    });

    $(document).click(function (event) {
        //download tab
        if (!($(event.target).parents("#DLpanel").length > 0) && !(event.target.id == "DLpanel") && !(event.target.id == "trigger") && $("#DLpanel").is(":visible")) {
            $("#trigger").toggleClass("active");
            $(".DLpanel").toggle("fast");
        }
    });

    $(".DpanelError").hide();
</script>
