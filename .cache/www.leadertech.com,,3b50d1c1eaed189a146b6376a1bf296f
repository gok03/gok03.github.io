		function startReportingSlideshow() {
		$('#picReportOne').fadeIn(1000).delay(2000).fadeOut(2000);
		$('#picReportTwo').delay(6000).fadeIn(2000).fadeOut(4000, startReportingSlideshow);
		}
		
		function startSurveySlideshow() {
		$('#picSurveyOne').fadeIn(1000).delay(2000).fadeOut(2000);
		$('#picSurveyTwo').delay(3000).fadeIn(3000).fadeOut(3000);
		$('#picSurveyThree').delay(7000).fadeIn(4000).fadeOut(4000, startSurveySlideshow);
		}
		
		$(document).ready(function() {
			startReportingSlideshow();
			startSurveySlideshow();
		});
</script>
