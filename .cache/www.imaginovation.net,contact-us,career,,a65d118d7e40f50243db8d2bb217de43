// Cookies ser 	landing-blog 
    function createCookie(name, value, days, opt) {
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            var expires = "; expires=" + date.toGMTString();
        }
        else
            var expires = "";

        var fixedName = '';
        name = fixedName + name;
        document.cookie = name + "=" + value + expires + "; path=/";
        if (opt) {
        } else {
            afr_submit_hide();
        }

    }


    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ')
                c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) == 0)
                return c.substring(nameEQ.length, c.length);
        }
        return null;
    }

    function fun_hide_msg()
    {
        setTimeout(function () {   //calls click event after a certain time

            var snv = readCookie('snv');
            if (snv == null) {
                if ($("#landing_blog_snv").hasClass("landingslideup"))
                {
                    $('#landing_blog_snv').removeClass('landingslideup');
                }
                else
                {
                    //$('#landing_blog_snv').addClass('landingslideup');
                }
                //fun_hide_msg();
            } else
            {
                $('#landing_blog_snv').addClass('landingslideup');
            }

        }, 15000);
    }


    // on browser close remove cookie
    function setCookie_tmp(name, value, days) {
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days));
            date.setTime(date.getTime() + (days));
            var expires = "; expires=" + date.toGMTString();
        }
        else
            var expires = "";
        var fixedName = '';
        name = fixedName + name;
        document.cookie = name + "=" + value + expires + "; path=/";
    }

    var aaa = 0;
    function add_cookie_set()
    {//console.log(aaa++);
        var snv = readCookie('snv');
        // console.log('sa_test='+readCookie('snv'));
        if (snv == null || snv == 'clear1')
        {
            //createCookie('sa_test', '', 2000);
            //console.log('ent if');
        }
        else
        {
            //console.log('ent else');
            createCookie('snv', '', -1);
            createCookie('snv', 'snv submit', 1);
            //console.log('cookie created' +readCookie('snv'));
        }
        //setTimeout(add_cookie_set, 3000);
    }

    //add_cookie_set();

    function loading_blog_start()
    {
        setTimeout(function () {   //calls click event after a certain time


            var snv = readCookie('snv');
            if (snv == null) {
                $('#landing_blog_snv').removeClass('landingslideup');
                //fun_hide_msg();
            }
            else
            {
                $('#landing_blog_snv').addClass('landingslideup');
            }

        }, 5000);
    }
    function afr_submit_hide()
    {
        setTimeout(function () {
            $('#landing_blog_snv').addClass('landingslideup');
        }, 3000);
    }


    var isWorking = 0;
    $(window).scroll(function () {
        var height = $(document).height();
        height = parseInt(($(document).height() * 75) / 100);

        if ($(window).scrollTop() + height >= height) {
            if (isWorking == 0) {
                isWorking = 1;
                loading_blog_start();
                popupblogg();
            }

        }
    });


    function popupblogg()
    {
        
        setTimeout(function () {
            setInterval(function () {
                //( Date.now() - startTime );
                $count = $('.count_number');
                $count.html(function (_, num) {
                    return +num - 1;
                })
            }, 2000);
        }, 5000);

        setTimeout(function () {
            //alert('hi')

            $('.blog-dialog').css('display', 'block');
            $('.countdown-start').addClass('persion-none');
            createCookie("snv", "snv submit", 1, 1);
            isWorking = 0;
        }, 10000);



    }

</script> 
