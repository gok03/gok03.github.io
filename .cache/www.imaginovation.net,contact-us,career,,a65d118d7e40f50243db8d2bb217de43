		randomNr2 = Math.floor(Math.random()*10);
		totalNr_2 = randomNr1 + randomNr2;
		var texti_2 = "What is "+randomNr1+" + "+randomNr2;
		$('#ebcaptchatext_2').text(texti_2);
			
			$('#gform_2 input[type=submit]').click(function(){
				
				var form_id =$(this).parents("form").attr("id");
				var ebcaptchainput_val_2 = jQuery('#input_2_6').val();
				
				var name 	= jQuery('#input_2_1_3').val();
				var email    = jQuery('#input_2_3').val();
				var phone    = jQuery('#input_2_2').val();
				
				
				if((ebcaptchainput_val_2 != ''))
				{	if(ebcaptchainput_val_2 != totalNr_2)
					{
						jQuery('#field_2_6 .ginput_container').next().remove('div');
