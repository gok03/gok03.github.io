    randomNr1 = Math.floor(Math.random() * 10);
    randomNr2 = Math.floor(Math.random() * 10);
    totalNr_1 = randomNr1 + randomNr2;
    var texti_1 = "What is " + randomNr1 + " + " + randomNr2;
    //alert(texti_1);
    $('#ebcaptchatext_1').html(texti_1);
