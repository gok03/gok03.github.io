    $('#gform_1 input[type=submit]').click(function () {

        var form_id = $(this).parents("form").attr("id");
        var ebcaptchainput_val_1 = jQuery('#input_1_9').val();
        //alert('y'+ebcaptchainput_val_1);
        var fname = jQuery('#input_1_1_3').val();
        var lname = jQuery('#input_1_1_6').val();
        var file = jQuery('#input_1_2').val();
        var position = jQuery(".gfield_radio input[type='radio']:checked").val();
        var email = jQuery('#input_1_5').val();
        var phone = jQuery('#input_1_6').val();
        var file_temp = $("#" + form_id + " .gform_delete").attr("title");


        if ((ebcaptchainput_val_1 != ''))
        {
            if (ebcaptchainput_val_1 != totalNr_1)
            {
                jQuery('#field_1_9 .ginput_container').next().remove('div');
                jQuery('#field_1_9 .gfield_description').html('');
                jQuery('#field_1_9').addClass('req-fix');
                jQuery('#field_1_9 .ginput_container').after('<div class="gfield_description validation_message">Please enter the valid captcha.</div>');
                return false;
            }
        }

        if ((fname != '') && (lname != '') && (file != '' || file_temp == 'Delete file') && (email != '') && (phone != '') && position && (ebcaptchainput_val_1 == ''))
        {

            jQuery('#field_1_9 .ginput_container').next().remove('div');
            jQuery('#field_1_9 .gfield_description').html('');
            jQuery('#field_1_9').addClass('req-fix');
            jQuery('#field_1_9 .ginput_container').after('<div class="gfield_description validation_message">Please enter the valid captcha.</div>');
            return false;

        }
        else
        {
            jQuery('#' + form_id).submit();
        }
    });



