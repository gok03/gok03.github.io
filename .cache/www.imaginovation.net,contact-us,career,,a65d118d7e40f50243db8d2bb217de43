                        $('.cs_numbers li').on('click', function (e) {
                            var tabname = $(this).data('tabs');
                            //alert(tabname);
                            $('.cs_numbers li').removeClass('active');
                            $('.cs_tabs.active').fadeOut(500);
                            $('.cs_tabs').removeClass('active');
                            $(this).addClass('active');
                            $('#' + tabname).fadeIn(1000);
                            setTimeout(function () {
                                $('#' + tabname).addClass('active');
                            }, 1000);
                        });
                    });
