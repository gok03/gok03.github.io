					{

						pfs(port_arr2[por2-1]).animate({"left": "345px"}, "slow").hide('slow');

						pfs(port_arr2[por2-2]).css('left','-345px').show().animate({"left": "0px"}, "slow");

						por2--;
					}		
				}

				

				pfs('#port_right').click(function(){

					//alert('right');	

					portfolio2();

					clearInterval(Portfolio_Timer2);

					Portfolio_Timer2 =setInterval(portfolio2, 3000);

				});

				

				pfs('#port_left').click(function(){

					//alert('left');	

					portfolio_leftSlide2();

					clearInterval(Portfolio_Timer2);

					Portfolio_Timer2 =setInterval(portfolio2, 3000);

				});					 
});

/* ---------------------- Home Portfolio Slider --------------------*/
pfs("document").ready(function(){
 				var port_arr3 = jQuery.makeArray(pfs('.port_folio_list_home .portfolio_slides_home'));
                //demo3 
				var por3=1; 
				//alert(arr.length);
				pfs('.port_folio_list_home .portfolio_slides_home').hide();
				pfs('.port_folio_list_home .portfolio_slides_home:first-child').show();
				
                var Portfolio_Timer3 =setInterval(portfolio3, 3000); 
				
		
				pfs('.portfolio_slides_home').mouseover(function(){
				  clearInterval(Portfolio_Timer3);
				  pfs('.port_arrow').css('opacity', '0.8');
				 
				});
				pfs('.portfolio_slides_home').mouseout(function(){
				  Portfolio_Timer3 = setInterval(portfolio3, 3000);
				  pfs('.port_arrow').css('opacity', '0.1'); 
				});
				pfs('.port_folio_list_home .port_arrow').mouseover(function(){
					pfs('.port_arrow').css('opacity', '0.8');
				});
				pfs('.port_folio_list_home .port_arrow').mouseout(function(){
					pfs('.port_arrow').css('opacity', '0.1');
				});
				
				
    			function portfolio3() 
				{
				  //alert(i);
				  //alert(por3);
					if(por3==0)
					{
						pfs(port_arr3[port_arr3.length-1]).animate({"left": "-270px"}, "slow").hide('slow');
						pfs(port_arr3[por3]).css('left','270px').show().animate({"left": "0px"}, "slow");
					}else
					{
						pfs(port_arr3[por3-1]).animate({"left": "-270px"}, "slow").hide('slow');
						pfs(port_arr3[por3]).css('left','270px').show().animate({"left": "0px"}, "slow");
					}		
					por3++;
					if(por3 >= port_arr3.length)
						{
							por3=0;
						}	
						
				}
				
				function portfolio_leftSlide3() 
				{

					//alert(por3);
					if(por3==0)
					{	//alert("test1")

						pfs(port_arr3[port_arr3.length-1]).animate({"left": "270px"}, "slow").hide('slow');

						pfs(port_arr3[port_arr3.length-2]).css('left','-270px').show().animate({"left": "0px"}, "slow");

						por3=port_arr3.length-1;
						
					}else if(por3==1)
					{
                        //alert("test2")
						pfs(port_arr3[por3-1]).animate({"left": "270px"}, "slow").hide('slow');

						pfs(port_arr3[port_arr3.length-1]).css('left','-270px').show().animate({"left": "0px"}, "slow");

						por3--;
						
					}else
					{

						pfs(port_arr3[por3-1]).animate({"left": "270px"}, "slow").hide('slow');

						pfs(port_arr3[por3-2]).css('left','-270px').show().animate({"left": "0px"}, "slow");

						por3--;
					}		
				}

				

				pfs('.port_folio_list_home #port_right').click(function(){

					//alert('right');	

					portfolio3();

					clearInterval(Portfolio_Timer3);

					Portfolio_Timer3 =setInterval(portfolio3, 3000);

				});

				

				pfs('.port_folio_list_home #port_left').click(function(){

					//alert('left');	

					portfolio_leftSlide3();

					clearInterval(Portfolio_Timer3);

					Portfolio_Timer3 =setInterval(portfolio3, 3000);

				});					 
});

