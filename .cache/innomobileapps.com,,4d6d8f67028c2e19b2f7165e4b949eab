</script> <script type="text/javascript" language="javascript">
var pfs=jQuery; pfs.noConflict();
pfs("document").ready(function(){
 				var port_arr = jQuery.makeArray(pfs('.port_folio_list .portfolio_slides'));
                //demo3 
				var por=1; 
				//alert(arr.length);
				pfs('.port_folio_list .portfolio_slides').hide();
				pfs('.port_folio_list .portfolio_slides:first-child').show();
				
                var Portfolio_Timer =setInterval(portfolio, 3000); 
				
		
				pfs('.portfolio_slides').mouseover(function(){
				  clearInterval(Portfolio_Timer);
				  pfs('.port_arrow').css('opacity', '0.6');
				 
				});
				pfs('.portfolio_slides').mouseout(function(){
				  Portfolio_Timer = setInterval(portfolio, 3000);
				  pfs('.port_arrow').css('opacity', '0.1'); 
				});
				pfs('.portfolio_slides .port_arrow').mouseover(function(){
					pfs('.port_arrow').css('opacity', '0.6');
				});
				pfs('.portfolio_slides .port_arrow').mouseout(function(){
					pfs('.port_arrow').css('opacity', '0.1');
				});
				
				
    			function portfolio() 
				{
				  //alert(i);
				  //alert(por);
					if(por==0)
					{
						pfs(port_arr[port_arr.length-1]).animate({"left": "-500px"}, "slow").hide('slow');
						pfs(port_arr[por]).css('left','500px').show().animate({"left": "0px"}, "slow");
					}else
					{
						pfs(port_arr[por-1]).animate({"left": "-500px"}, "slow").hide('slow');
						pfs(port_arr[por]).css('left','500px').show().animate({"left": "0px"}, "slow");
					}		
					por++;
					if(por >= port_arr.length)
						{
							por=0;
						}	
						
				}
				
				function portfolio_leftSlide() 
				{

					//alert(por);
					if(por==0)
					{	//alert("test1")

						pfs(port_arr[port_arr.length-1]).animate({"left": "500px"}, "slow").hide('slow');

						pfs(port_arr[port_arr.length-2]).css('left','-500px').show().animate({"left": "0px"}, "slow");

						por=port_arr.length-1;
						
					}else if(por==1)
					{
                        //alert("test2")
						pfs(port_arr[por-1]).animate({"left": "500px"}, "slow").hide('slow');

						pfs(port_arr[port_arr.length-1]).css('left','-500px').show().animate({"left": "0px"}, "slow");

						por--;
						
					}else
					{

						pfs(port_arr[por-1]).animate({"left": "500px"}, "slow").hide('slow');

						pfs(port_arr[por-2]).css('left','-500px').show().animate({"left": "0px"}, "slow");

						por--;
					}		
				}

				

				pfs('#port_right').click(function(){

					//alert('right');	

					portfolio();

					clearInterval(Portfolio_Timer);

					Portfolio_Timer =setInterval(portfolio, 3000);

				});

				

				pfs('#port_left').click(function(){

					//alert('left');	

					portfolio_leftSlide();

					clearInterval(Portfolio_Timer);

					Portfolio_Timer =setInterval(portfolio, 3000);

				});					 
});

