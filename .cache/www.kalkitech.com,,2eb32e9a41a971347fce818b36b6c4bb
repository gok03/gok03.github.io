		//			$targetul.dequeue().fadeOut(droplinemenu.animateduration.out)
				}
			) //end hover
			
			
			$("li.drop1").hover(
				function(e){
					var $targetul=$(this).children("ul:eq(0)")
					if ($targetul.queue().length<=1) //if 1 or less queued animations
						if (this.istopheader)
							$targetul.css({left: $mainmenu.position().left, top: $mainmenu.position().top+this._dimensions.h})
$targetul.css({'z-index': '9999'})
						if (document.all && !window.XMLHttpRequest) //detect IE6 or less, fix issue with overflow
							$mainmenu.find('ul').css({overflow: (this.istopheader)? 'hidden' : 'visible'})
						$targetul.dequeue().fadeIn(droplinemenu.animateduration.over)
				},
				function(e){
					var $targetul=$(this).children("ul:eq(0)")
                                        $targetul.css({'z-index': '9998'})
                                        $targetul.dequeue().stop()

				}
			) //end hover
			
			$('#navnew').mouseleave(
									function(e){
										$mainmenu.find("ul").dequeue().fadeOut(droplinemenu.animateduration.out)
