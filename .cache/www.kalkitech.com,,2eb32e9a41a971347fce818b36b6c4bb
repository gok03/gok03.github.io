							$targetul.css({left: $mainmenu.position().left, top: $mainmenu.position().top+this._dimensions.h, 'z-index': '9999'})
							
						}
						if (document.all && !window.XMLHttpRequest) //detect IE6 or less, fix issue with overflow
							$mainmenu.find('ul').css({overflow: (this.istopheader)? 'auto' : 'visible'})
						$targetul.dequeue().fadeIn(droplinemenu.animateduration.over)
						
				}
			) //end click
			
		}) //end $headers.each()
		$mainmenu.find("ul").css({display:'none', visibility:'visible', width:$mainmenu.width()})
	}) //end document.ready
