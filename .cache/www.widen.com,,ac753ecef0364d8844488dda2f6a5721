var left = new CountUp("numberleft", 0, 460, 0, 3.5, options1);

var options2 = {
    useEasing : true,
    useGrouping : true,
    separator : ',',
    decimal : '.',
    prefix : '',
    suffix : ''
};
var middle = new CountUp("numbermiddle", 0, 177760, 0, 3.5, options2);

var options3 = {
    useEasing : true,
    useGrouping : true,
    separator : ',',
    decimal : '.',
    prefix : '',
    suffix : ''
};
var right = new CountUp("numberright", 0, 203, 0, 3.5, options3);

</script></div>
