						jQuery('body').css('padding-top', '0px');
						jQuery('#header').css('margin-top','0px');
						break;

					case 'regularsize':
						jQuery('body').css('padding-top', msg.params+' !important');
						jQuery('iframe[data-inffuse-app="welcomebar"]').css('height',msg.params);
						var toolbar = jQuery('#welcomebar-iframe');
						toolbar.parents('.inside').css('height',msg.params);
						break;

					case 'removeBar':
						jQuery('body').css('padding-top', '0px');
						jQuery('iframe[data-inffuse-app="welcomebar"]').css('display','none');
						jQuery('#header').css('margin-top','0px');
						jQuery('.cento-header, .paris-header, #navigation, .birdseye-header, .header-wrap').css('margin-top','0px');
						jQuery('.body-wrap').css('padding-top','0px');
						jQuery('#header-wrap').css('margin-top','0px');
					break;			

					case 'mobile':
						jQuery('iframe[data-inffuse-app="welcomebar"]').css('height',msg.params);
					break;

					case 'debugmode':
						debugger;
					break;
						
					default:
						InffuseClient.postToWidget(msg.project,msg.type,msg.params);
						break;
				}
			}

			this.resize = function(project_id,params)
			{
				var widget = jQuery('[data-inffuse-project="'+project_id+'"]');
				if (params.height)
					widget.height(params.height);

				if (params.width)
					widget.width(params.width);
			}

			this.postToWidget = function(project_id,type,params)
			{
				var msg = {
					app: 'inffuse',
					type: type,
					params: params
				}

				var widget = jQuery('[data-inffuse-project="'+project_id+'"]');
				widget[0].contentWindow.postMessage(msg, "*");
			}
		}
	}

	var default_height = '50px';
	var toolbar = jQuery('#welcomebar-iframe');
	toolbar.css('height',default_height)
	//toolbar.parent().prependTo('body');
	toolbar.parents('.inside').css({
		'position': 'fixed',
		'left': '0',
		'top': '0',
		'height': default_height,
		'width': '100%',
		'z-index': '99999',
		'xdisplay': 'none'
	});


	//jQuery('body').css('padding-top', '50px !important');


	var MyElement = PlatformElement.extend({});
	return MyElement;
})();
;

			if (typeof _ElementDefinition == 'undefined' || typeof _ElementDefinition == 'null') {
				var _ElementDefinition = PlatformElement.extend({});
			}

			var _Element = _ElementDefinition.extend({
				initialize: function() {
					// we still want to call the initialize function defined by the developer
					// however, we don't want to call it until placeholders have been replaced
					this.placeholderInterval = setInterval(function() {
						// so use setInterval to check for placeholders.
						if (this.$('.platform-element-child-placeholder').length == 0) {
							clearInterval(this.placeholderInterval);
							this.constructor.__super__.initialize.apply(this);
						}
					}.bind(this), 100);
				}
			});

			_Element.prototype.settings = new PlatformElementSettings([]);
			_Element.prototype.settings.page_element_id = "484049276374735591";
			
			_Element.prototype.element_id = "09d08779-e09d-471d-9b6b-81a2e4cb13d6";
			_Element.prototype.user_id = "64678889";
			_Element.prototype.site_id = "514871638505035055";
