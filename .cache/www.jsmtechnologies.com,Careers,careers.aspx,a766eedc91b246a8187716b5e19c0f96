    <script type="text/javascript">
        $(document).ready(function () {
            //Hide Back to top button
            $("#back-to-top").hide();
            $(window).scroll(function () {
                if ($(window).scrollTop() > 200) {
                    $('#back-to-top').fadeIn();
                } else {
                    $('#back-to-top').fadeOut();
                }
            });
            $('#back-to-top img').click(function () {
                $('body').animate({
                    scrollTop: 0
                }, 1000);
            });
        });
    </script>

  


    

