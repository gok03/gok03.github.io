                $('.slide-out-div2').tabSlideOut1({
                    tabHandle: '.handle2',                    //class of the element that will become your tab
                    //pathToTabImage: 'images/right-panel.jpg', //path to the image for the tab //Optionally can be set using css
                    //imageHeight: '241px',                     //height of tab image //Optionally can be set using css
