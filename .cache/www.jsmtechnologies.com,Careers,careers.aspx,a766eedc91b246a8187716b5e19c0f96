        (function ($) {
            $(function () {
                $('.handle2').click(function () {
                    $('.sliderforms').css('display', 'none');
                    var elmid = $(this).attr('id');
                    var formid = elmid + '_form';
                    $('#' + formid).css('display', '');
                    return false;
                });

