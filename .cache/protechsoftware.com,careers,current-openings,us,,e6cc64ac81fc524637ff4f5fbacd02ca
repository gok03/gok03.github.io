		});
	$('.demo .expand .even').click(function(){
		$('.demo .expand .odd').removeClass('active');
		$('.demo .expand .odd').removeClass('active1');
		$('.demo .expand .even').addClass('active1');
		
		});	*/
    
	
	});
</script>



