function FrontPage_Form3_Validator(theForm)
{

  if (theForm.UserName.value == "")
  {
    alert("Please enter a value for the \"UserName\" field.");
    theForm.UserName.focus();
    return (false);
  }

  if (theForm.UserName.value.length < 3)
  {
    alert("Please enter at least 3 characters in the \"UserName\" field.");
    theForm.UserName.focus();
    return (false);
  }

  if (theForm.UserEmail.value == "")
  {
    alert("Please enter a value for the \"Email Address\" field.");
    theForm.UserEmail.focus();
    return (false);
  }

  if (theForm.UserEmail.value.length < 3)
  {
    alert("Please enter at least 3 characters in the \"Email Address\" field.");
    theForm.UserEmail.focus();
    return (false);
  }

  if (theForm.UserHome.value == "")
  {
    alert("Please enter a value for the \"UserHome\" field.");
    theForm.UserHome.focus();
    return (false);
  }

  if (theForm.UserHome.value.length < 7)
