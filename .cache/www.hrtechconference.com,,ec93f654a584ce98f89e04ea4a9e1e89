<!-- begin email sign up script-->
<script>
		function showEmail() {
			$('body').append($(document.createElement('div'))
									.attr('id', 'lrp_overlay')
									.attr('style',
											'z-index:500 !important;background: none repeat scroll 0 0 #000; top:0; position:absolute; zoom: 1; filter: alpha(opacity=40); opacity: 0.4; height:'
													+ $(window).height() + 'px; width:' + $(window).width()	+ 'px;'));
			$('#relative_email_anchor').css('display', 'block');
			$('#closelrpemail').on('click', function() {
				$('#lrp_float_email').empty();
				$('#relative_email_anchor').css("display", "none");
				$('#lrp_overlay').css('display', 'none');
			});
			$('#submitemail').on('click', function() {
				$('#lrp_float_email').empty();
				$('#relative_email_anchor').css("display", "none");
				$('#lrp_overlay').css('display', 'none');
			});
		}
		function setCookie(cname, cvalue, exdays) {
			var d = new Date();
			d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
			var expires = "expires=" + d.toGMTString();
			document.cookie = cname + "=" + cvalue + "; " + expires;
		}
		function getCookie(cname) {
			var name = cname + "=";
			var ca = document.cookie.split(';');
			for ( var i = 0; i < ca.length; i++) {
				var c = ca[i];
				while (c.charAt(0) == ' ')
					c = c.substring(1);
				if (c.indexOf(name) != -1)
					return c.substring(name.length, c.length);
			}
			return "";
		}
		if (getCookie("v") == "") {
			setTimeout(function() {showEmail();}, 3000);
			setCookie("v", "x", 180);
		}
	</script><!--end email sign up script--><!-- InstanceEndEditable -->
