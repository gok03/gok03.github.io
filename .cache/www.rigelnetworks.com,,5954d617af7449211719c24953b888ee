<script>
$(document).ready(function(){
	//alert('hekki');
	




var appendthis =  ("<div class='modal-overlay js-modal-close'></div>");

  $('a[data-modal-id]').click(function(e) {
    e.preventDefault();
    $("body").append(appendthis);
    $(".modal-overlay").fadeTo(500, 0.7);
    //$(".js-modalbox").fadeIn(500);
    var modalBox = $(this).attr('data-modal-id');
    $('#'+modalBox).fadeIn($(this).data());
	
	var pdfName = $(this).data('pdf');
	
	//alert(pdfName);
	if(y != undefined)
	document.getElementById("hiddenfield1").value = pdfName;
if(x != undefined)
	document.getElementById("hiddenfield").value = pdfName;
if(a != undefined)
	document.getElementById("hiddenfield2").value = pdfName;
if(b != undefined)
	document.getElementById("hiddenfield3").value = pdfName;
	
	//alert(document.getElementById("hiddenfield").value);
	//alert(document.getElementById("hiddenfield1").value);
	//alert(document.getElementById("hiddenfield2").value);
	//alert(document.getElementById("hiddenfield3").value);
	
  });  
  
  
$(".js-modal-close, .modal-overlay").click(function() {
  $(".modal-box, .modal-overlay").fadeOut(500, function() {
    $(".modal-overlay").remove();
  });
});
 

 
//$(window).resize();

// Set pdf download popup

var x = document.getElementById("hiddenfield");
//alert(x);
if(x != undefined)
x.setAttribute("type", "hidden");
 
var y = document.getElementById("hiddenfield1");
 if(y != undefined)
 //alert(y);
y.setAttribute("type", "hidden");

var a = document.getElementById("hiddenfield2");
//alert(a);
if(a != undefined)
a.setAttribute("type", "hidden");

var b = document.getElementById("hiddenfield3");
//alert(b);
if(b != undefined)
b.setAttribute("type", "hidden");
// Set value of hidden input field
/* if(x != undefined)
document.getElementById("hiddenfield").value = pdfName;
 if(y != undefined)
document.getElementById("hiddenfield1").value = pdfName;  */
 }); 
 




</script>	
	<script type="text/javascript">
	
	
			
	if(window.location.hash) 
	{
      var hash = window.location.hash.substring(1); //Puts hash in variable, and removes the # character

	   // PDF popup
	   if(hash !== "")
	   {
			//alert(hash);
			
			
			if(hash == "wpcf7-f4527-o4" || hash == "wpcf7-f4528-o4" ){
				//alert("got it");
				
				var e = document.getElementById("c-tabs_Publications");
					e.className += " c-tabs-nav__link";
					//alert(2);
				
				var d = document.getElementById("c-tabs_customer_success");
					d.className += " c-tabs-nav__link is-active";
					//alert(3);
				if($("#c-tabs_customer_success").hasClass("is-active")){
					$("#c-tabs_Publications").removeClass( "is-active" );
				   // $(".c-tab").addClass("is-active");
				   $(".c-tab:eq(0)").removeClass("is-active");
					$(".c-tab:eq(1)").addClass("c-tab is-active");
    				
				}					
				$("#popup2").show();
				$("#popup3").show();
				
				
				var stateMesaeg= $(".wpcf7-mail-sent-ok").css('visibility');
			 
				//alert(stateMesaeg);
				
				if(stateMesaeg == "visible")
				{
					//alert(stateMesaeg);
					 
					// Remove the # from the hash, as different browsers may or may not include it
					window.location.hash = '';
					//$("#popup").hide(5000);
					setTimeout(function() {
						
						$('#popup2').fadeOut('fast');
						$('#popup3').fadeOut('fast');
						
						$(".wpcf7-mail-sent-ok").fadeOut('fast');
						
					}, 5000);
					

					
				}
			}else{
				//alert("hello");
				$("#popup").show();
				$("#popup1").show();
				
				var stateMesaeg= $(".wpcf7-mail-sent-ok").css('visibility');
				 
				//alert(stateMesaeg);
				
				if(stateMesaeg == "visible")
				{
					 
					// Remove the # from the hash, as different browsers may or may not include it
					window.location.hash = '';
					//$("#popup").hide(5000);
					setTimeout(function() {
						$('#popup').fadeOut('fast');
						$('#popup1').fadeOut('fast');
						
						
						$(".wpcf7-mail-sent-ok").fadeOut('fast');
						
					}, 5000);
					

					
				}
			
			}
			
	   }
	  
	} 

	
</script>
