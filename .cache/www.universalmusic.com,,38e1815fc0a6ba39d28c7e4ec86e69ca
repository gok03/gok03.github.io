<script>

	function submitSignUp() {
		has_errors = false;
//		jQuery("#too-young").hide();

		// validate date
		c = jQuery("#country-dd").val();
		z = jQuery("#zip").val();
		em = jQuery("#email-inp").val();

		
		if (c == '') {
			jQuery("#country-wrap").addClass('error');
			has_errors = true;
		}

		if (em == '') {
			jQuery("#email-wrap").addClass('error');
			has_errors = true;
		}


		if (z == '') {
			jQuery("#zip-wrap").addClass('error');
			has_errors = true;
		}

		if (!has_errors) {
			 jQuery.ajax({
		        url: 'http://www.universalmusic.com/wp-admin/admin-ajax.php',
		        data: {
		            'action':'email_subscribe',
		            'email' : em,
		            'country':c,
		            'zip':z
		        },
		        success:function(data) {
					jQuery('#shareModal').foundation('reveal', 'open');
					console.log(data);
		        },
		        error: function(errorThrown){
		            jQuery('#shareModal').foundation('reveal', 'open');
		        }
		    });
		} 
	}
</script>


