		<!-- LiveChat Widget -->
		<script type="text/javascript">
		   var chnValue = null;
		   var psValue = null;
		   var srcValue = null;
		   var cidValue = null;
		   var adgValue = null;

		   if (window.location.href.indexOf('omn_chn') > -1) {
		      chnValue = getQueryStringParameter("omn_chn");
		   }
		   if (window.location.href.indexOf('omn_ps') > -1) {
		      psValue = getQueryStringParameter("omn_ps");
		   }
	       if (window.location.href.indexOf('omn_src') > -1) {
	          srcValue = getQueryStringParameter("omn_src");
	       }
	       if (window.location.href.indexOf('omn_cid') > -1) {
	          cidValue = getQueryStringParameter("omn_cid");
	       }
	       if (window.location.href.indexOf('omn_adg') > -1) {
	          adgValue = getQueryStringParameter("omn_adg");
	       }
	   
           if ((chnValue != null && chnValue != "" && chnValue != undefined)){
              $.cookie("SEMTrackingChnVal",chnValue,{path:'/'});
           }
		   if ((psValue != null && psValue != "" && psValue != undefined)){
              $.cookie("SEMTrackingPSVal",psValue,{path:'/'});
           }
           if ((srcValue != null && srcValue != "" && srcValue != undefined)) {
              $.cookie("SEMTrackingSrcVal",srcValue,{path:'/'});
           }
           if ((cidValue != null && cidValue != "" && cidValue != undefined)) {
              $.cookie("SEMTrackingCidVal",cidValue,{path:'/'});
           }
           if ((adgValue != null && adgValue != "" && adgValue != undefined)) {
              $.cookie("SEMTrackingAdgVal",adgValue,{path:'/'});
           }
           //Cookie Code Ends
           
           var chat_value = 'Website';
           if ((chnValue != null && chnValue != "" && chnValue != undefined) & (psValue != null && psValue != "" && psValue != undefined)) {
              $('.hiddenChnValue').val(chnValue);
              $('.hiddenPsValue').val(psValue);
           }
           else {
              $('.hiddenChnValue').val($.cookie("SEMTrackingChnVal"));
              $('.hiddenPsValue').val($.cookie("SEMTrackingPSVal"));
           }
		   
		   if ((cidValue != null && cidValue != "" && cidValue != undefined)) {
              $('.hiddenCIDValue').val(cidValue);
           }
           else {
              $('.hiddenCIDValue').val($.cookie("SEMTrackingCidVal"));
           }

           if(($('.hiddenChnValue').val() != "") && ($('.hiddenChnValue').val() != "null") && ($('.hiddenChnValue').val() != undefined  ) & ($('.hiddenPsValue').val() != "") && ($('.hiddenPsValue').val() != "null") && ($('.hiddenPsValue').val() != undefined  )) {
              if($('.hiddenChnValue').val() == "ps" && $('.hiddenPsValue').val() == 1){
			     chat_value = 'Website SEM';
			  }
           }
		  if(($('.hiddenChnValue').val() != "") && ($('.hiddenChnValue').val() != "null") && ($('.hiddenChnValue').val() != undefined  ) ) {
              if(($('.hiddenChnValue').val() == "sn")){
			     chat_value = 'Website Social';
			  }
           }		 
           
           // Live Chat config
           var __lc = {};
           __lc.license = 3263242;
           __lc.ga_version = 'gtm';
           
           // Custom variables
           var chat_lable = 'chat source: ';
           //var chat_value = 'Website';
           __lc.params =   [{ name: chat_lable.toString(), value: chat_value.toString()}];
           
           // Live Chat code
           (function (d, sr) {
              var lc = d.createElement(sr);
              lc.src = '//cdn.livechatinc.com/tracking.js';
              lc.type = 'text/javascript';
              lc.async = 'true';
              var s = d.getElementsByTagName(sr)[0];
              s.parentNode.insertBefore(lc, s);
           })
           (document, 'script');
		</script>


<script type="text/javascript">
//Starts: Script to remove extra space from Top    
$('img').each(function(index,value){
	var src = $(this).attr('src');
	if(src.indexOf('googleads.g.doubleclick.net')>0){
		$(this).hide();
	}
});
//Ends: Script to remove extra space from Top     
</script>

