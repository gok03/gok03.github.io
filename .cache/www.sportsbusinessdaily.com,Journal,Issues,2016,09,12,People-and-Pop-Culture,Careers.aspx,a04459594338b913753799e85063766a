<script type="text/javascript" language="javascript">
    function addLoadEvent(func) {
        var oldonload = window.onload;
        if (typeof window.onload != 'function') {
            window.onload = func;
        } else {
            window.onload = function () {
                if (oldonload) {
                    oldonload();
                }
                func();
            }
        }
    }

    function setupStickyRail() {
        var footerPosition = $(".footerLinkCols").position();
        var storyHeight = $('.articleDetailContent').height();

        $(document).bind('scroll', function (e) {
            var scrollTop = $(this).scrollTop();

            console.log("storyHeight: " + storyHeight);
            console.log("scrollTop: " + scrollTop);

            if (storyHeight - scrollTop <= 600) {
                $(".upsellWrapper").css({ 'position': 'absolute' });
                $(".upsellWrapper").css({ 'bottom': 'initial' });
                $(".upsellWrapper").css({ 'top': footerPosition.top - 200 });
            } else {
                $(".upsellWrapper").css({ 'position': 'fixed' });
                $(".upsellWrapper").css({ 'bottom': '150px' });
                $(".upsellWrapper").css({ 'top': 'initial' });
            }
        });
    };

    addLoadEvent(setupStickyRail);
</script>
