    <script>
        $.fn.watch = function (property, callback) {
            return $(this).each(function () {
                var self = this;
                var old_property_val = $(this).css("height");
                var timer;

                function watch() {
                    if ($(self).data(property + '-watch-abort') == true) {
                        timer = clearInterval(timer);
                        $(self).data(property + '-watch-abort', null);
                        return;
                    }
                    if ($(self).css("height") != null && $(self).css("height") != old_property_val) {
                        old_property_val = $(self).css("height");
                        callback.call(self);
                    }
                }
                timer = setInterval(watch, 700);
            });
        };
        if ($('.scWebEditRibbon') != null)
            $('.scWebEditRibbon').watch("style", function () { $('.navbar-fixed-top').css({ top: $(".scWebEditRibbon").outerHeight() + 'px' }) })
    </script>
