           <script>
	  jQuery(window).load(function(e) {
        // set heights for billboard wrap text
		jQuery('.billboardWrap').each(function(index, element) {
			var h2Height = jQuery('h2',this).height();
			var btnHeight = jQuery('a', this).height();
			jQuery(this).height(h2Height+btnHeight);
            //console.debug(h2Height);
        });
    });
	  
	  
		jQuery(document).foundation('orbit', {
		  timer_speed: 7000,
		  animation_speed: 500,
		  bullets: false,
		  slide_number: false,
		  pause_on_hover: false,
		  stack_on_small: false
		});
  </script>
