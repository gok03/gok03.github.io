					$(".inv").removeClass("active");
					$(".store").removeClass("active");
				}
			});


$("a[href^=http]").each(function(){

      // NEW - excluded domains list
      var excludes = [
         'recruitingsite.com',
         'healthstream.com',
         'excluded.subdomain.com'
      ];
      for(var i=0; i < excludes.length; i++) {
         if(this.href.indexOf(excludes[i]) != -1) {
            return true; // continue each() with next link
         }
      }

      if(this.href.indexOf(location.hostname) == -1) {

           // attach a do-nothing event handler to ensure we can 'trigger' a click on this link
           $(this).click(function() { return true; });

           $(this).attr({
               target: "_blank",
               title: "Opens in a new window"
           });

           $(this).click(); // trigger it
      }
   })

   $('.down-nav').click(function(){
   		var proHeight = $('.pro-nav').height();
   		$('.pro-nav').animate({ scrollTop : proHeight   }, 500);
   });

   $('.pro-nav').bind('scroll', function(){
   		if($('.pro-nav').scrollTop() > 230) {
   			$('.down-nav').fadeOut();
   		} else {
   			$('.down-nav').fadeIn();
   		}

	});

   	$(window).resize(function(){
   		if($(window).height() > 735) {
   			$('.down-nav').fadeOut();
   		}
   		else {
   			$('.down-nav').fadeIn();
   		}
   	});

   	//trigger for survey research link inside patient experience (products navigation element)
    $('.survey-trigger').click(function(e){
		e.preventDefault();
		$('.drop[data-sf-id="637457cc-2e5e-68eb-b67a-ff0000121ea5"] a').trigger('click');
	});




    $('.event-h1').click();

		});//End Window Load

