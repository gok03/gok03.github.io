<script type="text/javascript">
    $('.marquee-wrapper > *').clone().appendTo('.marquee-wrapper');
    
    var total_dur = 10000 * $('.marquee-wrapper a').length;
    var total_dist = 0.5 * $('.marquee-text-container').width();
    
    function loop() {
      $('.marquee-text-container').css('text-indent', 0);
      
      $('.marquee-text-container').animate(
        { 'text-indent': -1*total_dist }, 
        { duration: total_dur, 
          easing: 'linear', 
          done: loop });
    }
    
    $('.marquee-text-container').hover(function() {
        $('.marquee-text-container').pause();
    }, function () {
        $('.marquee-text-container').resume();
    });
                                         
    loop();
</script><span></span>
            
