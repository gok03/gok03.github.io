			$(this).scroll(function(){
				var window_new_top = $(window).scrollTop();
				if(window_top > window_new_top ) {
					$( ".resource-nav" ).slideUp();
					$( ".community-nav" ).slideUp();
					$( ".pro-nav" ).slideUp();
					$( ".inv-nav" ).slideUp();
