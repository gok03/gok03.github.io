			$( ".pro2" ).hoverIntent(function() {
				var bottom_nav = $('.top').position().top+$('.top').outerHeight(true);
				    window_top = $(window).scrollTop();

				$(this).addClass("active");
				$(".res").removeClass("active");
				$(".com").removeClass("active");
				$(".inv").removeClass("active");
				$(".store").removeClass("active");
				$( ".resource-nav" ).slideUp();
				$( ".store-nav" ).slideUp();
				$( ".community-nav" ).slideUp();
				$( ".inv-nav" ).slideUp();
				$( ".pro-nav" ).slideDown( "slow" );
				$( ".pro-nav, .community-nav, .resource-nav, .inv-nav, .store-nav" ).css('top', bottom_nav);
				return false;
			});//End Footer SiteMap Accordion

