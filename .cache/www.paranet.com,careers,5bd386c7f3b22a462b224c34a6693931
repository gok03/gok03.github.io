<script>
    'use strict';

    (function(){
        console.log('3r');
        if ('mainFormInteval' in window){
            clearInterval(window.mainFormInteval);
        }

        var formWrapper = $('[id*="hs_form_target_module"]');
        window.mainFormInteval = setInterval(function(){
            var form = $('[id*="hs_form_target_module"] form');
            console.log('form', form);
            if (form.length > 0){
                clearInterval(window.mainFormInteval);
                form.attr('target', 'iframe_for_subscribe_form');
                console.log('set target', form);

                formWrapper.append('<div id="iframe_for_subscribe_form_message">Thanks for scheduling your free price quote! A Paranet representative will be in touch with you shortly.</div>');

                $('#iframe_for_subscribe_form').on('load', function(){
                    form.hide();
                    $('#iframe_for_subscribe_form_message').show();
                });
            }
        }, 50);
    })();

</script>
