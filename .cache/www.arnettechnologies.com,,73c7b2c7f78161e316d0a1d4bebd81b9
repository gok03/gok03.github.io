function navigate(val,itemid)
{
if(val == 1)
	window.location="index.php?option=com_joomconnect&amp;view=register&amp;layout=form&amp;Itemid="+itemid;
if(val == 2)
	window.location="index.php?option=com_joomconnect&amp;view=emaillist&amp;Itemid="+itemid;
}
</script>

