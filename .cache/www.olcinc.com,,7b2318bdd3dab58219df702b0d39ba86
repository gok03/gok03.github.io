		function showPhoto(u){
			$("#mast_photo img").attr("src",u);
		}
		
		function clearMastClasses(){
			
			$(".mast_btn0").removeClass("mast_btn0_hover");
			$(".mast_btn1").removeClass("mast_btn1_hover"); 
			$(".mast_btn2").removeClass("mast_btn2_hover"); 
			$(".mast_btn3").removeClass("mast_btn3_hover");
			
