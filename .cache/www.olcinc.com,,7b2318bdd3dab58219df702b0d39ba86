				os = $(this).offset(); _top = os.top+2; _left = os.left-8;
				$("#subnav_products_tab")
					.css("top", _top+"px").css("left", _left+"px")
					.show(function(){ 
						$("#subnav_products")
							.css("top", (_top+21)+"px").css("left", (_left-82)+"px")
							.slideDown();
					});
			});
			$("#subnav_products").mouseleave(function(){
				$("#subnav_products").slideUp(function(){ $("#subnav_products_tab").hide();});
			});
			$("#subnav_products_tab").click(function(){ window.location.href="services.php"; });
			
