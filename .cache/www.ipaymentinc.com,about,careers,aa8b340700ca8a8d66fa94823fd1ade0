$('button.clearForm').click(function () {
  var $form = $(this).closest('form');
  $($form).find('input').not('#navSearchInput').val('');
  $($form).find('select').val('');
  $($form).find('textarea').val('');
  return false;
});

stickyNavbar();

//Bind Search
$('input#navSearchInput').keypress(function(event) {
  if (event.which === 13) {
    event.preventDefault();
    var searchTextBox = $('input#navSearchInput').filter(':visible');
    searchSite(searchTextBox.val());
  }
});

$('div#iconSearch').click(function() {
  var searchTextBox = $('input#navSearchInput').filter(':visible');
  searchSite(searchTextBox.val());
});

