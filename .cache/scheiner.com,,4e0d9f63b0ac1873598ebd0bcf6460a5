	</script>

	<script>
		
		
		var apiEndpoint = 'http://vimeo.com/api/v2/';
		
		var videosCallback1 = 'setupGallery1';
		var videosCallback2 = 'setupGallery2';
		var videosCallback3 = 'setupGallery3';
		var vimeoUsername = '910916';//beyondthestill//staffpicks
		
		var vimeotype = "channel"; // = channel or group or user or album
	    var numberofvideos = 6;
		
		
		var videos1 = Array();
		var videos2 = Array();
		var videos3 = Array();
		
		var code = colorcode("#000000");
				
		$(document).ready(function() {
		
				makeCall(1);
		});
		
		function makeCall(page){
			
			if(vimeotype=='user'){
				if(page==1)
				$.getScript(apiEndpoint + vimeoUsername + '/videos.json?page='+page+'&callback=' + videosCallback1);
				
				else if(page==2)
				$.getScript(apiEndpoint + vimeoUsername + '/videos.json?page='+page+'&callback=' + videosCallback2);
				
				else if(page==3)
				$.getScript(apiEndpoint + vimeoUsername + '/videos.json?page='+page+'&callback=' + videosCallback3);
					
			}else{
				if(page==1)
				$.getScript(apiEndpoint + vimeotype + "/" +vimeoUsername + '/videos.json?page='+page+'&callback=' + videosCallback1);	
				
				else if(page==2)
				$.getScript(apiEndpoint + vimeotype + "/" +vimeoUsername + '/videos.json?page='+page+'&callback=' + videosCallback2);
				
				else if(page==3)
				$.getScript(apiEndpoint + vimeotype + "/" +vimeoUsername + '/videos.json?page='+page+'&callback=' + videosCallback3);
			}
			
		}
		
		function setupGallery1(videos) {
			
			videos1 = videos1.concat(videos);
			//console.log(videos1);
			makeCall(2);
			
		}
		function setupGallery2(videos) {
			videos2 = videos2.concat(videos);
			//console.log(videos2);
			makeCall(3);
		}
		function setupGallery3(videos) {
			videos3 = videos3.concat(videos);
			//console.log(videos3);
			videos = videos1.concat(videos2,videos3);
			//console.log(videos);
			
			$('#thumb_container ul').empty();
			
			if(numberofvideos > videos.length){
				numberofvideos = videos.length;
			}
			// Add the videos to the gallery
			
			var playerColor="#fa6900";
			playerColor = playerColor.replace('#', '');
			var qry="?color="+playerColor+"&title=0&portrait=0&loop=0&byline=0";
			
			for (var i = 0; i < numberofvideos; i++) {
				var html = '<li><a class="fancybox-media" data-type="vimeo" href="' + videos[i].url + qry + '"><img src="' + videos[i].thumbnail_large + '" class="thumb" />';
				html += '<div class="dtls"><p>' + videos[i].title + '</p><div class="dltsBtn"><i class="fa fa-eye"></i></div></div></a></li>';
				$('#thumb_container ul').append(html);
			}
			
			$('#u5321 #thumb_container ul li a div.dtls').css("background-color", "rgba("+code+", 0.5)");
			
			$(".fancybox-media").fancybox({
				
				maxWidth 	: 650,
				maxHeight 	: 365,
				autoSize    : true,
				autoHeight  : true,
				autoWidth   : true,
				openEffect  : 'elastic',
				closeEffect : 'fade',
				openSpeed 	: 'slow',
				closeSpeed  : 'slow',
				padding 	: 10,
				tpl: {
					closeBtn : '<a title="Close" class="fancybox-item fancybox-close fa fa-times-circle" href="javascript:;"></a>'
				},
				helpers : {
					media : {}
				},
				vimeo  : {
 		           	autoplay      : 0,
 		           	loop		  : 0,
					color		  : playerColor,
					title    	  : 0,
					byline   	  : 0,
					portrait 	  : 0
 	         	}
			});
		}
		
		function hexToR(h) {return parseInt((cutHex(h)).substring(0,2),16)}
		function hexToG(h) {return parseInt((cutHex(h)).substring(2,4),16)}
		function hexToB(h) {return parseInt((cutHex(h)).substring(4,6),16)}
		function cutHex(h) {return (h.charAt(0)=="#") ? h.substring(1,7):h}
		
		function colorcode(color){
			R = hexToR(color);
			G = hexToG(color);
			B = hexToB(color);
			
			var code = R+", "+G+", "+B;
			
			return code;
		}
	
		
</script>
	

	
