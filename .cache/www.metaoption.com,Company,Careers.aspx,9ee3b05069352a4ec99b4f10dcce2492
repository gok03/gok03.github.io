<script type="text/javascript">
//<![CDATA[
Sys.Application.add_init(function() {
    $create(Sys.Extended.UI.TextBoxWatermarkBehavior, {"ClientStateFieldID":"ContentPlaceHolder1_tweSearchjob_ClientState","WatermarkCssClass":"SerchArticle","WatermarkText":"Search Keywords...","id":"ContentPlaceHolder1_tweSearchjob"}, null, null, $get("ContentPlaceHolder1_txtSearchJob"));
