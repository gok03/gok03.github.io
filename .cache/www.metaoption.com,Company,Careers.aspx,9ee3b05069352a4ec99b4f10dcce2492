        $('document').ready(function () {
        try{
            $('#flip-container').quickFlip();

            $('#flip-navigation li a').each(function () {
                $(this).click(function () {
                    $('#flip-navigation li').each(function () {
                        $(this).removeClass('selected');
                    });
