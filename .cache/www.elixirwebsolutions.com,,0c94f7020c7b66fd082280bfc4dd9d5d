        <script type="text/javascript">
            jQuery(document).ready(function(){ 
                jQuery('.links').hide();
                var hash = window.location.hash;
                if(hash == "#services") {
                    jQuery('#topmenuhome').removeClass('active');
                }else{
                    jQuery('#topmenuservices').removeClass('active');
                }
                jQuery('#topmenuservices').click(function(){                   
                    jQuery('#topmenuhome').removeClass('active');
                    jQuery('#topmenuservices').addClass('active');
                });
                jQuery('.action-links').hide();
                if (!jQuery.browser.opera) {    
                    // select element styling
                    jQuery('#home_free_quote select.form-select').each(function(){
                        var title = jQuery(this).attr('title');
                        if( jQuery('option:selected', this).val() != ''  ) title = jQuery('option:selected',this).text();
                        jQuery(this)
                        .css({'z-index':10,'opacity':0,'-khtml-appearance':'none'})
                        .after('<span class="select">' + title + '</span>')
                        .change(function(){
                            val = jQuery('option:selected',this).text();
                            jQuery(this).next().text(val);
                        })
                    });
                };		
            });
        </script>
        <script type="text/javascript">   
            jQuery(window).scroll(function () {
                var value = jQuery(window).scrollTop();
                if(value == 0) {
                    jQuery('.fixed').addClass('header_top');
                    jQuery('.fixed').removeClass('fixed');            
                }else{
                    jQuery('.header_top').addClass('fixed');
                    jQuery('.header_top').removeClass('header_top');
                }
                
            });
            var formstatus = '';
            
            function showQuoteDiv(){
                //jQuery.cookie("formstatus", "close");
                formstatus = 'close';
                w = jQuery(window).width();
                if((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPod/i)) || (w <=480)) {                                  
                    if(w <= 480) {                     
                        jQuery('#edit-submit--3').click(function(){                            
                            var newht = jQuery("#floatingbanner").height()+70;
                            jQuery("#floatingbanner").css({'top':-newht});                       
                        })                                     
                        jQuery('#blackout').css('visibility','visible');                
                        jQuery("#buttonclose1.reopenButton").hide("slow");
                        jQuery("#buttonclose1.closeButton").show("slow");                 
                        var cPos = jQuery("#home_free_quote");
                        var ht = jQuery("#floatingbanner").height();                
                        if(ht == 1) {
                            ht = 768;
                        }               
                        var fbTop = cPos.height()-ht;                       
                        //jQuery("#floatingbanner").css({'top':cPos.height(), height:'1px', width:'100%', 'margin-top':'0px'}).show();                
                        jQuery("#floatingbanner").css({'top':cPos.height(),width:'100%', 'margin-top':'0px'}).show();                
                        //jQuery("#floatingbanner").animate({'top':fbTop, height:'795px'}, 1000);
                        jQuery("#floatingbanner").animate({'top':fbTop}, 1000);
                        jQuery("#free_quote").hide();
                        jQuery("#request_free_quote_text").hide();
                        jQuery(".copyright").hide();
                        //jQuery('html,body').animate({scrollTop: jQuery(".strategy-branding").offset().top}, 5000);
                    } 
                }
                else{  
                    var marginTop = '';                 
                    jQuery('#blackout').css('visibility','visible');                
                    jQuery("#buttonclose1.reopenButton").hide("slow");
                    jQuery("#buttonclose1.closeButton").show("slow");                 
                    var cPos = jQuery("#home_free_quote");
                    var ht = jQuery("#floatingbanner").height();                
                    if(ht == 1) {
                        ht = 768;
                    }               
                    var fbTop = cPos.height()-ht;               
                    if(jQuery.browser.msie && jQuery.browser.version == "7.0") {                       
                        marginTop += fbTop-22;                     
                    }else{
                        marginTop += fbTop-42; 
                    }                    
                    //jQuery("#floatingbanner").css({'top':cPos.height(), height:'1px', width:'100%', 'margin-top':'0px'}).show();                
                    jQuery("#floatingbanner").css({'top':cPos.height(),width:'100%', 'margin-top':'0px'}).show();                
                    //jQuery("#floatingbanner").animate({'top':fbTop, height:'795px'}, 1000);
                    jQuery("#floatingbanner").animate({'top':marginTop}, 1000);
                }
                
                //                //jQuery('html,body').animate({scrollTop: 2200}, 1000);
                //                // jQuery('#request-free-quote').hide();
                //                //jQuery('#free_quote').hide();
                //                jQuery(window).resize(function(){
                //                        var w = jQuery(window).width();                    
                //                        if(w >= 480) {                        
                //                           jQuery('html,body').animate({scrollTop: jQuery("#free-quote-form").offset().top}, 1000);
                //                        }
                //                    });
            }   
            
            function hideQuoteDiv() {
                // jQuery.cookie("formstatus", "open");
                formstatus = '';
                jQuery("#buttonclose1.closeButton").hide("slow");
                jQuery("#buttonclose1.reopenButton").show("slow");
                // jQuery('#request-free-quote').show();
                // jQuery('#free_quote').show();
                jQuery("#free_quote").show();
                jQuery("#request_free_quote_text").show();
                jQuery(".copyright").show();
                var cPos = jQuery("#home_free_quote");
                //jQuery("#floatingbanner").animate({"top": cPos.height(), height:'1px'}, 1000, function(){ jQuery("#floatingbanner").hide(); });                    
                jQuery("#floatingbanner").animate({"top": cPos.height()}, 1000, function(){ jQuery("#floatingbanner").hide(); });                                    
            }
            
            jQuery("#home_free_quote").css({"position":"relative"});          
            jQuery("#request_free_quote_text,#free_quote").click(function(e){ e.stopPropagation(); 
                //var formstatus = jQuery.cookie("formstatus");
                console.log(formstatus);
                if(formstatus == "close") {
                    hideQuoteDiv();
                }else {
                    showQuoteDiv();
                }
            });            
            jQuery(".closeButton").click(function(){ 
                hideQuoteDiv();                  
            });        
        </script>


        <script>
            function nextclick() {
                var pageurl = window.location.pathname;                
                var base_url = Drupal.settings.basePath;
                var index = pageurl.indexOf("mobile-applications");
                if(index>0) {
                    var slider_title = jQuery('#slides_iphone').find('#image_section:visible').find('#image_section_left').find('#slider_node_title').text();                  
                    slider_url = slider_title.replace(/\s+/g, '-').toLowerCase();
                    window.history.pushState('tr6yryryr', '', 
                    base_url+'mobile-applications/'+slider_url);
                }else{                    
                    slider_title = jQuery('#slides_portfolio').find('#image_section:visible').find('#image_section_left').find('#slider_node_title').text();                
                    slider_url = slider_title.replace(/\s+/g, '-').toLowerCase();
                    window.history.pushState('tr6yryryr', '', 
                    base_url+'project/'+slider_url);
                }                
                
            }                               
                    
            var menuResponsive = jQuery('#block-home_page_content-responsive_menu');
            menuResponsive.hide();             
            jQuery(document).ready(function() {                                                
                var simplemenu = jQuery('#block-nice_menus-1');
                if((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPod/i)) || (navigator.userAgent.match(/iPad/i))) {
                    jQuery('.header_top .responsive_menu a').on('click', function(e) {
                        e.preventDefault();
                        menuResponsive.slideToggle();
                    });
                        
                    jQuery('#toggle3').bind('touchstart', function(){
                        jQuery('#make-toggle-3').toggle('slow');
                        jQuery('#make-toggle-2').hide();
                        jQuery('#make-toggle-1').hide();
                        jQuery('#make-toggle-4').hide();
                        jQuery('#make-toggle-5').hide();
                        jQuery('#make-toggle-6').hide();
                        jQuery('#make-toggle-7').hide();
                        jQuery('#make-toggle-8').hide();
                        jQuery('#make-toggle-9').hide();
                        jQuery('#make-toggle-10').hide();
                         
                    });
                    jQuery(window).resize(function(){
                        var w = jQuery(window).width();                    
                        if(w >= 480) {                        
                            menuResponsive.hide();             
                        }
                    });
                
                }else{
                    jQuery(function() {	
                        jQuery('#pull').click(function(e){
                            menuResponsive.slideToggle();
                            e.preventDefault();
                        })
                        jQuery('#toggle3').click(function(){                      
                            jQuery('#make-toggle-3').toggle('slow');
                            jQuery('#make-toggle-2').hide();
                            jQuery('#make-toggle-1').hide();
                            jQuery('#make-toggle-4').hide();
                            jQuery('#make-toggle-5').hide();
                            jQuery('#make-toggle-6').hide();
                            jQuery('#make-toggle-7').hide();
                            jQuery('#make-toggle-8').hide();
                            jQuery('#make-toggle-9').hide();
                            jQuery('#make-toggle-10').hide();
                        });
                        jQuery(window).resize(function(){
                            var w = jQuery(window).width();                    
                            if(w >= 767) {                        
                                menuResponsive.hide();             
                            }
                        });
                    });
                }
                
            });
                    
                
        </script>	


        <script>
            jQuery(function() {
                var pull1 		= jQuery('#portfolio_navigation #block-block-16');
                menu1 		= jQuery('#portfolio_navigation #block-portfolio-portfolio_navigation_menu');
                menuHeight1	= menu1.height();

                jQuery('#portfolio_navigation #block-block-16 a').on('click', function(e) {
                    e.preventDefault();
                    menu1.slideToggle();
                });

                jQuery(window).resize(function(){
                    var w = jQuery(window).width();
                    if(w > 900 && menu1.is(':hidden')) {
                        menu1.removeAttr('style');
                    }
                });
            });                
        </script>	
