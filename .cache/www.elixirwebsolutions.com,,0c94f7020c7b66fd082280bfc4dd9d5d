<script type="text/javascript" src="http://www.elixirwebsolutions.com/sites/all/themes/elixir/js/responsiveslides.min.js?od8d5y"></script>
<script type="text/javascript" src="http://www.elixirwebsolutions.com/sites/all/modules/custom/home_page_content/home_page_portfolio.js?od8d5y"></script>
