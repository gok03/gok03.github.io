<script type="text/javascript">

	var country_site_selector_property = "RYV";
	var country_site_selector_default_display = '<span class="country_flag"><img alt="" src="/flags/flag_us.jpg"/></span> <span class="country_name">US - English</span> <span class="country_dropdown"><img src="/images/drop_dwn_img1.png"/></span>';


function menu(jsonp, resultValue) {
    if ((resultValue.resultCode.indexOf("0") !== -1) && (!$.isEmptyObject(jsonp))) {
        var sb = "";
        sb += ("<ul>");
        sb += ("<li><a id='display'>"+ country_site_selector_default_display + "</a>");
        sb += ("<ul id='countryList'>");
        $.each(jsonp, function () {
            sb += ("<li><a href='" + this.url + "'>"+"<span class='country_flag'><img src='/flags/" + this.flag_file + "' alt='' /></span>" + "<span class='country_name'>"+this.display_name+"</span></a></li>");
        });
        sb += ("</ul>");
        sb += ("</li>");
        sb += ("</ul>");
            //sb +=  resultValue.resultCode ;
            $('#country_nav1').html(sb);
        }
    }
</script>
