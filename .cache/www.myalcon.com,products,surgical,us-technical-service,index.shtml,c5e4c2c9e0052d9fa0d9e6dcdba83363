<script type="text/javascript">
$(document).ready(function(){
	//$('#language_msdd').wrapAll('<div class="wrapper-flag"></div>');
});
setTimeout(window.onload=setLanguageParam,500);
function setLanguageParam(){
var url = document.location.href;
if(window.langAlcon == 'us-es'){
document.getElementById("loginOne").innerHTML='Inicio de sesión para pedir lentes de contacto';
}else if(window.langAlcon == 'ca-fr'){
document.getElementById("loginOne").innerHTML='Connectez-vous pour commander des lentilles de contact';
}else if(window.langAlcon == 'us-en'){
document.getElementById("loginOne").innerHTML='Login to Order Contact Lenses';
}else if(window.langAlcon == 'ca-en'){
document.getElementById("loginOne").innerHTML='Login to Order Contact Lenses';
}
}
function changeLocale(){
if(window.langAlcon == 'us-es'){
document.getElementById("loginOne").innerHTML='Inicio de sesión para pedir lentes de contacto';
}else if(window.langAlcon == 'ca-fr'){
document.getElementById("loginOne").innerHTML='Connectez-vous pour commander des lentilles de contact';
}else if(window.langAlcon == 'us-en'){
document.getElementById("loginOne").innerHTML='Login to Order Contact Lenses';
}else if(window.langAlcon == 'ca-en'){
document.getElementById("loginOne").innerHTML='Login to Order Contact Lenses';
}
}


</script>
