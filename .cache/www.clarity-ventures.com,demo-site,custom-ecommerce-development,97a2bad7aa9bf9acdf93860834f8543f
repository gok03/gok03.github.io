<script type="text/javascript">

                $(document).ready(function () {

                    // Change inner-background class based on page currently on
                    if ($('.count-label').text().trim().length > 0) {
                        $('.inner-background').addClass('inner-background-demo-site-content');
                    }
                    else {
                        $('.inner-background').addClass('inner-background-demo-site-mainpage');
                    }

                });

                $(window).load(function () {
                    $('.demo-site-wrapper').each(function (index) {
                        var count = index + 1;
                        var $divContent = $('.demo-site-content-wrapper-' + count.toString());

                        var $img = $('.demo-site-image-' + count.toString());
                        var $div = $('.demo-site-image-wrapper-' + count.toString());

                        var h = $img.height();
                        var d = $div.height();
                        var dC = $divContent.height();

                        if (dC > d) {
                            $img.css('margin-top', ((dC - h) / 2) + "px");
                            //alert('dC > d - Img H: ' + h + ', dC: ' + dC + ', d: ' + d);
                        }
                        else {
                            $img.css('margin-top', ((d - h) / 2) + "px");
                            //alert('d > dC - Img H: ' + h + ', dC: ' + dC + ', d: ' + d);
                        }
                    });
                });

            </script>
