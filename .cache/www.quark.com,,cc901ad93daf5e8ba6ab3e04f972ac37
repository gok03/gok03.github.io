<script>
$(document).ready(function(){
	var random = Math.floor(Math.random() * $('.item').length);
	$('.item').hide().eq(random).show();
	$(".btn.btn-primary.btn-lg").click(function(){
		$(".embed-responsive.embed-responsive-16by9").show();
		$(".embed-responsive.embed-responsive-16by9").css("clear", "right"); 
		$(".above-video").show().css("display","block");
	});
	$("a.close-video").click(function(){
		$(".above-video").hide();
		$(".embed-responsive.embed-responsive-16by9").hide();
	});
	$("#ContentPlaceHolder1_ContentPlaceHolder2_SearchField").attr("placeholder", "Search");
	$("#ContentPlaceHolder1_ContentPlaceHolder2_SearchField").css("width", "402px");
});
</script>

