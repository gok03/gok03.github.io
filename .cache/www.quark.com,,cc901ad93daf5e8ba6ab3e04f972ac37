	$(function(){
		$('.slide-out-div').tabSlideOut({
			tabHandle: '.handle1',					//class of the element that will be your tab
			pathToTabImage: '/images/handle.jpg',	//path to the image for the tab *required*
			imageHeight: '122px',					//height of tab image *required*
			imageWidth: '40px',						//width of tab image *required*    
			tabLocation: 'right',					//side of screen where tab lives, top, right, bottom, or left
			speed: 1000,							//speed of animation
			action: 'click',						//options: 'click' or 'hover', action to trigger animation
			topPos: '200px',						//position from the top
			fixedPosition: true,					//options: true makes it stick(fixed position) on scroll
			onLoadSlideOut: true
		});
	});
</script>

